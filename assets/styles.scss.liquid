            &.fix-btn-right {
                @include flexbox;
                align-items: center;
                width: auto;
                .title.big-size {
                    @media (max-width: 575px) {
                        font-size: 10px !important;
                    }
                }  
                .subtitle.big-size {
                    @media (max-width: 575px) {
                        font-size: 15px !important;
                    }
                }  
                .btn-banner {
                    margin: 0 0 0 auto; 
                    @media (max-width: 991px) {
                        display: none;  //fixed market-light home 7
                    } 
                    &.btn-secondary { 
                        @include margin-start(12px);
                    }  
                }
            }
            &.btn-transparent {  
                .title {
                    margin: 0;   
                } 
                .subtitle {
                    margin: 0;     
                }   
                .btn-banner {   
                    margin-top: 9px;  
                } 
            }  
        }
        &.top-left {
            .fix-btn-right {
                @include ab(n, n, n, n, n, 60px); 
            }
            .btn-full-width {
                @include ab(n, n, n, n, n, 0); 
            }
        }
        &.top-center {
            .text {
                @include ab(n, n, n, 50%, n, n);  
                @include translate(-50%, 0);
            } 
            .btn-full-width {
                width: 100%;
            }  
        }
        &.top-right {
            .fix-btn-right {
                @include ab(n, n, n, 60px, n, n);  
            }
            .btn-full-width {
                @include ab(n, n, n, 0, n, n); 
            }
        }
        &.center-left {
            .text { 
                top: 50%;
                @include translate(0, -50%);       
            }
            .fix-btn-right {
                @include ab(n, n, n, n, n, 60px); 
            }
            .btn-full-width {
                @include ab(n, n, n, n, n, 0);
            }
        }
        &.center-center {
            .text {
                @include translate(-50%, -50%);
                @include ab(n, n, 50%, 50%, n, n);
            }
            .btn-full-width {
                width: 100%;
            }  
        }
        &.center-right {
            .text {
                top: 50%;
                @include translate(0, -50%);     
            }
            .fix-btn-right {
                @include ab(n, n, n, 60px, n, n);  
            }
            .btn-full-width {
                @include ab(n, n, n, 0, n, n);  
            }
        }
        &.bottom-left {
            .fix-btn-right {
                @include ab(n, n, n, n, n, 60px);
            }
            .btn-full-width {
                @include ab(n, n, n, n, n, 0);
            }
        }
        &.bottom-center {
            .text {
                @include ab(n, n, n, 50%, n, n); 
                @include translate(-50%, 0);  
            }
            .btn-full-width {
                width: 100%;
            }
        }
        &.bottom-right {
            .fix-btn-right {
                @include ab(n, n, n, 60px, n, n);  
            }
            .btn-full-width {
                @include ab(n, n, n, 0, n, n);  
            }
        } 
    }   
    &.banner-item-2 {      
        &.text-box-overlay {
            .text, .text-2 {    
                width: auto; 
                @include ab(n, n, 50%, auto, n, n); 
                @include translate(0, -50%);
                .image {
                    max-width: 90px;  
                    margin: 0 auto 30px;
                    @media (max-width: 1024px) {
                        margin-bottom: 15px;
                    }          
                }   
                .title {
                    margin: 0 auto 26px;
                    @media (max-width: 1024px) {
                        margin-bottom: 10px;
                    }    
                }
            }
            .btn {
                @include margin-end(0);
                margin-top: 0;  
            }       
            .text-v2 {
                @include ab(absolute, block, 50%, 50%, n, n);
                z-index: 9; 
                @include translate(-50%, -50%);
                font-size: $size-h1 + 20px;  
                text-align: center;
                @media (max-width: 1199px) {
                    font-size: $size-h1; 
                } 
                @media (max-width: 991px) {
                    font-size: $size-h1 - 10px;
                } 
            }  
        } 
    } 
    .text, .text-2 {    
        text-align: center;
        padding: 15px 0 0;   
        img {
            margin-bottom: 27px;
            @media (max-width: 767px) {
                margin-bottom: 15px;
            }        
        }
        &.btn-full-width { 
            display: block;
            width: auto; 
            padding: 0 10px; 
            .btn {  
                display: block; 
                padding: 0 15px;  
            } 
        }  
    } 
} 

/*=== Section home-parallax ===*/
