/*=== section-instagram ===*/    
.footer-instagram-items {
    .footer_instagram_list .col-fix {
        padding: 0; 
    }
    .instagram_list, .footer_instagram_list {  
        margin: 0; 
        .col-fix {
            @media (max-width: 991px) {
                max-width: 16.666667%;
                @include flex(0 0 16.666667%); 
            }
            @media (max-width: 767px) {
                max-width: 33.33333%;
                @include flex(0 0 33.33333%);  
            }
        }
        a { 
            position: relative;
            display: block; 
            padding-bottom: 100%;
            width: 100%;
            height: 0;  
            img {
                position: absolute;
                display: block;
                width: 100%; 
                height: 100%;
                object-fit: cover; 
            }     
            &:before {
                content: "";
                @include ab(absolute, block, 0, 0, 0, 0);
                background: rgba(0, 0, 0, 0.2);  
                @include opacity(0);
                visibility: hidden;
                z-index: 1; 
                @include transition(all 0.35s ease-in-out);
            }
            &:after {
                content: "\f16d";
                font-size: 18px;
                font-family: $font-family-icon; 
                color: #ffffff;
                @include ab(absolute, block, 50%, 50%, n , n);
                @include opacity(0);
                visibility: hidden;
                z-index: 2;
                @include translate(-50%, -50%);
                @include transition(all 0.35s ease-in-out);
            }       
            &:hover { 
                &:before, &:after {
                    @include opacity(1);
                    visibility: visible;
                }      
            }
        }  
        &.owl-loaded .col-fix {
            max-width: 100%;
            padding: 0; 
            margin-bottom: 0; 
            @include flex(0 0 100%);
        }           
    }   
}
