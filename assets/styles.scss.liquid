/*=== Section two-column ===*/
.templateIndex {
    .section-two-column .left-vertical-menu .navbar { 
        height: auto;
        position: relative; 
        @include transform(scaleY(1));  
        @include transition(all 0.3s ease-in-out);     
        &.opened {
            @include transform(scaleY(0));
            height: 0;          
        }                                                                                                              
    }     
}         
.section-two-column {
    .position-left.padding-space {  
        @media (max-width: 1199px) { 
            padding-top: 20px !important; 
        }         
    } 
    .two-column-content {
        background-size: cover;
        background-position: center center;
        background-repeat: no-repeat;
    }  
    /* Left column */
    .left-vertical-menu {
        @include vertical-menu;  
        .navbar {
            max-width: 297px;
            min-width: 100%;
        }       
        .navbar-collapse {
            display: block;
        }  
    }
    .two-column-left .banner-item {
        margin-top: 20px;
    }  
    .block-product {
        border: 2px solid $progress-bar-secondary-border;
        padding: 20px 30px;     
        h3 {
            font-size: $size-h4;  
            font-weight: $font-bold;    
        }     
        .product-wrapper {
            margin-top: 16px;       
        }      
        .stock-available {
            text-align: center;
            font-size: 13px;
            display: block;
            margin-bottom: 4px;        
        }     
        .pieces-left {
            margin-top: 15px;
        }             
        .progress {
            @include progress-bar($progress-bar-secondary-border);          
        } 
    }
    /* Main column */
    .two-column-main { 
        @include flexbox;
        flex-wrap: wrap;
        margin:  0 -10px;       
        >div {
            padding: 0 10px;
        }         
    }                           
    /* Slideshow */
    .tc-swiper-container {
        position: relative;
        overflow: hidden; 
        margin-top: 20px;       
        max-width: 100%;
        @include flex(0 0 100%); 
        @media (min-width: 768px) {
            max-height: 550px; 
        }             
        &:before {
            content: "";
            @include ab(absolute, block, 0, 0, n, n);
            background: #f3f3f3; 
            width: 100%;
            height: 100%;  
        }                  
        &.swiper-container-initialized:before {
            content: none; 
        }   
        .swiper-button-white {
            @include opacity(0.2);
        }       
        .swiper-button-white {
            &.swiper-button-next {
                @include ab(n, n, n, n, n, 10px);
            }
            &.swiper-button-prev {
                @include ab(n, n, n, 10px, n, n);
            }          
        }  
    }   
    .swiper-slide {
        position: relative;
        height: auto;
        margin: 0 auto;
        @include opacity(0);       
        &.swiper-slide-active {
            @include opacity(1);
        }           
        .img-mobile {            
            @media (min-width: 768px) {
                display: none;
            }      
        }             
        .img-desk {         
            object-fit: cover; 
            font-family: 'object-fit:cover';
            width: 100%;           
            max-height: 550px;       
            @media (max-width: 767px) {
                display: none;
            }                 
        }                   
        .swiper-content {
            @include ab(absolute, block, 0, 0, 0 ,0);
            z-index: 99;
            .container-box {
                position: absolute;
                display: block;  
            }      
        }
    }  
    /* Notice bar */
    .two-column-notice-bar .text-box {
        text-align: center;
        padding-top: 6px;
        padding-bottom: 6px;      
    }     
    /*  Banner */
    .two-column-image {
        margin-top: 20px;  
        .banner-item .subtitle {
            @media (min-width: 1440px) {
                font-size: 35px;
            }     
        } 
    }   
}
