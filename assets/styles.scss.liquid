    &.notice-border-tb .notice-bar-container {
        border-top: 1px solid $body-border-color;
        border-bottom: 1px solid $body-border-color; 
    }  
    &.notice-border-bottom .notice-bar-container {
        border-bottom: 1px solid $body-border-color; 
    } 
    &.notice-border-radius .text-box {
        border: 1px solid $body-border-color;  
        @include border-radius-20;
        padding: 5px 139px; 
        @media (max-width: 991px) {
            padding: 5px 60px;   
        }
        @media (max-width: 767px) {
            padding: 5px 30px;    
        }
    }       
    &.layout-full {
        .notice-bar-container .border-line:before {
            @include ab(n, n, n, 15px, n, 15px);
        }      
    } 
    &.layout-right-column {
        .notice-bar-container {
            @media (min-width: 992px) {
                width: 83.33333%;  
            } 
        }
    } 
    .notice-bar-container {  
        background-position: top center;
        background-size: cover;
        text-align: center; 
        &.bg-image {
            .alignment-left .text-box.flex-layout {  
                @include padding-start(45px);
            }
        } 
        .alignment-left {
            text-align: start;    
        }    
        .alignment-left-2 {
            text-align: start;
            .text-box-content {
                @include padding-start(110px);
                @media (max-width: 991px) {
                    @include padding-start(30px);
                }   
            }  
        }
        .text-box {     
            display: inline-block;       
            &.flex-layout {           
                .text {
                    padding: 0; 
                    display: inline-block;  
                    vertical-align: middle;          
                    p { 
                        padding: 0; 
                    }    
                }
                .btn {
                    display: inline-block;
                    vertical-align: middle;  
                    margin: 0 35px;     
                    font-weight: 400; 
                    text-transform: initial;    
                    @media (max-width: 575px) {  
                        display: none;  
                    } 
                    &.bg-none {
                        margin: 0; 
                        padding: 0 10px;
                    }        
                }
            }
        } 
        .text {
            display: block;   
            padding: 5px 0 0;
            &:first-child {
