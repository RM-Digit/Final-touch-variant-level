.lc-container-v2 {
    .list-collection-v2 {
        overflow: hidden;
    } 
    h5 {
        color: $text-other; 
        font-weight: 400;
        margin-top: 17px;   
        display: block;
        width: 100%;   
    }
    &.style-image-text {
        .list-collection-v2.bg-icon {
            .collection-item {
                padding: 0 20px;
                @include flex(0 0 auto);
                max-width: none;
            }
            .image {  
                @include flexbox;
                align-items: center;
                >a {
                    width: 100%;
                }         
                &.it-icon { 
                    width: 80px;
                    height: 80px;
                }                
                &.it-upload {
                    width: 115px;  
                    height: 115px;    
                    padding: 21px;     
                    text-align: center;     
                }                       
            } 
        } 
        .lc-border-right-full, .lc-border-right-mini, .lc-border-right-rl {
            .type-row {
                flex-wrap: wrap;
                justify-content: center; 
                .collection-item {
                    @media (max-width: 991px) { 
                        border-top: 1px solid $body-border-color;
                    }  
                    @media (max-width: 991px) and (min-width: 576px) {
                        &:nth-child(-n+4) { 
                            border-top: none;     
                        }   
                    }
                    @media (max-width: 575px) {   
                        &:nth-child(-n+3) {
                            border-top: none;    
                        }       
                    } 
                }
            }
        }
        .lc-border-right-full .type-row { 
            .collection-item {
                @include border-end(1px solid $body-border-color);
                padding-top: 30px;  
                padding-bottom: 30px;  
                @media (max-width: 991px) and (min-width: 576px) {  
                    &:nth-child(4n) {
                        @include border-end(none);   
                    }
                }
                @media (max-width: 575px) {    
                    padding-top: 20px; 
                    padding-bottom: 20px; 
                    &:nth-child(3n) {
                        @include border-end(none);
                    }         
                }            
                &:last-child {
                    @include border-end(none); 
                } 
            }  
        } 
        .lc-border-right-mini .type-row {
             .collection-item {
                padding-top: 20px;
                padding-bottom: 20px;
                @media (max-width: 991px) and (min-width: 576px) {  
                    &:nth-child(4n) .item { 
                        @include border-end(none);
                    }
                } 
                @media (max-width: 575px) { 
                    &:nth-child(3n) .item { 
                        @include border-end(none);  
                    }     
                } 
                .item {
                    @include border-end(1px solid $body-border-color);
                }
                &:last-child .item {
                    @include border-end(none);    
                }  
            }
        }
        .lc-border-right-rl  .type-row {
            .collection-item {
                padding-top: 20px;
                padding-bottom: 20px;
                @media (max-width: 991px) and (min-width: 576px) {  
                    &:nth-child(4n+1) .item {
                        @include border-start(1px solid $body-border-color);
                    }
                } 
                @media (max-width: 575px) { 
                    &:nth-child(3n+1) .item { 
                        @include border-start(1px solid $body-border-color);  
                    }     
                } 
                .item {
                    @include border-end(1px solid $body-border-color);
                }
                &:first-child .item {
                    @include border-start(1px solid $body-border-color);  
                }      
            }         
        }
        .lc-border-right-none .type-row .collection-item {
            @include padding-start(10px);
            @include padding-end(10px); 
        } 
        .type-row {
            overflow-x: auto; 
            overflow-y: hidden;    
            @media (max-width: 812px) {
                -webkit-overflow-scrolling: touch; 
            }  
            .collection-item {
                @media (min-width: 992px) {
                    flex-basis: 0;  
                    flex-grow: 1; 
                    max-width: 100%;   
                }    
                @media (max-width: 991px) {
                    max-width: 25%; 
                    @include flex(0 0 25%);
                    padding: 10px 0;   
                }    
                @media (max-width: 575px) {  
                    max-width: 33.33333%; 
                    @include flex(0 0 33.33333%);    
                }        
                &:first-child {
                    @include margin-start(auto);
                } 
                &:last-child {
                    @include margin-end(auto); 
                }   
            } 
            &.item-4 {
                .collection-item  {
                    @media (min-width: 991px) { 
                        max-width: 16.66667%;
                        @include flex(0 0 16.66667%);
                    }
                    &:first-child {
                        @include margin-start(auto);
                    }   
                    &:last-child { 
                        @include margin-end(auto);
                    } 
                }
            }
        }
        .type-row-2 {
            justify-content: center;     
            flex-wrap: wrap;    
            .collection-item { 
                padding: 0 20px;  
                margin-bottom: 40px;
                @media (max-width: 575px) {
                    margin-bottom: 20px;   
                }             
            }
        } 
        .collection-item {
            text-align: center;
            position: relative;   
            .item.show-all {
                width: 115px;  
                height: 115px;    
                padding: 21px;  
                text-align: center;
                margin: 0 auto;
                border: 1px solid $body-border-color;  
                @include border-radius-circle;    
                h4 {
                    padding: 0; 
                    display: table;
                    width: 100%; 
                    height: 100%;
                }                           
                a {
                    color: $main-color;
                    font-weight: $font-bold; 
                    display: table-cell;  
                    vertical-align: middle;   
                    &:before {
                        content: "+";
                        font-size: 22px;
                        display: block;
                    }  
                    &:hover {
                        text-decoration: none;  
                    }       
                }             
            }   
            .image {                      
                margin: 0 auto;     
                @include border-radius-circle;    
                i {
                    font-size: 60px;       
                }                        
            }       
            h4 {
                color: $body-link-color; 
                font-weight: 400;    
                padding: 20px 0 0;  
                @media (max-width: 767px) {    
                    font-size: $body-font-size - 1px !important;
                }   
                @media (max-width: 767px) {
                    padding-top: 7px;
                }     
                a {
                    color: inherit;
                }
                &:hover {
                    color: $body-link-color-hover;
                }
            }
            &.textbox-right {
                padding: 0 10px;    
                .item {
                    @include flexbox;
                    flex-wrap: wrap;
                    align-items: center; 
                    flex-wrap: wrap; 
                    padding: 27px 0px;  
                    @media (max-width: 812px) {
                        -webkit-overflow-scrolling: touch;
                    } 
                    @media (max-width: 991px) {
                        display: block; 
                    }    
                    .image { 
                        @media (min-width: 992px) {
                            margin: 0;  
                        } 
                    }   
                    h4 {
                        padding: 0;  
                        @include padding-start(16px);
                        @include flex(1);
                        text-align: start;   
                        @media (max-width: 991px) {
                            padding: 10px 0 0;  
                            text-align: center;       
                        }
                        @media (max-width: 767px) {
                            font-size: $body-font-size - 3px !important;   
                        }   
                    }    
                }   
            }  
        }   
        .bg-icon .collection-item {
            .image i {
                font-size: 40px;   
            }     
            h4 {
                padding-top: 17px;  
            }          
        }
    }
    &.style-text {
        .type-row {
            overflow-x: hidden;
            flex-wrap: wrap;  
        }   
        .collection-item {
            text-align: start;  
            .item {   
                padding-top: 32px;
                padding-bottom: 40px;
                @include padding-start(45px);
                @include padding-end(49px);
                background: $bg-pro-v2-list;  
                @media (max-width: 991px) {
                    padding-top: 22px;
                    padding-bottom: 30px;  
                    @include padding-start(35px);
                    @include padding-end(39px);   
                }      
                h4 {
                    padding: 0; 
                    position: relative;  
                    &:after {
                        content: "\f178";   
                        font-size: 20px; 
                        font-family: $font-family-icon;
                        color: rgba($body-link-color, 0.2); 
                        @include ab(absolute, block, 50%, n, n, 0);
                        @include translate(0, -50%);
                    }                          
                }
            } 
        }    
    }  
    .btn-show-all {
        text-align: center;
        text-transform: uppercase;
        font-size: $button-size;      
        text-decoration: underline;
        margin: 20px 0 0;
        &.line {
            position: relative;
            &:before {
                content: "";
                @include ab(absolute, block, 100%, n, n, n);
                border-bottom: 1px solid $body-border-color;
                width: 100%;
                height: 1px; 
                margin-top: -16px;   
            }
            >a {
                display: inline-block;
                border: 1px solid $body-border-color; 
                text-decoration: none;
                font-weight: 500; 
                padding: 5px 20px; 
                background: $line-tab-bg;   
                position: relative;  
            }
        }     
    }    
}   
