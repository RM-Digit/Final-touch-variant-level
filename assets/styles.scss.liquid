@mixin swatch-product {
	.swatch {
		margin: 0 0 21px;
		position: relative;
		@media (max-width: 767px) {
			@include margin-end(15px);
		}
		.header {
			padding: 0 0 4px;
			display: block;
			margin: 0;
			color: $main-color;
			text-transform: uppercase;
			font-size: $body-font-size - 2px;
			font-weight: $font-bold;
		}
		input {
			display: none;
		}
		.swatch-element {
			margin: 0;
			padding: 0;
			float: left;
			&.colour label {
				background-color: transparent;
			}
			.crossed-out {
				display: none;
			}
			label {
				min-width: $color-swatch-width;
				height: $color-swatch-height;
				line-height: $color-swatch-height - 5px;
				position: relative;
				margin: 0 0 5px;
				@include margin-end(5px);
				text-align: center;
				color: $body-general-color;
				font-size: $body-font-size - 1px;
				word-break: break-word;
				white-space: normal;
				font-weight: 600;
				text-transform: none;
				cursor: pointer;
				background-color: #e6e6e6;
				background-repeat: no-repeat;
				background-position: center;
				background-size: cover;
				border: 2px solid transparent;
				padding: 0 5px;
				@include border-radius-reset;
				@include box-shadow(none);
				&:before {
					@include ab(absolute, block, 0, 0, n, n);
					width: 100%;
					height: 100%;
					border: 1px solid $body-border-color; 
				}
			}
			input:checked+label {
				box-shadow: none;
				cursor: default;
				border: 2px solid $main-color; 
			}
			&.circle label {
				@include border-radius-circle;
			}
			&.img-true label {
				min-width: $image-swatch-width;
				height: $image-swatch-height;
			}
			&:hover {
				@media (min-width: 1025px) {
					label {
						border: 2px solid $main-color;
						font-weight: 600;  
					}
				}
			}
			&.soldout {
				label {
					cursor: default;
				}
				&:hover {
					@media (min-width: 1025px) {
						label {
							cursor: default;
							color: $body-general-color;
							border: 2px solid transparent;
						}
					}
				}
			}
			&.colour:not(.soldout) {
				input:checked+label {
					cursor: default;
					border: 2px solid $main-color;
				}
				&:hover {
					@media (min-width: 1025px) {
						label {
							border: 2px solid $main-color;
						}
					}
				}
			}
		}
		&:last-child {
			margin-bottom: 15px;
		}
	}
}
@mixin drawer-sidebar {
	@include ab(fixed, n, 0, 0, n, n);
	z-index: 999999;
	height: 100%;
	width: 300px;
	padding: 50px 20px 30px;
	overflow-y: auto;
	background: $body-background-color;
	@include translate(-300px, 0);
	@include opacity(0);
	@include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));
	@media (max-width: 300px) {
		width: 100%;
		padding-bottom: 20px;
	}
	.f-close,
	.fof-close {
		text-align: center;
		cursor: pointer;
		@include border-radius-circle;
		@include transition(all 0.35s ease 0s);
		@include width-btn;
		color: $body-link-color;
		@include ab(absolute, block, 10px, n, n, 10px);
		i:before {
			content: "\e816";
			font-family: $font-family-icon;
		}
		&:hover {
			color: $body-link-color-hover;
		}
	}
}
@mixin toggle-sidebar {
	position: relative;
	width: 100%;
	display: none;
	padding: 25px 15px 0;
	margin: 0 auto 20px;
	background: $body-background-color;
	@include box-shadow(0 0 10px rgba($black, 0.05));
	.sb-title,
	.f-close {
		display: none;
	}
	.sb-filter-wrapper {
		width: 100%;
		display: table;
		vertical-align: top;
	}
	.sbw-filter {
		width: 25%;
		display: inline-block;
		vertical-align: top;
		@media (max-width: 991px) {
			width: 33.33333%;
		}
		@media (max-width: 767px) {
			width: 50%;
		}
		@media (max-width: 575px) {
			width: 100%;
		}
		>div .sb-filter {
			padding: 0 15px;
			clear: none;
			float: none;
			display: inline-block;
			letter-spacing: 0;
			vertical-align: top;
			border: none;
			margin: 0 0 30px;
			width: 100%;
		}
	}
}
@mixin icons-number {
	@include ab( absolute, block, -15px, n, n, -10px);
	@include border-radius-circle;
	width: 20px;
	height: 20px;
	line-height: 20px;
	background: $header-icon-color;
	color: $header-number-on-icon-color;
	text-align: center;
	font-size: 11px;
	font-weight: $font-bold;
}
@mixin hover-icons-number {
	-webkit-animation-name: hvr-icon-bob-float, hvr-icon-bob;
	animation-name: hvr-icon-bob-float, hvr-icon-bob;
	-webkit-animation-duration: .3s, .75s;
	animation-duration: .3s, 0.75s;
	-webkit-animation-delay: 0s, .3s;
	animation-delay: 0s, .3s;
	-webkit-animation-timing-function: ease-out, ease-in-out;
	animation-timing-function: ease-out, ease-in-out;
	-webkit-animation-iteration-count: 1, infinite;
	animation-iteration-count: 1, infinite;
	-webkit-animation-fill-mode: forwards;
	animation-fill-mode: forwards;
	-webkit-animation-direction: normal, alternate;
	animation-direction: normal, alternate;
}
@mixin clearfix {
	&:before,
	&:after {
		display: block;
		width: 100%;
		content: "";
		clear: both;
	}
}
@mixin is-countdown-separate($size, $lheight, $height) {
	&:after {
		content: ':';
		color: $body-general-color;
		font-size: $size;
		font-weight: $font-bold;
		line-height: $lheight;
		height: $height;
		@include ab(absolute, block, 0, n, n, -4px);
	}
	&:first-child {
		@include padding-start(0); 
	}
	&:last-child {
		@include padding-end(0);
		&:after {
			content: none;
		}
	}
}

@mixin carousel-loading {
	@include opacity(0); //height 0 vertical srcoll
	visibility: hidden;
	&.owl-loaded {
		@include opacity(1);
		visibility: visible;
	}
}
@mixin product-add-cart {
	.demo-icon {
		@include margin-end(10px);
		display: none;
	}
	.btn-add-cart {
		text-align: center;
		background: $button-btn1;
		color: $button-btn1-txt;
		padding: 9px 4px 10px;
		text-transform: uppercase;
		font-weight: $font-bold;
		display: block;
		&.sold-out {
			@include opacity(0.65);
		}
	}
	&:hover .btn-add-cart {
		text-decoration: underline;
		&.disabled {
			text-decoration: none;
		}
		&.sold-out {
			text-decoration: none;
			cursor: default;
		}
	}
}
@mixin icon-product {
	float: left;
	width: 36px;
	height: 36px;
	line-height: 34px;
	text-align: center;
	@include margin-end(2px);
	background: $body-background-color;
	color: $body-link-color;
	cursor: pointer;
	&:last-child {
		@include margin-end(0);
	}
	a {
		color: inherit;
		font-size: $body-font-size - 1px;
		@include transition(none);
	}
	i {
		color: inherit;
		font-size: $body-font-size - 1px;
	}
	&:hover {
		color: $body-link-color-hover;
	}
}
@mixin width-boxed {
	width: 100%;
	margin: 0 auto;
	@include padding-start(15px);
	@include padding-end(15px);
	@media (min-width: 576px) {
		max-width: 100%;
	}
	@media (min-width: 768px) {
		max-width: 760px;
	}
	@media (min-width: 992px) {
		max-width: 980px;
	}
	@media (min-width: 1200px) {
		max-width: 1200px;
	}
	@media (min-width: 1440px) {
		max-width: $container-width;
	}
}
@mixin icon-btn-banner {
	position: relative;
	font-weight: 500;
	padding: 5px 30px; 
	&:after {
		content: "\e876";
		font-size: 12px;
		font-family: $font-family-icon;
		color: inherit;
		display: inline-block;
		vertical-align: middle;
		@include padding-start(10px);
	}    
}
@mixin vertical-menu {
	position: relative;
	background: $vertical-header-nav-bg;
	.navigation {
		border: 1px solid $vertical-nav-border; // fixed bug ul
		&.border-color-none {
			border: none;
			.main-nav {
				padding: 22px 0;
				min-height: 450px; 
				>li {
					border-bottom: none;
					>.dropdown-inner >a, 
					>a {
						padding: 9px 0; 
					}
				}
			}
		}
	} 
	.head {
		color: $vertical-header-nav-text;
		font-size: $nav-font-size;
		font-weight: $font-bold;
		cursor: pointer;
		padding: 12px 0; //header 1
		@include padding-start(20px);
		@include padding-end(78px);
		@media (max-width: 1199px) {
			@include padding-end(30px);
		}
		i {
			@include padding-end(9px);
		}
	}
	.navbar {
		z-index: 1001;
		width: 100%;
		min-width: 260px;
		background: $vertical-nav-bg;
		padding: 0;  
		@include ab(absolute, block, 100%, 0, n, n);
		@include transform(scaleY(0));
		@include transform-origin(0 0);
		@include transition(transform 0.3s ease-in-out);
		.navbar-collapse {
			position: relative;  
			display: block;
		}
		&.opened {
			@include transform(scaleY(1));  
		}
	}
	.navbar-collapse >ul {   
		position: static;
		border: none;
		margin: 0;
		padding: 0;
		.labels-wrapper {
			@include ab(n, n, 2px, auto, n, -30px);
		}  
		>li {
			padding: 0;
			@include padding-start(29px);
			@include padding-end(15px);
			position: static;
			display: block;
			border-bottom: 1px solid $vertical-nav-border; 
			cursor: pointer;
			>.dropdown-inner >a,
			>a {
				font-size: $nav-font-size - 2px; // furniture
				margin: 0;
				padding: 14px 0;
				display: inline-block;
				text-decoration: none;
				color: $vertical-nav-link-color;
				line-height: 19px;
				position: relative;
				@media (max-width: 1199px) {
					font-size: $nav-font-size - 2px;
				}
				>span:not(.labels-wrapper) {  
					position: relative; 
					&:after {
						content: "";
						@include ab(absolute, block, n, 0, 1px, n);
						width: 0;
						background: $vertical-nav-hover-color;  
						height: 1px;
						@include transition(width 0.3s ease-in-out);  
					}
				}
			}
			>.dropdown-inner {
				position: relative;
			}
			&:hover {
				>a,
				>.dropdown-inner >a {
					color: $vertical-nav-hover-color;
					>span:after {
						width: 100%;
					}
				}
			}
			/*have submenu*/
			&.dropdown {
				>.dropdown-menu {
					background: $nav-dropdown-background;
					font-size: $body-font-size;
					padding: 20px 0 35px;
					@include ab(n, n, 0, 100%, n, n);
					@include margin-start(0);
					@include border-start(1px solid $body-border-color); 
					min-height: 100%;   
					z-index: -1001; 
					@include transition(all 0s); 
					&.width-100 {
						width: calc(980px - 30px - 260px); // skin market    
						@media (min-width: 1200px) {
							width: calc(1200px - 30px - 260px);   
						}
						@media (min-width: 1440px) {
							width: calc(#{$container-width} - 30px - 260px);
						}
					}
					&.width-75 {
						width: calc((980px - 30px - 260px) * 3 / 4);   
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) * 3 / 4);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) * 3 / 4);
						}
					}
					&.width-50 {
						width: calc((980px - 30px - 260px) / 2);    
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) / 2);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) / 2);
						}
					}
					&.width-25 {
						width: calc((980px - 30px - 260px) / 4);    
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) / 4);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) / 4); 
						}
					}
					>li {
						position: static;  
						.dropdown-inner {
							position: relative;
						}
						a.dropdown-link {
							@include padding-end(0);
							@include margin-end(30px);
						}
						.dropdown-menu {
							background: $nav-dropdown-background;
							@include border-start(1px solid $body-border-color); 
							min-height: 100%;  
							>li {
								>a {
									color: $nav-dropdown-link-color;
									white-space: normal;
								}
								&:hover {
									>a {
										color: $nav-dropdown-link-hover-color;
									}
								}
							}
						}
						&:hover {
							>a,
							>.dropdown-inner >a {
								color: $nav-dropdown-link-hover-color;
							}
						}
					}
					.block-text {
						margin-top: 24px;
						.text-1 {
							color: rgba($nav-dropdown-link-color, 0.5);  
							.special {
								color: #d80006; 
							}
						}
						.text-2 {
							color: $nav-dropdown-link-color;
							font-size: $size-h1 - 5px; 
							line-height: 1.2; 
						}
					}
				}
				.dropdown-menu-sub {
					background: transparent;
					border: 0;
					@include border-radius-reset;
					box-shadow: none;
					position: static;
					width: 100%;
					min-width: auto;
					margin: 0;
					>li {
						>a {
							color: $nav-dropdown-link-color;
							padding: 0;
							border-bottom: none;
							span {
								color: $nav-dropdown-link-color;
							}
						}
						&:hover {
							background: none;
							>a {
								color: $nav-dropdown-link-hover-color;
								span {
									color: $nav-dropdown-link-hover-color;
								} 
							}
						}
					}
				}
				.banners {
					.dropdown-menu-sub >li >a {
						display: block;    
						width: 100%; 
						&:after {
							content: none; 
						}
					}
					img {
						@include dropdown-hidden;  
					}
				}
				&.mega-is-hover {
					.dropdown-menu {
						z-index: 1001;
						@include dropdown-visible; 
					} 
					.banners img {
						@include dropdown-visible;
					}
				}   
			}
			&:last-child {
				border-bottom: none;
			}
		}
		>li.mega-menu {
			>.dropdown-menu {
				padding: 33px 37px;
			} 
			.dropdown-menu {
				.mega-sub-link {
					margin: 0 0 20px;
					&.big-size >a {
						font-size: $size-h1 + 5px;
						margin-bottom: 18px;  
					}
					>a {
						font-size: $nav-sub-font-size + 1px;
					}
					.dropdown-menu-sub {   
						li {
							padding: 5px 0 4px; 
							a:hover {
								color: $nav-dropdown-link-hover-color;
							}
							&.bp-item-2 {
								padding-top: 15px;
							}
						}
					}
					&:last-child {
						margin: 0;
					}
				}
				.product-wrapper {
					background: none;
					padding: 0;
					margin-top: 0; 
					&:after,
					&:before {
						content: none;
					}
					.product-head .product-add-cart .btn-add-cart {
						padding: 5px 4px;
						border: 1px solid $button-btn1-border;
						&:hover {
							color: $button-hover-btn1-txt; 
							border: 1px solid $button-hover-btn1-border;   
						}  
					}
					.featured-img a {
						padding: 0;
					}
					.product-name {
						border: none;
						font-size: $product-font-size;
						&:before {
							content: none;
						}
						a {
							padding: 0;
							font-size: $product-font-size;
						}
					}
					.product-price+.product-review {
						padding-top: 5px; 
					}
				}
			} 
		}
	} 
	&.vertical-secondary-nav { 
		.vertical-navbar {
			border: 2px solid $main-color; 
		} 
		.navbar-collapse >ul >li {
			>.dropdown-inner >a,
			>a {
				font-size: $nav-font-size;	   
			}
			>.dropdown-menu {
				border: 2px solid $main-color;
				@include border-start(1px solid $body-border-color); 
				margin-top: -2px;   
			}  
			&.mega-menu .dropdown-menu .mega-sub-link .dropdown-menu-sub li {
				padding: 2px 0; 
			}
		} 
		.navigation.border-color-none .navbar-collapse >ul:not(.secondary-nav) {
			min-height: auto;
			padding: 19px 0 22px;     
			>li {
				>.dropdown-inner >a,
				>a {
					padding: 4px 0;  
				}  
			}
		}
		ul.secondary-nav {
			border-top: 1px solid $body-border-color;  
			padding: 22px 0;     
			li {
				font-weight: 400;   
				border-bottom: none; 
				a {
					padding: 4px 0;    
					font-size: $nav-font-size - 1px;  
				}
			}
		}
	}   
} 
@mixin progress-bar($bg-bar) {
	@include border-radius-reset;
	height: 10px;
	background: #e6e6e6;
	.progress-bar {
		background: $bg-bar;
	}   
}
@mixin padding-follow-menu($pt, $pb) {
	.table-row {
		>.header-logo, >.header-icons, //furniture home 1  
		>.searchbox, >.header-switcher {   
			@media (min-width: 992px) {
				padding-top: $pt;
				padding-bottom: $pb; 
			}
		}
	}
}
@mixin currency-language {
	.translate-position .weglot-container .country-selector ul {
		@media (max-width: 1024px) {
			@include ab(n, n, n, auto, n, 0);
		}
	}
	.currency-position ul.currencies {
		@media (max-width: 1024px) {
			@include ab(n, n, n, auto, n, 0);
		}
	}
}
@mixin similar-search($w) { 
	.searchbox {
		width: $w;
		.navbar-form {
			width: 100%;
		}
		.search-icon {
			@include ab(n, n, n, n, n, 0);
			width: 40px;
			height: 100%;
			background: $main-color;
			color: $header-search-icon;
		}
		.result-ajax-search {
			width: 100%;
		}
	}
	+.searchbox-mobile .search-icon {
		@include ab(n, n, n, n, n, 0);
		width: 40px;
		height: 100%;
		background: $main-color;
		color: $header-search-icon;
	} 
}

@mixin line-left-right {
	@include flexbox;
	position: relative;
	align-items: center;
	&:before, &:after {
		content: "";
		border-bottom: 1px solid $body-border-color;     
		width: 3000px;
		@include ab(absolute, block, n, n, n, n);
	}
	&:before {
		@include ab(n, n, n, n, n, 100%);
	}
	&:after {
		@include ab(n, n, n, 100%, n, n);  
	} 
}
@mixin header-icon-left {
	@include ab(n, n, -15px, -15px, n, auto);
}
/*=== section separator ===*/
.section-separator {
    overflow: hidden;
    &.color, 
    &.image {
        padding: 25px 0;               
    }
    &.image.lazyloaded {
        background-size: 100%;
    }   
    &.layout-right-column .separator-content {
        @media (min-width: 992px) {
            width: 83.33333%;  
        }
    }  
}    
.separator-heading { 
    max-width: 1200px;
    padding: 0 15px;
    margin: 0 auto;  
}
.separator-line {
    border-top-width: 1px;
    border-bottom-width: 0;
    border-left-width: 0;
    border-right-width: 0;
    max-width: 1170px;
    margin: 0 auto;
    &.line-solid {
        border-style: solid;
    }
    &.line-dotted {
        border-style: dotted;
    }
    &.line-dashed {
        border-style: dashed;
    }
}
.separator-heading-logo { 
    max-width: 1200px;
    padding: 0 15px;
    margin: 0 auto;
    text-align: center;
    h2 {    
        margin: 0 auto 11px;    
    }
    span {
        display: block;
        &.separator-logo {
            margin: 0 auto 16px;      
            &.logo-bt {
                margin: 10px auto 0;        
            }
        }
    }
}
.separator-heading-line {
    margin: 0 auto; 
    text-align: center;   
    h2 {    
        margin: 0 auto;
        font-family: $font-heading;
        @media (max-width: 991px) {
            font-size: $size-h1 !important;
        }
        @media (max-width: 991px) {
            font-size: $size-h1 - 4px !important;
        } 
        &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
            line-height: 1.0;
            font-weight: $font-bold;
        }
        &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
            font-weight: 100;
        }
    }
    .separator-subtext {
        display: block;
        font-family: $font-heading;
        &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
            line-height: 1.0;
            font-weight: $font-bold;
        }
        &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
            font-weight: 100;
        }
    } 
    .separator-sub-line {   
        max-width: 100%; 
        margin: 0 auto;  
        overflow: hidden;   
        border-top-width: 1px;
        border-bottom-width: 0;
        border-left-width: 0;
        border-right-width: 0;
        border-color: $body-border-color;    
        &.sub-double-line {
            max-width: 80px;
            border-width: 1px;
        }
    }    
    &.side-line {
        h2 {
            position: relative;      
            margin: 0 auto;
            display: inline-block;
            padding: 0 50px;
            @media (max-width: 479px) {
                padding: 0 30px;
            }
            span {
                display: inline-block;
                padding: 0 20px;
                position: relative;
                z-index: 1;
                white-space: nowrap;
                background: $body-background-color;
            }
            &:after,
            &:before {
                content: '';
                height: 1px;
                width: 50%;   
                border-top-width: 1px;
                border-bottom-width: 0;
                border-left-width: 0;
                border-right-width: 0;
                @include translate(0, -50%);
            }
            &:before {
                @include ab(absolute, block, 50%, 50%, n, n);
            }
            &:after {
                @include ab(absolute, block, 50%, n, n, 50%);
            }
        }    
        &.line-solid h2 {
            &:after,
            &:before {
                border-style: solid;
            }
        } 
        &.line-dotted h2 {
            &:after,
            &:before {
                border-style: dotted;
            }
        }  
        &.line-dashed h2 {
            &:after,
            &:before {
                border-style: dashed;
            }
        } 
        .separator-logo {
            margin: 8px auto 13px;
            display: block;
        }
        .separator-subtext {
            margin: 8px auto 0;            
        }
    } 
    &.left-line {
        text-align: start;
        h2 {
            position: relative;
            padding: 0;
            display: block; 
            &:after {
                content: none !important;
            }   
            &:before {
                content: '';
                height: 1px;
                width: 100%;
                border-width: 1px;
                @include ab(absolute, block, 50%, n, n, 0);
                @include translate(0, -50%);
            }
            span {
                display: inline-block;
                padding: 0; 
                @include padding-end(8%);
                position: relative;
                background: $body-background-color;
            }       
        }
        &.line-solid h2:before {
            border-style: solid;
        }
        &.line-dotted h2:before {
            border-style: dotted;
        }
        &.line-dashedh2:before {
            border-style: dashed;
        } 
    }
    &.right-line {
        text-align: end;
        h2 {
            position: relative;
            padding: 0;
            display: block; 
            &:after {
                content: none !important;
            }   
            &:before {
                content: '';
                height: 1px;
                width: 100%;
                border-width: 1px;
                @include ab(absolute, block, 50%, 0, n, n);
                @include translate(0, -50%);    
            }   
            span {
                display: inline-block;
                padding: 0; 
                @include padding-start(8%);
                position: relative;
                background: $body-background-color;
            }       
        }
        &.line-solid h2:before {
            border-style: solid;
        }
        &.line-dotted h2:before {
            border-style: dotted;
        }
        &.line-dashed h2:before {
            border-style: dashed;
        } 
    }
    &.line-solid .separator-sub-line {
        border-style: solid;
    } 
    &.line-dotted .separator-sub-line {
        border-style: dotted;
    } 
    &.line-dashed .separator-sub-line {
        border-style: dashed;
    } 
}  
.shopify-section {
    margin: 0 auto;
    clear: both;  
    display: block;  
    width: 100%;
    @include clearfix();
}
/*=== Section Slideshow ===*/
.lazy-loading-img {
    #home-slideshow .swiper-slide.lazyload {
        background-image: url({{'loading.gif' | asset_url }}) !important;     
        width: 100% !important;
        background-size: 100px auto !important;
        @include opacity(1 !important);
        .swiper-content {
            @include opacity(0);
        }
    }  
} 
.bc-wrapper {
    position: relative;  
    &.full-height {
        .swiper-slide {
            min-height: 100vh;
            img {
                &.img-desk, &.img-mobile {
                    min-height: 100vh;
                }
            }
        } 
    }
    &.layout-boxed { 
        @include width-boxed;
        .swiper-button-white { 
            width: 40px;
            height: 40px;
            &:before {
                line-height: 40px; 
                font-size: $font-icon; 
            }
            &.swiper-button-next {
                @include ab(absolute, block, n, n, n, 0);     
            }
            &.swiper-button-prev {
                @include ab(absolute, block, n, 0, n, n);
            }  
        }  
    }   
    &.layout-wide-padding { 
        @include padding-start(10.6%);
        @include padding-end(10.6%);
        @media (max-width: 1440px) {
            @include padding-start(0);
            @include padding-end(0);
        }              
    }  
    .slideshow-search {
        position: relative;
    }  
    .searchbox {
        @include ab(absolute, block, 20px, 15px, n, 15px);
        z-index: 99;
        @media (max-width: 1199px) {
            top: 10px; 
        }
        .navbar-form {
            height: 54px; 
            width: 100%;
            position: relative;
            @media (max-width: 1199px) {
                height: 48px; 
            }
            input {
                padding: 13px 20px;
                height: 100%; 
                border: 2px solid $main-color; 
                background: $header-search-bg;
                color: $header-search-text;
                &:focus {
                    background: $header-search-bg;  
                }
            }
            button {
                @include ab(absolute, block, 50%, n, n, 23px); 
                z-index: 3;
                text-align: center;
                padding: 0;
                margin: 0;
                border: none;
                font-size: 20px; 
                color: $header-search-icon; 
                background: transparent;     
                @include translate(0, -50%); 
                &:hover {
                    color: $body-link-color-hover; 
                } 
            }
        }
        .result-ajax-search {
            width: 100%; 
        }
    } 
} 
.home-slideshow-wrapper {
    &:before {
        content: "";
        @include ab(absolute, block, 0, 0, 0, 0);
        background: rgba(214, 214, 214, 0.14);     
    }
    &.swiper-container-initialized:before {
        content: none;     
    }     
} 
.bc-home-slideshow {
    position: relative;      
    .swiper-slide {
        position: relative;
        height: auto;
        margin: 0 auto;
        text-align: center;
        @include opacity(0);
        .img-mobile { 
            @media (min-width: 768px) {
                display: none;  
            }      
        }
        .img-desk { 
            @media (max-width: 767px) {
                display: none;
            }
        }
        &.has-video {
            .container-box {
                @include opacity(0);
            }     
            .video {
                min-width: 100%;
                min-height: 100%; 
                width: auto;
                height: auto;     
                @include ab(absolute, block, 50%, 50%, n, n);
                @include translate(-50%, -50%);     
            }           
            iframe {
                max-width: 100%;
                width: 100%;
                height: 100%;
            }
            &:hover {
                .container-box {
                    @include opacity(1);
                }  
            }
        }
        &.swiper-slide-active {
            @include opacity(1); 
        }  
    }
    .swiper-content {   
        @include ab(absolute, block, 0, 0, 0, 0);
        z-index: 99;  
        .container-box {
            position: absolute;        
            padding: 0 10px; 
            display: table;
            width: -moz-fit-content;
            width: fit-content;
            &:not(.bg-color-none) {
                min-width: 470px;
                @media (max-width: 1199px) {  
                    min-width: auto;    
                }                
            }      
        }              
    } 
    &:hover {
        .swiper-button-white {
            @media (min-width: 1025px) {
                @include opacity(0.2);
            } 
        }
    }  
    .slideshow-image-gallery {   
        &.number-5 >div >div { 
            max-width: 20%;
            @include flex(0 0 20%);
        }    
        .title {
            margin: 0;
        }
        .btn-banner {
            margin-top: 27px;
            @media (max-width: 1199px) {
                margin-top: 15px;
            }         
        } 
        .bottom-center .text { 
            @include ab(n, block, n, n, 10px, n);
            padding: 0 10px;
            width: 100%;
            .wrap-title {
                text-align: center;
            }
            .btn-banner {
                display: block; 
                padding: 2px 5px;
                margin-top: 10px;   
            }      
        } 
        &.position-overlay {
            @include width-boxed;
            @include ab(absolute, block, n, 0, 9%, 0);      
            z-index: 9;
            @media (max-width: 1440px) {
                @include ab(n, n, n, n, 3%, n);
            } 
            .center-left .text {
                @include ab(n, n, n, 15%, n, n);
            }  
            .center-right .text {
                @include ab(n, n, n, n, n, 15%);
            }
            .image-gallery-border {
                padding: 10px 5px 0; 
                margin: 0;
            }  
        }                    
        &.position-overlay-2 {
            @include width-boxed; 
            margin-top: -110px;  
            position: relative;
            z-index: 9;
            @media (max-width: 1199px) {
                margin-top: -60px; 
            } 
            .center-left .text {
               @include ab(n, n, n, 10%, n, n);
            }   
            .center-right .text {
                @include ab(n, n, n, n, n, 10%);
            }   
            .image-gallery-border {
                padding: 10px 5px 0; 
                margin: 0;
            }            
        }
        &.position-overlay-3 { 
            @include ab(absolute, block, 40px, n, n, 7.5%);     
            z-index: 9;  
            width: 27.2%;  
            >div {
                display: block;  
                >div {
                    max-width: 100%;
                    @include flex(0 0 100%);
                    display: none;
                    margin-bottom: 20px;
                    &:nth-child(-n+3) {
                        display: block; 
                    }   
                    &:nth-of-type(3) {
                        margin-bottom: 0;
                    }   
                }    
            }
            .center-left .text {
                @include ab(n, n, n, 13%, n, n);  
            }
            .center-right .text {
                @include ab(n, n, n, n, n, 13%); 
            }
            .image-gallery-border { 
                padding: 10px 5px; 
            }
        }
        &.position-overlay-4 {
            margin-top: -17%;  
            @include margin-start(307px);
            position: relative;
            z-index: 9; 
            @media (max-width: 1199px) {
                @include margin-start(0);  
            }
            >div >div {
                max-width: 33.333333%;
                @include flex(0 0 33.333333%);
                &:first-child {
                    max-width: 66.666667%;
                    @include flex(0 0 66.666667%); 
                }
                &:last-child {
                    max-width: 100%;
                    @include flex(0 0 100%);
                    margin-bottom: 0;
                }  
            } 
            .center-left .text {
                @include ab(n, n, n, 7%, n, n);
            }
            .center-right .text {
                @include ab(n, n, n, n, n, 7%);
            } 
            .bottom-center .text {
                .wrap-title {
                    display: block;  
                    @include padding-end(0);
                }
                .btn-banner {
                    display: block;
                    margin-top: 15px; 
                }  
            }
            .text { 
                .wrap-title {
                    display: inline-block;
                    vertical-align: middle;
                    @include padding-end(30px); 
                }
                .btn-banner {
                    display: inline-block;
                    vertical-align: middle; 
                    margin-top: 0;
                }   
            } 
            .image-gallery-border { 
                padding: 10px 0;  
            } 
        }
    }
    &.image-gallery-overlay-1,
    &.image-gallery-overlay-2 {
        .swiper-pagination.left-bottom { 
            @include ab(n, n, n, 150px, 35%, n);
            @media (max-width: 1199px) {
                @include ab(n, n, n, 100px, n, n);
            }    
            @media (max-width: 991px) {
                @include ab(n, n, n, n, 4%, n);
            }  
        }
    }
    &.image-gallery-overlay-3 {
        .swiper-button-white {
            &.swiper-button-prev {
                @include ab(n, n, n, 0, n, n); 
            }       
            &.swiper-button-next {
                @include ab(n, n, n, n, n, 0);
            }  
        }  
    } 
    &.image-gallery-overlay-1,
    &.image-gallery-overlay-2,
    &.image-gallery-overlay-3,  
    &.image-gallery-overlay-4, 
    &.image-gallery-right {
        .container-box>div.caption-link .slider-button {
            padding: 5px 48px;
        }       
    }    
    &.image-gallery-right,  
    &.image-gallery-right-2 {        
        .slideshow-image-gallery {
            >div {
                @include margin-end(0); 
                @include margin-start(0);    
                >div {
                    padding: 0; 
                    margin-bottom: 0;
                    max-width: 100%;
                    border-top: 1px solid $body-border-color;
                    display: none;
                    @include flex(0 0 100%);
                    &:first-child {
                        border-top: none;
                    }   
                    &:nth-child(-n+3) {
                        display: block; 
                    }    
                }          
            } 
            .banner-item {
                &.center-left .text {
                    @include ab(n, n, n, 10%, n, n); 
                }   
                &.center-right .text {
                    @include ab(n, n, n, 44%, n, n); 
                }  
            }        
        }   
        .swiper-button-white {
            &.swiper-button-prev {
                @include ab(n, n, n, 10px, n, n); 
            }       
            &.swiper-button-next {
                @include ab(n, n, n, n, n, 10px);
            }
        }
    }   
    &.image-gallery-overlay-4 {
        .swiper-pagination.center-bottom {
            @include ab(n, n, n, n, 39%, n);
            @media (max-width: 1440px) { 
                @include ab(n, n, n, n, 44%, n);
            }     
            @media (max-width: 991px) {
                @include ab(n, n, n, n, 4%, n);
            }   
        }
        .block-categories {
            top: 32px;
            padding: 0; 
            @include box-shadow(none);
        }  
        .wrap-list {
            border: 2px solid $main-color;  
        }
        .block-service-bar {
            border: 1px solid $body-border-color; 
            border-top: none; 
        }     
    }
    .image-gallery-border {
        border: 2px solid $progress-bar-secondary-border;  
    }
    .block-service-bar {
        border-top: 1px solid $body-border-color; 
        padding: 0 30px;
        .item {
            border-bottom: 1px solid $body-border-color;
            padding: 16px 0 11px;   
            &:last-child {
                border-bottom: none; 
            }  
        } 
        .icon {
            display: inline-block;
            vertical-align: middle;
            width: 24px;
            i {
                font-size: $font-icon; 
            }
        }  
        .text {
            display: inline-block;
            vertical-align: middle;
            @include padding-start(15px);
            font-size: $body-font-size - 2px;
        }    
    }   
    .block-categories {
        @include ab(absolute, block, 50px, n, n, n);    
        z-index: 999;
        width: 270px;                  
        background: $body-background-color;         
        @include box-shadow(0 3px 2px rgba(1, 1, 1, 0.13));
        @media (max-width: 1199px) {
            display: none;  
        } 
        svg {
            width: 25px;
            height: 25px; 
        }                         
        >ul >li >a {
            color: $body-link-color;
            &:hover {
                color: $body-link-color-hover;
            }   
        }                
        ul.categories-list {
            border-bottom:  1px solid $body-border-color;  
            padding: 10px 0 15px;   
            >li {
                @include flexbox;
                flex-wrap: wrap;
                align-items: center;
                padding: 10px 30px;           
                .image {
                    width: 25px;
                    @include margin-end(18px);
                }                  
                >a {
                    font-size: $body-font-size;
                    font-weight: $font-bold;        
                } 
            } 
        }
        ul.menu-list {
            padding: 17px 0 20px;    
            >li {
                padding: 5px 30px;
                >a {
                    font-size: $body-font-size - 1px; 
                }     
            }     
        }
    }               
}
.icon-btn-banner {
    .container-box>div.caption-link .slider-button {
        @include icon-btn-banner;   
    }  
}
.btn-round { 
    .container-box>div.caption-link .slider-button {
        padding: 6px 50px;
        @include border-radius-50;
    }    
}   
.container-box { 
    padding: 0;        
    position: relative;    
    > div {        
        background: transparent;  
        line-height: 1.0;
        padding: 0;     
        margin: 0 auto;   
        font-family: $font-heading;  
        position: relative;                                                         
        -webkit-animation-duration: 1s;
        animation-duration: 1s;
        -webkit-animation-fill-mode: backwards;
        -moz-animation-fill-mode: backwards;
        -ms-animation-fill-mode: backwards;
        -o-animation-fill-mode: backwards;
        animation-fill-mode: backwards;
        -webkit-animation-iteration-count: 1;
        animation-iteration-count: 1;              
        font-weight: 300; 
        b, strong {
            font-weight: $font-bold; 
        }
        &.heading {   
            margin: 0 auto 1.4%;
            padding: 0;                     
            line-height: 0.9;    
            @media (max-width: 991px) { 
                font-size: 3vw !important;    
            }  
            &.bold-true {
                font-weight: $font-bold;
            }           
        } 
        &.subheading {              
            line-height: 1;      
            margin: 0 auto 0.5%;   
            @media (max-width: 991px) { 
                font-size: 4vw !important;        
            }             
            @media (max-width: 767px) { 
                font-size: 3.5vw !important;                  
            }  
            .big-font {
                font-weight: $font-bold;
            }     
            &.bold-true {
                font-weight: $font-bold;
            }           
        }
        &.cation {             
            margin: 0;                           
            line-height: 1;                       
            display: inline-block;   
            @media (max-width: 991px) {  
                font-size: 4vw !important;         
            }                        
            @media (max-width: 767px) { 
                font-size: 3.5vw !important;        
            }
            .big-font {
                font-weight: $font-bold;
            }
            span {
                text-decoration: underline;
                color: $main-color;       
            }   
            &.bold-true {
                font-weight: $font-bold;
            }                  
        }
        &.caption-link {
            margin: 5% 0 0;       
            position: relative;    
            @media (max-width: 767px) {
                display: none;
            }  
            .slider-button {                    
                font-weight: $font-bold;    
                font-size: $body-font-size + 1px;
                padding: 13px 58px;
                line-height: 26px;
                @include transform-text;
                display: inline-block; 
                @include border-radius-reset;  
                @include margin-end(6px);
                text-transform: initial;  
                @media (max-width: 1024px) {
                    display: none;
                }   
                @media (max-width: 767px) {         
                    font-size: $body-font-size;
                    padding: 5px 25px;
                } 
                &:hover {
                    text-decoration: underline; 
                }          
            }
        }
        .bg-label {
            background: #000;
            padding: 20px 15px;
            margin-top: 20px;
            display: block;
            text-decoration: none !important;    
            @include border-radius(5px, 0px, 0px, 5px);
            position: relative;
            @media (max-width: 991px) {
                @include border-radius(5px, 5px, 5px, 5px);
            }
            &:before {
                content: "";
                @include ab(absolute, n, -1px, n, n, -29px);
                height: 100%;
                border-top: 41px solid transparent;
                @include border-start(30px solid #000);  
                border-bottom: 41px solid transparent;
                @media (max-width: 991px) {
                    content: none;
                }
            }
        }
    }  
}
.flex-header-left {
    .container-box>div {
        &.heading {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 3.5vw !important; 
            }  
        }
        &.subheading {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 4.5vw !important; 
            } 
        }
        &.cation {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 4.5vw !important; 
            }  
        }
    }
    .banner-item .text .title {
        @media (max-width: 1440px) and (min-width: 992px) {
            font-size: 3vw !important; 
        } 
    }
}  
.wide:not(.flex-header-left) {
    .bc-wrapper.layout-full .swiper-button-white { 
        &.swiper-button-prev {
            @include ab(n, n, n, 45px, n, auto);
            @media (max-width: 1024px) {
                @include ab(n, n, n, 0, n, n);
            }
        }
        &.swiper-button-next {
            @include ab(n, n, n, auto, n, 45px);  
            @media (max-width: 1024px) {
                @include ab(n, n, n, n, n, 0);  
            }   
        }  
    }
}
.btn-round .swiper-button-white {
    width: 30px;
    height: 30px;
    @include border-radius-circle;
    &:before {
        line-height: 30px;
        font-size: $body-font-size; 
    }  
}    
.swiper-button-white {  
    z-index:4; 
    height: 60px; 
    width: 60px;  
    background-image: none;  
    border: none;
    text-align: center;         
    background: none;
    outline: none; 
    color: $btn-carousel-txt;
    background: $btn-carousel-bg;
    margin: 0;  
    @include ab(absolute, block, 50%, n, n, n);
    @include translate(0, -50%);
    @include transition(all 0.35s ease 0s);          
    @media (min-width: 1025px) {
        @include opacity(0);
    }  
    @media (max-width: 767px) {
        width: 40px;
        height: 40px;
    } 
    &:before {
        display: block;              
        text-align: center; 
        font-size: $font-small-icon;  
        line-height: 60px;
        width: 100%;
        height: 100%; 
        font-family: $font-family-icon;   
        @media (max-width: 767px) {
            line-height: 40px;        
        }
    } 
    &.swiper-button-prev {
        background-image: none;  
        @include ab(n, n, n, 0, n, auto);     
        &:before {
            content: "\e819";        
        }   
    }
    &.swiper-button-next {
        background-image: none;   
        @include ab(n, n, n, auto, n, 0);  
        &:before {
            content: "\e818";      
        } 
    }  
    &:hover {
        @media (min-width: 1025px) {
            background: $btn-carousel-bg-hover;
            color: $btn-carousel-txt-hover;  
            @include opacity(1 !important);
        }     
    } 
}  
.swiper-pagination {
    z-index: 2;
    @include ab(absolute, block, n, 50%, 4%, n);
    @include translate(-50%, 0);
    &.right-center { 
        @include ab(n, n, 50%, auto, auto, 8%);
        @include translate(0, -50%); 
        @media (max-width: 767px) {
            @include ab(n, n, n, n, n, 15%);
        }      
        .swiper-pagination-bullet {
            display: block;
            margin: 7px auto;
            top: 0;
        } 
    }
    &.left-center { 
        @include ab(n, n, 50%, 8%, auto, n);
        @include translate(0, -50%); 
        @media (max-width: 767px) {
            @include ab(n, n, n, 15%, n, n);
        }               
        .swiper-pagination-bullet {
            display: block;
            margin: 7px auto;
            top: 0;
        }     
    }
    &.left-bottom {
        @include ab(n, n, n, 14%, 5%, n);       
    }  
    &.round-2 .swiper-pagination-bullet {
        top: 0;
        background: $text-other; 
        margin: 0 4px;     
        &:hover,  
        &.swiper-pagination-bullet-active {
            background: $black;   
            width: 8px;
            height: 8px; 
        }   
    }                
    .swiper-pagination-bullet {
        position: relative;
        width: 8px;
        height: 8px;
        margin: 0 7px;
        background: $black;
        outline: none; 
        top: -3px;
        @include opacity(1);
        @include border-radius-circle;
        @include transition(all 0.3s ease 0s);
        &:hover,
        &.swiper-pagination-bullet-active {
            background: transparent;
            border: 3px solid $black;    
            width: 18px;
            height: 18px;
            top: 0; 
        } 
    }
    &.center-bottom-2 {
        position: static;
        margin-top: -7px;       
        @include translate(0, 0);
    }                   
    &.line {
        &.left-center, &.right-center {
            .swiper-pagination-bullet {
                height: 40px;
                width: 3px;
                &:hover,        
                &.swiper-pagination-bullet-active {
                    height: 40px;
                    width: 3px;
                }               
            }
        }
        .swiper-pagination-bullet {
            width: 40px;
            height: 3px;
            @include border-radius-reset;   
            background: rgba($black, 0.1);
            top: 0;      
            &:hover,        
            &.swiper-pagination-bullet-active {
                background: $black;  
                border: none;    
                width: 40px;
                height: 3px; 
                top: 0;
            }          
        }
    }   
}
.swiper-wrapper {  
    .owl-controls .owl-dots {
        @include ab(absolute, block, n, n, 6%, 0);
        margin: 0 auto;
        text-align: center;
        width: 100%;
        .owl-dot {
            margin: 0 5px !important;
            display: inline-block;
            line-height: 20px;
            span {          
                width: 12px !important;
                height: 12px !important; 
                margin: 0;     
                background: $main-color;
                border: 1px solid $main-color; 
                @include border-radius-circle;
                @include opacity(0.5);
                position: relative;
                display: inline-block;
                padding: 0;                
            }
            &:hover,
            &:focus,
            &.active {
                span {  
                    @include opacity(1);
                }
            }
        }
    }
}
