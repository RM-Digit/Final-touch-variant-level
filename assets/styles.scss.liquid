/*=== Customize Block ===*/
.custom-content-width { 
    .col-md-12 {    
        padding: 0;
        margin: 0 auto;   
    }
    .custom-content .custom-item {
        &.pad-left {   
            @media (min-width: 768px) {
                @include padding-end(30px);       
            }
            @media (min-width: 1200px) {
                @include padding-end(70px);     
            } 
        }
        &.pad-right {
            @media (min-width: 768px) {
                @include padding-start(30px);       
            }
            @media (min-width: 1200px) { 
                @include padding-start(70px);     
            }
        }
    }
    &.wide .custom-content .custom-item {  
        &.pad-left { 
            @media (max-width: 767px) {
                padding: 0 25px;
            }
            @media (min-width: 768px) {   
                @include padding-start(calc((100vw - (760px - 30px)) / 2));     
            }
            @media (min-width: 992px) {
                @include padding-start(calc((100vw - (980px - 30px)) / 2));   
            }
            @media (min-width: 1200px) {
                @include padding-start(calc((100vw - (1200px - 30px)) / 2)); 
            }
            @media (min-width: 1440px) {
                @include padding-start(calc((100vw - (#{$container-width} - 30px)) / 2));     
            }
        } 
        &.pad-right { 
            @media (max-width: 767px) {
                padding: 0 25px;
            } 
            @media (min-width: 768px) {   
                @include padding-end(calc((100vw - (760px - 30px)) / 2));     
            }
            @media (min-width: 992px) {
                @include padding-end(calc((100vw - (980px - 30px)) / 2));   
            }
            @media (min-width: 1200px) {
                @include padding-end(calc((100vw - (1200px - 30px)) / 2)); 
            }
            @media (min-width: 1440px) {
                @include padding-end(calc((100vw - (#{$container-width} - 30px)) / 2));     
            } 
        }      
    }  
} 
.custom-content-header {
    margin: 0 auto 3%;
}
.custom-item-inner-video,
.custom-item-inner-collection,
.custom-item-inner-html {
    display: block;
}
.custom-block-heading-text,
.custom-block-body-text {  
    @include margin-start(auto);
    @include margin-end(auto);
}
.custom-block-heading-text {
    font-weight: $font-bold;
}
.custom-block-body-text p {
    font-size: inherit;
    margin: 0;
    line-height: 1.1; 
} 
.custom-content { 
    @include flexbox();
    width: 100%;
    -ms-flex-align: stretch;
    -webkit-align-items: stretch;
    -moz-align-items: stretch;
    -ms-align-items: stretch;
    -o-align-items: stretch;
    align-items: stretch;
    -webkit-flex-wrap: wrap;
    -moz-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    width: auto;    
    padding: 0; 
    .placeholder-svg {    
        fill: rgba($main-color, 0.35);
        width: 100%;
        height: 100%;
        max-width: 100%;
        max-height: 100%;
        background: #f7f7f7;
    }
    .info {   
        padding: 15px 20px;
    } 
    .custom-block-heading-text {
        margin: 0 auto 3%;
        &.big-size {
            @media (max-width: 1440px) {      
                font-size: 25px !important;       
            }      
        } 
    }
    .custom-block-body-text.big-size {
        @media (max-width: 1440px) {      
            font-size: 20px !important;       
        }       
    }
    .btn-banner {
        text-transform: capitalize; 
    }
}
.custom-item {  
    margin: 0 0 30px; 
}
.custom-item-inner {
    position: relative;  
    max-width: 100%;
    &.custom-item-inner-bgimage {
        max-width: 1170px;
        margin: 0 auto;
        padding: 0 4%;    
        .custom-block-heading-text {
            max-width: 100%;
        }    
    } 
}
.custom-item-inner-product .image .padding {
    margin-bottom: 15px; 
    display: block; 
}
.custom-item-inner-product,
.custom-item-inner-collection {
    background: #ebebeb;
    .info {
        > a {
            margin: 0;  
            line-height: 1.1; 
            color: $body-link-color;  
            font-size: $size-h5;  
            text-transform: uppercase;
            font-family: $font-heading;
            @media (max-width: 479px) {  
                @include transform-text;
            }
        }
        .product-author {
            color: $body-general-color;
            font-size: $body-font-size;      
            padding: 0 0 10px;
        }
        .product-name {
            color: $body-link-color;             
            margin: 0 0 5px;                
            @include transform-text;    
        }
        .product-price {
            display: inline-block;
        }
    }
}
.custom-block-video {
    position: relative;
    overflow: hidden;
    max-width: 100%;
    padding-bottom: 56.25%; 
    height: 0;
    iframe {
        @include ab(absolute, block, 0, 0, n, n);
        width: 100% !important;
        height: 100% !important;
        max-width: 100% !important;
        max-height: 100% !important;
        margin: 0 !important;
        -webkit-transition-duration: 0.35s !important;  
        transition-duration: 0.35s !important;
    }
    .mbYTP_wrapper {
        -webkit-transition-duration: 0.35s !important;  
        transition-duration: 0.35s !important;
        z-index: 2 !important;
    }
    .player {
        @include ab(absolute, block, 0, 0, n, n);
        display: block !important;
        width: 100%;
        height: 100%;
        background-image: url({{'loading.gif' | asset_url }});  
        background-position: center center;
        background-repeat: no-repeat;
        z-index: 1;
        + button {
            @include opacity(0);
        }
        &.isMuted {
            background-image: none;
            + button {
                @include opacity(1);
            } 
        }
    }
    .buttonBar {
      display: none;
    }
    button {
        @include ab(absolute, block, 50%, 50%, n, n);
        @include translate(-50%, -50%);    
        font-size: 0;    
        background: none;
        border: none;
        width: 80px;
        height: 80px;
        line-height: 50px;
        color: #222;
        text-align: center;
        z-index: 3;
        @include border-radius(3px, 3px, 3px, 3px);
        @include transparent($white, 0.5);
        @include transition(all ease-in-out 0.35s 0s);
        span {
            display: none;
        }
        &:before {
            font-size: 40px;   
            display: block;
        }
        &.b-play {
            &:before {
                content: '\ee906';
            }
        } 
        &.b-pause {
            @media (min-width: 1025px) {
                @include opacity(0);
            }      
            &:before {
                content: "\60";
                font-family: 'ElegantIcons';
            }
        }
    }
    &:hover {
        button.b-pause {
            @include opacity(1);
        }
    }
} 
.image-layer {
    @include ab(absolute, block, 50%, 0, n, 0);
    z-index: 2;
    max-height: 100%;
    max-width: 100%;
    margin: 0 auto;
    padding: 0 15px;
    @include translate(0, -50%);   
    @media (min-width: 768px) {
        width: 760px;
    }
    @media (min-width: 992px) {
        width: 980px;
    }
    @media (min-width: 1200px) {
        width: 1200px;
    }
    img {
        max-width: 100%;
        max-height: 100%;
        display: inline-block;  
        @media (min-width: 320px) and (max-width: 479px) {
            max-width: 69%;
        }
        @media (min-width: 480px) and (max-width: 767px) {
            max-width: 49%;
        }
        @media (min-width: 768px) and (max-width: 1024px) {
            max-width: 60%;
        }
        @media (min-width: 1025px) and (max-width: 1199px) {
            max-width: 64%;
        }
    }
    &.layer-center {    
        text-align: center;    
    }
    &.layer-left {
        text-align: start;
    }
    &.layer-right {
        text-align: end;
    } 
}    

