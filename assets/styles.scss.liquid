/*=== Section lookbook ===*/
.lookbook-section {
    .lookbook-title {
        padding-top: 37px;    
        @media (max-width: 991px) { 
            padding-bottom: 30px !important;  
        }
        @media (max-width: 767px) {
            padding-top: 20px; 
            padding-bottom: 15px !important;
        } 
        .heading {
            font-weight: 300;
            padding: 0;
            margin: 0;
            color: $body-general-color;
            &.big-size {
                @media (max-width: 991px) {
                    font-size: 30px !important;
                }
                @media (max-width: 767px) {
                    font-size: 20px !important; 
                }
            }
        }
        .subheading {
            font-weight: $font-bold;
            line-height: 1.0;
            color: $body-general-color;
            &.big-size { 
                @media (max-width: 991px) {
                    font-size: 40px !important;
                }
                @media (max-width: 767px) {
                    font-size: 30px !important; 
                }
            }
            span {
                color: $main-color;
                text-decoration: underline;
            }
        }
    }
    .lookbook-content .row >div {
        margin-top: 20px;
        &:first-child {
            @media (max-width: 767px) {
                margin-top: 0;  
            } 
        }
        @media (min-width: 768px) { 
            &.col-md-6:nth-child(-n+2) { 
                margin-top: 0; 
            } 
            &.col-12:not(.col-md-6) {
                &:first-child {
                    margin-top: 0; 
                }
                + .col-md-6 {
                    margin-top: 20px;
                }
            }
        }
    } 
    .lookbook {
        position: relative;
        .lookbook-text {
            position: absolute;
            display: table;  
            font-weight: $font-bold;
            z-index: 1;
            line-height: 1.0;
            .heading {
                @media (max-width: 767px) {
                    font-size: 4vw !important;
                }
            }
            .subheading {
                margin-top: 20px;  
                @media (max-width: 767px) {
                    margin-top: 10px;      
                    font-size: 2vw !important;  
                }      
            }
        }  
        .pin { 
            position: absolute; 
            display: block;
            .open-pin {
                position: relative;
                display: block;
                width: 42px;
                height: 42px;
                line-height: 36px;   
                font-size: 18px;
                font-weight: $font-bold;
                text-align: center;
                z-index: 1;
                cursor: pointer;
                @include border-radius-circle;
                @media (max-width: 767px) {
                    width: 24px;
                    height: 24px;
                    line-height: 20px;
                    font-size: 14px;
                    border: 2px solid $white;
                }
                &:hover {
                    text-decoration: underline;
                }
            }
            .pin-content {
                position: relative;
                z-index: 99;
                display: block;
                margin: -60px 0 0;
                @include margin-start(20px);
                width: 212px;
                background: $body-background-color;
                @include opacity(0); 
                visibility: hidden;
                height: 0; 
                @include box-shadow(0px 10px 20px 0px rgba(0, 0, 0, 0.1));
                @include transition(opacity 0.2s ease-in-out);
                @media (max-width: 767px) {
                    width: 200px;
                    margin: -48px 0 0;    
                    @include margin-start(-88px);
                }
            }  
            &.active {
                .open-pin {
                    z-index: 999;     
                }
                .pin-content { 
                    @include opacity(1);
                    visibility: visible;
                    height: auto;
                } 
            }
        }
        .product-wrapper {
            .product-head .product-image img {
                @include transition(none); 
            }
            .detail-deal {
                display: none; 
            }
            .product-label {
                @include ab(n, n, n, auto, n, 0);
            } 
            .product-content {
                text-align: center;
            }
            .product-button {
                @include flexbox;
                flex-wrap: wrap;
                align-items: center;
                justify-content: center;
                padding: 7px 0 0;
                >div {
                    @include icon-product();  
                }
            }
            .product-name {
                padding-top: 15px; 
            } 
            .product-add-cart {
                background: $button-btn1;
                color: $button-btn1-txt; 
                margin: 5px 0 0; 
                padding: 9px 10px 10px;
                text-transform: uppercase;
                font-weight: $font-bold;  
                a {
                    color: inherit;  
                }   
                .demo-icon {
                    @include margin-end(10px);
                    display: none;
                } 
            }
        }
    }
}
/*=== Section newsletter - layout 1, 2 footer  ===*/ 
.input-bg-white, .input-bg-none {
    .newsletter-container, 
    .sb-newsletter {  
        form .newsletter-checkbox label:before {
            border: 1px solid $body-border-color; 
        }
    }                 
}  
.newsletter-container, 
.sb-newsletter {  // Left column market-gray theme
    form {
        max-width: 675px;
        margin: 0 auto;
        position: relative;
        display: block;
        .action-form {
            @include flexbox;
            justify-content: center; 
            @media (max-width: 575px) {
                display: block;  
            }
            input {
                width: 380px;   
                display: block;
                @include margin-end(10px);
                @media (max-width: 575px) {
                    width: 100%;
                    @include margin-end(0);
                }  
            }
            button {
                text-transform: uppercase;
                @media (max-width: 575px) {
                    width: 100%;
                    margin-top: 15px;
                }
            }
        }
        .newsletter-checkbox {
            margin: 13px 0 0;   
            position: relative;
            display: inline-block; 
            text-align: start;
            input {
                @include ab(absolute, block, 0, 0, n, n); 
                width: 75px;
                height: 100%;
                cursor: pointer; 
                z-index: 2;
            } 
            input:checked ~ label {
                &:after {
                    content: "\e83d"; 
                    font-size: 14px;
                    font-family: $font-family-icon;
                    color: $main-color;
                    z-index: 1;
                    width: 20px;
                    height: 20px;
                    line-height: 20px;
                    text-align: center;
                    @include ab(absolute, block, 0, 0, n, n);
                }    
            } 
            label {
                font-size: $body-font-size - 2px;
                position: relative;
                display: inline-block;
                cursor: pointer;
                color: $text-other;
                @include padding-start(30px);
                margin-bottom: 0;  
                &:before {
                    content: "";
                    @include ab(absolute, block, 0, 0, n, n);
                    width: 20px;
                    height: 20px;
                    background: $input-bg;
                }      
                a {
                    color: inherit;
                    padding: 0 5px;
                    text-decoration: underline;
                    &:hover  {
                        color: $body-link-color-hover;
                    }     
                }      
            }
        }
    } 
}          
.newsletter-container {
    background-position: top center;
    background-repeat: no-repeat;
    background-attachment: scroll;
    background-size: cover;   
    .newsletter-inner {
        position: relative;
    }
    .nt-wrapper {
        .newsletter-title {
            font-size: $size-h1;
            font-weight: $font-bold; 
            @media (max-width: 767px) {
                font-size: $size-h1 - 10px; 
            } 
        }
        p {
            margin: 2px 0 0;
            color: $text-other;
        }
        &.bigger-size .newsletter-title {
            font-size: $size-h1 + 10px; 
            @media (max-width: 767px) {
                font-size: $size-h1;    
            }
        } 
    }                      
} 
