	.address {
		padding: 0 0 19px;
		color: $footer-text-color;
		@media (max-width: 767px) {
			padding: 0 0 15px;
		}
		span {
			color: $footer-copyright-color;
			font-size: $body-font-size - 2px;
		}
		&.flex {
			float: left;
			@media (max-width: 1199px) {
				float: none;
			}
