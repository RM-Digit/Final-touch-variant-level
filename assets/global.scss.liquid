}
/*=== Footer service-bar ===*/
.footer-service-bar {
	.content-inner {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
	}
	.text-position-right-center .item {
		align-items: center;
	}
	.border-right-item {
		.row-1>div:not(.line) {
			@include border-start(1px solid rgba($footer-text-color, 0.2));
			@media (max-width: 767px) {
				@include border-start(none);
			}
			&:first-child {
				@include border-start(none);
				.item {
					@include padding-start(0);
				}
			}
		}
		.item {
			@include padding-start(40px);
			@media (max-width: 1199px) {
				@include padding-start(15px);
			}
			@media (max-width: 767px) {
				@include padding-start(0);
			}
		}
	}
	.border-radius-round {
		.service-bar-container {
			border: 1px solid rgba($footer-text-color, 0.2);
			padding: 12px 10px;
			@include border-radius-50;
			@media (max-width: 767px) {
				padding: 5px 10px;
			}
			.row-1 {
				.flex-item {
					@media (min-width: 768px) {
						max-width: 100%;
						flex: 1;
					}
				}
				>div:first-child .item {
					@media (max-width: 767px) {
						border-top: none;
					}
				}
				.item {
					@media (max-width: 767px) {
						padding: 5px 10px !important;
						border-top: 1px solid rgba($footer-text-color, 0.2);
					}
				}
			}
		}
	}
	.border-radius-square {
		.service-bar-container {
			border: 1px solid rgba($footer-text-color, 0.2);
			.row-1 {
				.flex-item {
					@media (min-width: 768px) {
						max-width: 100%;
						flex: 1;
					}
				}
				>div {
					@media (min-width: 768px) {
						padding: 0 10px;
					}
					&:first-child .item {
						@include padding-start(10px);
						@media (max-width: 767px) {
							border-top: none;
						}
					}
					&:last-child .item {
						@include padding-end(10px);
					}
				}
				.item {
					padding: 7px 10px;
					@media (max-width: 767px) {
						padding: 5px 10px !important;
						border-top: 1px solid rgba($footer-text-color, 0.2);
					}
				}
			}
		}
	}
	.line {
		border-bottom: 1px solid rgba($footer-text-color, 0.2);
		flex: 1;
		@media (max-width: 991px) {
			display: none;
		}
		&:last-child {
			display: none;
		}
	}
	.text-position-below .item {
		display: block;
		text-align: center;
		@include padding-start(0);
		.icon {
			margin: 0 auto;
			@include padding-end(0);
		}
		.text {
			padding-top: 10px;
		}
	}
	.item {
		@include flexbox;
		position: relative;
		@media (max-width: 767px) {
			padding-bottom: 10px;
		}
		.icon {
			color: $main-color;
			@include padding-end(20px);
			@include flex(none);
			i:before {
				font-size: 45px;
				@media (max-width: 767px) {
					font-size: 30px;  
				}
			}
		}
		.text {
			.heading {
				line-height: 1.1;
				font-weight: $font-bold;
				span {
					text-decoration: underline;
				}
			}
			p {
				padding-top: 5px;
				font-size: $body-font-size;
				color: $footer-copyright-color;
				margin: 0;
			}
		}
	}
	.style-1 {
		.row-1>div {
			@media (min-width: 768px) {
				max-width: 33.33333%;
				@include flex(0 0 33.33333%);
			}
		}
	}
	.style-2,
	.style-4 {
		.row-1 {
			align-items: center;
			>div:not(.line) {
				max-width: 100%;
				width: auto;
				margin: 0 40px;
				@include border-start(none);
				@include flex(0 0 auto);
				@media (max-width: 1199px) {
					margin: 0 10px;
				}
				@media (max-width: 991px) {
					margin: 0;
				}
				@media (max-width: 767px) {
					@include flex(0 0 100%);
				}
				&:first-child {
					@include margin-start(0);
				}
				&:nth-last-child(-n+2) {
					@include margin-end(0);
				}
				.item {
					@include padding-start(0);
				}
			}
		}
	}
	.style-2 {
		>.row-1>div:nth-last-child(2) {
			.item {
				@media (max-width: 767px) {
					padding-bottom: 0;
				}
			}
		}
	}
	.style-3 {
		.item .icon {
			@include padding-end(18px);
		}
	}
	.style-4 {
		.item .icon {
			@include padding-end(18px);
			@media (max-width: 1199px) {
				@include padding-end(10px);
			}
		}
	}
	.style-5 {
		.row-1 {
			justify-content: center;
			.flex-item {
				padding: 0 10px;
				@media (max-width: 767px) {
					width: 100%;
				}
				&:first-child .item {
					@include padding-start(0);
				}
				&:last-child .item {
					@include padding-end(0);
				}
			}
		}
		.item {
			justify-content: center;
			@include padding-start(30px);
			@include padding-end(30px);
			@media (max-width: 1199px) {
				@include padding-start(10px);
				@include padding-end(10px);
			}
			@media (max-width: 767px) {
				justify-content: initial;
				@include padding-start(0);
				@include padding-end(0);
				.icon {
					min-width: auto;
				}
			}
			.text .heading {
				font-weight: 400;
			}
		}
	}
	.style-6 {
		.row-1 {
			.flex-item {
				padding: 0 10px;
				@media (max-width: 767px) {
					width: 100%;
				}
				&:first-child .item {
					@include padding-start(0);
				}
				&:last-child .item {
					@include padding-end(0);
				}
			}
		}
		.item {
			@include padding-start(30px);
			@include padding-end(30px);
			@media (max-width: 1199px) {
				@include padding-start(10px);
				@include padding-end(10px);
			}
			@media (max-width: 767px) {
				@include padding-start(0);
				@include padding-end(0);
				.icon {
					min-width: auto;
				}
			}
			.text .heading {
				font-weight: 400;
			}
		}
	}
	.style-7 {
		.row-1 >div:not(.payment-icons){
			@media (min-width: 992px) {
				max-width: fit-content; 
				@include flex(0 0 auto); 
				@include padding-end(48px); 
			}
		} 
		.item {
			align-items: center; 
		}
		.payment-icons {
			@include margin-start(auto);
			padding: 0 10px; 
			@media (max-width: 1199px) {
				@include margin-start(0);
				padding-top: 15px;  
				width: 100%;  
			}
		}
	}
}
