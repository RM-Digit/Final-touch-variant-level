/*  Images -------------------------- */
.blur-up {
    -webkit-filter: blur(7px);
    filter: blur(7px);
    transition: filter 300ms, -webkit-filter 300ms;
} 
.blur-up.lazyloaded {
    -webkit-filter: blur(0);
    filter: blur(0);
}
.image-lazysize {
    display: block;
}
img {
    max-width: 100%; 
    height: auto;  
    border: 0; 
    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;  
    @include transform(translateZ(0));  
    vertical-align: middle;   
    &.blur-up.lazyloaded {
        object-fit: cover;
        height: 100%;
        font-family: 'object-fit:cover'; 
    }
    &.img-lazy {
        @include ab(absolute, block, 0, 0, n, n);
        width: 100%;
        height: 100%;
    } 
    &.lazyload { 
        @include opacity(0);
    }  
}
.lazyloaded {
    @include opacity(1);
}
.lazyloading {
    @include opacity(0);  
    @include transition(opacity 400ms);
}  
.lazyloaded img.lazyloaded {
    width: auto;
} 
.product-ratio-auto:before {
    content: "";
    display: block;
    padding-bottom: 100%; 
}  
.product-ratio-false:before {
    content: "";
    display: block;
    padding-bottom: 0;
}
.product-ratio-100:before {
    content: "";
    display: block;
    padding-bottom: 100%;
} 
.product-ratio-150:before {
    content: "";
    display: block;
    padding-bottom: 150%;
}
.product-ratio-75:before {
    content: "";
    display: block;
    padding-bottom: 75%;
} 

.placeholder-svg {
    background-color: rgba($body-general-color, 0.1);
    fill: rgba($body-general-color, 0.35);
}
.image { 
    position: relative;  
}

/*  Typography -------------------------- */
h1, h2, h3, h4, h5, h6 {       
    margin: 0;  
    padding: 0; 
    color: $body-link-color;  
    line-height: 1.1;  
    font-family: $font-heading;
    font-weight: $font-bold;
}
h1, .h1 {
    font-size: $size-h1;      
}
h2, .h2 {
    font-size: $size-h2;  
}
h3, .h3 {
    font-size: $size-h3;   
}
h4, .h4 {
    font-size: $size-h4;   
}
h5, .h5 {
    font-size: $size-h5;    
}
h6, .h6 {
    font-size: $size-h6;    
}
.h1, .h2, .h3, .h4, .h5, .h6 {
    margin: 0;  
    line-height: 1.1; 
    color: $body-link-color;  
    font-family: $font-heading;
}
.page-title {
    font-size: $size-h2;  
    font-family: $font-heading;     
}
p {
    margin: 0 0 20px;
    line-height: 22px;  
}
small { font-size: 12px;}
h2 small.pull-right { padding-top: 0.538461538em; }
.bold-text {
    font-weight: 600 !important;  
}  
/*  Links -------------------------- */
a { 
    outline: none; 
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    color: $body-link-color;  
    text-decoration: none;
    outline: none;
    &:focus { text-decoration: none; color: $body-link-color-hover; outline: none;}  
    &:hover { outline: none; color: $body-link-color-hover; text-decoration: underline;}
}
.text-link {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
    color: $body-link-color;
    text-decoration: none;
}
.text-link:hover, 
.text-link:focus {
    @media (min-width: 1025px) {
        color: $body-link-color-hover;
    }
}
/*  Global classes -------------------------------------------------- */
.no-style { margin: 0 !important; padding: 0 !important; border: 0 !important; background: none !important; }
.tal { text-align: start; }
.tar { text-align: end; }
.tac { text-align: center;}
.fl { float: left; }
.fr { float: right; }
.fn { float: none; }
.cl { clear: left; }
.cr { clear: right; }
.cb { clear: both; }
.hide,
.hidden {display: none !important;}
.move {cursor: move;}

/*  Formatting ------------------------------------------ */
.hidden { display: none;}
.move { cursor: move;}
.section-title {  display: none;}
.owl-theme .owl-controls .owl-buttons > div { position: absolute;  }
iframe {
    border: none; 
}
.d-visibility {
    visibility: hidden;
}
.cc-window.cc-floating {
    @media (max-width: 767px) {
        padding: 5%;
    }
}
.cc-window.cc-bottom {
    @media (max-width: 991px) {
        bottom: 22px;
    } 
}
.hide-filter-price .price {
    @media (min-width: 992px) {
        display: none;    
    }  
} 
.price-range-slider {
    @media (max-width: 1024px) { 
        display: none;
    } 
}
