	}
	.phone-email {
		padding-bottom: 32px;
		@media (max-width: 767px) {
			padding-bottom: 15px;
		}
		&.flex {
			float: left;
			padding-bottom: 19px;
			@include padding-start(50px);
			@include padding-end(0);
			@media (max-width: 1440px) {
				@include padding-start(20px);
			}
			@media (max-width: 1199px) {
				float: none;
				padding: 0 0 32px;
			}
			@media (max-width: 767px) {
				padding-bottom: 15px;
			}
			+.widget-social:not(.flex) {
				clear: both;
			}
		}
		&.flex-email {
			@include flexbox;
			margin: 0 -10px;
			>span {
				padding: 0 10px;
			}
		}
		+ .logo-footer {
			padding-top: 5px;    
		}
	}
	&.textbox {
		.text {
			color: $footer-text-color; 
			font-size: $body-font-size; 
			span {
				color: $main-color; 
			}
		}
		.text-1 {
			padding-bottom: 22px;
			@media (max-width: 767px) {
				padding-bottom: 15px;
			}
		}   
	}
	.footer-logo-inner {
		>div:last-child {
			padding-bottom: 0; 
		}
	}
	&.footer-menu {
		.icon {
			@media (max-width: 767px) {
				@include ab(absolute, block, 50%, n, n, 0);
				@include translate(0, -50%);
				&:before {
					content: "\e843";
					font-family: $font-family-icon;
					font-size: $size-h6 - 3px;
					cursor: pointer;
					font-weight: 400;
					display: block;
					text-align: center; 
					width: 20px;
					line-height: 21px;
				}
			}
		}
		ul.f-list {
			@media (max-width: 767px) {
				display: none;
			}
		}
		&.active {
			@media (max-width: 767px) {
				h6 .icon:before {
					content: "\e895";
				}
