					&:not(:last-child) {
						margin-bottom: 15px;
					}
					a {
						color: $footer-text-color;
						font-size: $body-font-size; 
						line-height: 25px;
						font-weight: 400; 
						&:hover {
							color: $footer-hover-color; 
						} 
						i {
							@include padding-end(6px);
						}
					}
				}
			}
			li {
				padding: 0 10px;
				a {
					color: $footer-link-color;
					font-size: 18px;
					&:hover {
						color: $footer-hover-color;
					}
