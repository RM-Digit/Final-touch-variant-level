.footer-block {
	h6 {
		color: $footer-heading-color;
		font-weight: $font-bold;
		position: relative;
		padding-bottom: 31px;
		@media (max-width: 767px) {
			cursor: pointer;
			padding-bottom: 0;
		}
	}
