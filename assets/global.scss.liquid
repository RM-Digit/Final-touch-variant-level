			img {
				max-width: 30px;
				display: inline-block;
				vertical-align: middle;
				@include margin-end(12px);
			}
			a {
				color: $footer-link-color;
				font-size: $body-font-size; 
				line-height: 25px;
				font-weight: 400;
				&:hover {
