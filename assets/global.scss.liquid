                }
                .product-wrapper {
                    margin-top: 0;
                } 
            }
            &:nth-child(3n+1) {   
                &.product-border-right:before {
                    content: none;  
                }  
            }
        }
        &.col-sm-6 {  
            @include flex(0 0 50%);
            max-width: 50%; 
            &:nth-child(-n+2) { 
                &.product-border-top {
                    padding-top: 0;
                    &:after {
                        content: none;
                    }
                }
                .product-wrapper {
                    margin-top: 0;
                } 
            }
            &:nth-child(2n+1) {   
                &.product-border-right:before {
                    content: none;  
                }  
            }
        }
        &.col-sm-12 { 
            &:first-child { 
                &.product-border-top {
                    padding-top: 0;
                    &:after {
                        content: none;
                    }
                }
                .product-wrapper { 
                    margin-top: 0; 
                } 
            }
            &.product-border-right:before { 
                content: none;     
            }
        } 
    }   
}
.flex-header-left {
