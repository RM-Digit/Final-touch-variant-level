/*=== Countdown ===*/   
#home-main-content.col-lg-9 { 
    .product-grid-item  .is-countdown {   
        position: relative;   
        .countdown-amount {
            font-size: $body-font-size - 4px;
            @media (max-width: 1440px) {
                font-size: 9px;   
            }  
        }
        .countdown-period  {
            font-size: 6px; 
        }            
    } 
} 
.is-countdown {
    overflow: hidden;
    background: none;
    border: none;  
    .countdown-row {
        @include flexbox;
        padding: 0; 
    }  
    .countdown-section { 
        background: $body-background-color;
        padding: 5px;  
        width: 25%;
        @include border-end(1px solid $body-border-color); 
        @media (max-width: 1200px) and (min-width: 768px) { 
            padding: 5px 2px;     
        }    
        &:last-child {
            @include border-end(none);
        } 
        .countdown-amount {
            font-size: $body-font-size + 3px;
            font-family: $font-standard;
            font-weight: $font-bold;
            color: $countdown-color;   
            margin: 0;
            padding: 0;
            display: block;
            width: auto;
            text-align: center;  
        }
        .countdown-period { 
            font-size: $body-font-size - 5px; 
            font-weight: $font-bold;
            font-family: $font-standard;
            color: $countdown-text-color;  
            text-transform: uppercase;
            display: block;  
        }
    }
}  
/*=== button theme ===*/
.btn-round .btn {
    @include border-radius-50; 
}     
.btn {     
    padding: 5px 30px; 
    line-height: 26px; 
    font-weight: $font-bold; 
    white-space: normal;
    font-size: $button-size;  
    text-transform: uppercase; 
    @include border-radius-reset;    
    @include transition(all 0.35s ease-in-out 0s);
    @media (max-width: 575px) {
        padding: 2px 15px; 
    }
    &:hover, &:focus, &:active {
        outline: none !important;
        outline-offset: 0 !important;
        text-decoration: underline; 
        @include box-shadow(0 0 0 $white !important);
    }  
}   
