		li {
			clear: both;
			display: block;
			list-style: none;
			&:not(:last-child) {
				border-bottom: 1px solid $body-border-color;
			}
			&.mobile-layout-bar {
				background: $header-bg-mobile-color; 
				.waiting {
					background: transparent;
					padding: 0;
				}
				img {
					image-rendering: pixelated;
				}
				.m-block-icons {
					display: table;
					width: 100%;
					table-layout: fixed;
					>li {
						cursor: pointer;
						display: table-cell;
						vertical-align: middle;
						text-align: center;
						border-bottom: none;
						@include border-end(1px solid rgba($header-text-mobile-color, 0.1));
						>i {
							font-size: $font-small-icon; 
							color: $header-text-mobile-color; 
						}
						>a {
							color: $header-text-mobile-color;
							display: block;
							position: relative;
							padding: 10px 15px;
							&.waiting {
								height: 100%;
							}
							>span {
								position: relative;
							}
							i {
								font-size: $font-small-icon;
							}
							.expand {
								position: static;
								width: auto;
								height: auto;
								line-height: 20px;
								padding: 0;
								display: inline-block;
								&:before {
									content: none;
								}
								i {
									font-size: 14px;
								}
							}
						}
						&:last-child {
							border: none;
						}
						&.wishlist-target .wishlist-icon,
						&.compare-target .compare-icon {
							.number {
								@include icons-number;
								@include ab(n, n, -6px, n, n, -14px); 
								background: $header-icon-mobile-color;
								color: $header-number-on-icon-mobile-color; 
							} 
						}
					}
				}
			}
		}
	}
	.mobile-contact-bar {
		@include ab(fixed, table, n, 0, 0, 0);
		background: $header-bg-mobile-color;
		max-height: 52px;
		width: 100%;
		table-layout: fixed;
		padding: 5px 0;
		li {
			display: table-cell;
			vertical-align: middle;
			text-align: center; 
			a {
				color: $header-text-mobile-color;
				display: block;
				position: relative; 
				font-size: 12px;  
			}
			i {
				display: block;
				font-size: $font-small-icon;
				padding-bottom: 2px;
			}
			span {
				display: block;
			}
		}
	}
	.contactbar-info {
		@include ab(fixed, block, n, 0, 55px, 0);
		border-top: 1px solid $body-border-color;
		padding: 35px 20px 15px; 
		color: $body-general-color;
		background: $body-background-color;
		@include opacity(0);
		visibility: hidden;
		z-index: 9999;
		@include translate(0, 50%);
		@include transition(all 0.35s ease-in-out);
		p {
			margin: 0 0 15px;
		}
		.contactbar-info-close {
			cursor: pointer;
			@include ab(absolute, block, 15px, n, n, 20px);
			i:before {
				content: "\e816";
				font-size: $body-font-size; 
			}    
		}
		&.active {
			@include opacity(1);
			visibility: visible;
			@include translate(0, 0);
		}
	}
}
.m-close {
	text-align: end;
	border: none;
	background: $main-color;
	z-index: 99999;
	cursor: pointer;
	width: 45px;
	height: 48px;
	line-height: 48px;
	@include opacity(0);
	@include border-radius-circle;
	@include transition(all 0.35s ease 0s);
	@include ab(absolute, block, 0, n, n, 0);
	i:before {
		@include close-btn;
		color: $body-link-color;
		line-height: 48px;
	}
}
.dropdown-menu >li.dropdown-submenu {
	>.dropdown-menu {
		@include ab(n, n, 0, 100%, n, n); 
		font-size: $body-font-size;
	}
	&:hover {
		@media (min-width: 992px) {
			> .dropdown-menu {      
				@include dropdown-visible;
			}
		}
	}
}
.dropdown-menu {
	@include ab(absolute, block, 100%, 0, n, n);
	float: none;
	text-align: start;
	width: 253px;
	z-index: -999;       
	padding: 0; 
	margin: 0;
	border: none;   
	@include dropdown-hidden;
	background: $nav-dropdown-background; 
	@include border-radius-reset;
	@media (max-width: 1199px) and (min-width: 992px) {
		width: 235px;
	}
	>li >a {
		padding: 3px 20px;
		line-height: 1.42857143;
		&:hover,
		&:focus {
			background-color: transparent;
			color: $nav-dropdown-link-color;
		}
	}
	>.active >a {
		background-color: transparent;
		color: $nav-dropdown-link-color;
		&:hover,
		&:focus {
			background-color: transparent;
			color: $nav-dropdown-link-color;
		}
	}
} 
@media (max-width: 991px) {
	.header-main {
		.m-cart-icon {
			@include ab(absolute, block, 50%, n, n, 0);
			@include translate(0, -50%);
			min-width: 40px;
			text-align: center;
			&.waiting {
				background: transparent !important;
			}
			.mobile-basket {
				position: relative;
				color: $header-cart-icon-color;
				>.demo-icon {
					font-size: 25px;
					color: $header-cart-icon-color;
				}
				>img {
					max-width: 20px;
				}
				.number {
					position: relative;
					font-weight: $font-bold;
				}
				.n-item {
					@include icons-number;
					background: $header-cart-bg-icon-color;
					color: $header-number-on-icon-color;
				}
			}
		}
	}
	.menu-opened {
		.navbar-responsive-menu span {
			&:nth-of-type(2) {
				@include opacity(0);
			}
			&:nth-of-type(1) {
				@include transform(translate3d(0, 9px, 0px)rotate(45deg));
			}
			&:nth-of-type(3) {
				@include transform(translate3d(0, -5px, 0) rotate(-45deg));
			}
		}
	}
	.navbar-responsive-menu {
		border: none;
		margin: 0;
		padding: 0 !important;
		cursor: pointer;
		z-index: 9;
		@include ab(absolute, block, 50%, 0, n, n);
		@include translate(0, -50%);
		.responsive-menu {
			width: 24px;
			height: 18px;
			position: relative;
			display: block;
			cursor: pointer;
			span {
				width: 100%;
				height: 2px;
				display: block;
				position: relative;
				background: $text-header-color;
				@include transition(all 0.35s ease-in-out);
				&:nth-of-type(2) {
					top: 5px;
				}
				&:nth-of-type(3) {
					top: 10px;
				} 
			}
		}
	}
	.product-image-inner>.col-md-10 {
		width: 80%;
	}
	.sb-wrapper {
		margin: 0;
		background: none;  
		border: none;
		height: 100%;
		overflow: auto;
		.sb-widget {
			margin: 0 auto; 
		} 
	}
	.header-menu-wrapper {
		display: none;
	}
	table {
		thead th {
			padding: 10px 15px;
			&.price {
				padding: 10px 15px;  
			}
		}
		td {
			padding: 15px;
		}
	}
	.cart-table table th {
		padding: 10px 15px;
	}
	.left-column-container #related_item {
		margin-top: 40px;
	}
}
@media (max-width: 479px) {
	.header-main .m-cart-icon .mobile-basket {
		>.demo-icon {
			margin: 0;
		}
		.number .money {
			display: none;
		}
		.n-item {
			@include ab(n, n, n, -15px, n, n); 
		}
	}
}
/*=== Fix skin Furniture ===*/
.skin-furniture {
	.vertical-menu .sidemenu-holder .navbar-collapse>ul>li:hover {
		background: #ffffff; 
	}
}
/*=== Fix skin Cosmetics ===*/
.skin-cosmetics {
	.sidemenu-holder .dropdown-menu>li {
		.dropdown-inner >a, >a {
			text-transform: capitalize;
			font-weight: 400;
		}
	}
