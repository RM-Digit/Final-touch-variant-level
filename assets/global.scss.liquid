@mixin effect-image {
	@include ab(absolute, block, n, 0, 0, n); 
	content: '';
	width: 100%;
	height: 100%;
	z-index: 1;
	background: -webkit-linear-gradient(to bottom, rgba(255, 255, 255, 0) 60%, rgba(255, 255, 255, 0.8) 100%);
	background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 60%, rgba(255, 255, 255, 0.8) 100%);
	@include opacity(0);
	@include translate3d(0, 50%, 0);
	@include transition(opacity 0.35s, transform 0.35s);
}
@mixin effect-image-hover {
	@include opacity(1);
	@include translate3d(0, 0, 0);
}
