	/*ul dropdown translate - currency*/
	.currency-position ul.currencies,  
	.translate-position .weglot-container .country-selector ul {  
		@include ab(absolute, block, 100%, -15px, n, n);
		background: $body-background-color;
		border: 1px solid $body-border-color;  
		padding: 5px 0;
		max-height: 200px;     
		width: fit-content; 
		overflow-y: auto;
		text-align: start;
		@include dropdown-hidden;
		@include translate(0, 10px);
		@include transition(all 0.25s ease-in-out); 
		min-width: 100px;
		li {
			line-height: normal;
			margin: 0;
			padding: 5px 0;
			a {
				font-size: $top-font-size - 1px;
				font-weight: $font-bold;
				text-transform: uppercase;
				height: auto;
				line-height: normal;
				color: $body-link-color;
				padding: 0 15px;
				display: block;
				white-space: nowrap;
				&:hover {
					text-decoration: none;
					color: $main-color;
				}
			}
		}
	}
	/* translate-position */
	.translate-position {
		&:before {
			content: none !important;
		}
		.weglot-container .country-selector {
			position: static;
			background: transparent;
			.flag--1 a:before {
				content: none;
			}
			&.wg-list {
				.wgcurrent {
					&:after {
						content: none !important;
					}
					a {
						padding: 0;
						&:before {
							@include margin-end(0);
						}
					}
				}
				ul li {
					display: block;
				}
			}
			.wgcurrent {
				border: none;
				position: relative;
				color: $topbar-text-color;
				cursor: pointer;
				&:focus {
					outline: none;
				}
				&:after {
					content: "\e812" !important;
					font-family: $font-family-icon;
					font-size: 22px;
					background-image: none;
					height: auto;
					width: auto;
					color: rgba($topbar-text-color, 0.5);
					text-align: end;
					line-height: 28px;
					@include ab(absolute, block, 50%, n, n, -9px);  
					@include transform(translate(0, -50%));
				}
				a {
					line-height: 28px;
					height: auto;
					@include padding-start(0);
					@include padding-end(15px);
					text-transform: uppercase;
					font-weight: $font-bold;
					font-size: $top-font-size - 1px;
					color: inherit;
				}
			}
			ul {
				@media (max-width: 1024px) {
					@include dropdown-visible;
					z-index: 1001;
					@include translate(0, 0); 
				}
				li .c-code {
					display: none;
				} 
			}
			&.closed {
				@media (max-width: 1024px) {
					ul {
						@include dropdown-hidden; 
						@include translate(0, 10px);    
					} 
				}
				.wgcurrent:after {
					@include transform(translate(0, -50%) rotate(0));
				}
			}
			&:hover {
				@media(min-width: 1025px) {
					.wgcurrent >a {
						text-decoration: underline;
					}
					ul {
						@include dropdown-visible;
						@include translate(0, 0);
						z-index: 1001;
					}
				}
			}
		}
	}
	/* currency-position */
	.currency-position {
		.currency_wrapper {
			position: relative;
		}
		a.currency_wrapper {
			font-weight: $font-bold;
			font-size: $top-font-size - 1px;
			text-transform: uppercase;
			line-height: 28px;
			position: relative;
			@include padding-end(15px);
			display: block;
			text-align: start;
			color: $topbar-text-color;
		}
		&:hover .currency_wrapper {
			text-decoration: underline;
		}
		.show-code {
			.currency_code .c-name {
				display: none;
			}
			ul.currencies li .c-name {
				display: none;
			}
		}
		.show-name {
			.currency_code .c-code {
				display: none;
			}
			ul.currencies li .c-code {
				display: none;
			}
		}
		.show-both .c-code {
			@include padding-end(3px);
		}
		.currency-flag {
			@include margin-end(6px);
		}
		.expand {
			@include ab(absolute, block, 50%, n, n, -9px);
			line-height: 28px;
			text-align: end;
			@include transform(translate(0, -50%)); 
			i {
				font-size: $font-small-icon;
				color: rgba($topbar-text-color, 0.5);
			}
		}
		ul.currencies.opened { 
			@media (max-width: 1024px) {
				z-index: 1001;
				@include translate(0, 0);
				@include dropdown-visible;
			}
		}  
		&.flag-none .currency-flag {
			display: none;
		}
		&:hover {
			@media(min-width: 1025px) {
				ul.currencies {
					@include translate(0, 0);
					@include dropdown-visible; 
					z-index: 1001;
				}
			}
		}
	}
