        }     
    } 
}
.btn-round {
    .product-wrapper .product-head {
        .product-button>div {
            @include border-radius-circle;
        } 
        .product-add-cart {
            @include border-radius-50; 
        }   
    }  
}     
.product-wrapper {
    position: relative;  
    margin: 36px 0 0; 
    .product-head {
        overflow: hidden;
        position: relative;
        .product-image {
            position: relative; 
            margin: 0 auto;
            padding: 0;  
            .product-ratio-false img:not(.img-lazy) {
                position: static; 
            }   
            img {
                @include ab(absolute, block, 0, 0, n, n);
                max-width: 100%;
                max-height: 100%;
                width: 100%;
                height: 100%; 
                margin: 0 auto;
                @include transition(all 0.2s ease-in-out);
                &.auto-crop-true {
                    object-fit: cover;
                    font-family: "object-fit: cover";
                } 
                &.auto-crop-false {
                    object-fit: contain;
                    font-family: "object-fit: contain";
                }   
            } 
            span.img-back {  
                width: 100%; 
                height: 100%;    
                @include ab(absolute, block, 50%, 50%, n, n);
                @include translate(-50%, -50%);
                img {
                    @include opacity(0);                                                   
                }       
            }  
        }  
        .featured-img { 
            overflow: hidden;
            display: block;
            position: relative;        
            text-align: center; 
            width: 100%;
            margin: 0 auto;
            a {  
                display: block;     
                >img {
                    &.lazyloading {
                        filter: blur(5px); 
                        -webkit-filter: blur(5px); /* Prefixed CSS3 blur filters */
                        -moz-filter: blur(5px); 
                        -o-filter: blur(5px); 
                        -ms-filter: blur(5px);
                        @include opacity(0.2);              
                    }
                    &.lazyloaded {              
                        filter: blur(5px);
                        -webkit-filter: blur(0px); /* Prefixed CSS3 blur filters */
                        -moz-filter: blur(0px); 
                        -o-filter: blur(0px); 
                        -ms-filter: blur(0px);
                        @include opacity(1);
                    }
                }  
            }  
            &.switch-image a {
                &:before {
                    content: none;
                }    
                img {     
                    @include preserve-3d; 
                    @include transition(all 0.35s ease-in-out);
                    &.featured-image {
                        @include opacity(1);
                    } 
                }  
            }   
        } 
        &:hover {
            @media (min-width: 1025px) {
                .switch-image a {                          
                    > img, >span.image-lazysize {
                        @include opacity(0);
                    }         
                    > span.img-back img {
                        @include opacity(1);                                                  
                    }                  
                }  
            } 
        }        
        .product-des-list {
            font-size: $body-font-size - 1px;   
            color: $body-general-color;           
            line-height: 18px;
            margin: 23px 0 0; 
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            display: none;
        }  
        .product-button {
            @include ab(absolute, block, n, 0, 47px, 0);    
            @include opacity(0);
            visibility: hidden;
            @include flexbox;
            flex-wrap: wrap;
            justify-content: center; 
            @include transition(opacity 0.2s ease-in-out); 
            @media (max-width: 1024px) and (min-width: 768px) {
                bottom: 5px;
                @include opacity(1);
                visibility: visible;
            }
            @media (max-width: 767px) {
                display: none; 
            }
            >div {
                @include icon-product;
                @media (max-width: 1200px) and (min-width: 1025px) {  
                    width: 33px;
                    height: 33px;
                    line-height: 30px;
                }      
            }
        } 
        .product-add-cart {
            @include ab(absolute, block, n, 0, 0, 0);
            @include product-add-cart;
            z-index: 4;  
            @include translate(0, 60px);      
            @include transition(transform 0.25s ease-in-out);
            @media (max-width: 1300px) and (min-width: 1025px) {
                font-size: $body-font-size - 2px; 
            }      
            @media (max-width: 1024px) and (min-width: 768px) {
                position: static;
                @include opacity(1);
                visibility: visible;
                @include translate(0, 0);
            }   
            @media (max-width: 767px) {
                display: none; 
            } 
            .btn-add-cart.disabled { 
                position: relative;        
                &:before {
                    content: '\f110';       
                    font-size: 20px;
                    font-family: $font-family-icon;
                    @include ab(absolute, block, 6px, 0, n, 0);     
                    -webkit-animation: fa-spin 2s infinite linear;
                    animation: fa-spin 2s infinite linear;
                }                     
                .text {    
                    color: transparent;
                }           
            }                
        } 
        .detail-deal {
            @include ab(absolute, block, n, 10px, 10px, 10px);       
            z-index: 1;          
            @media (max-width: 1024px) and (min-width: 768px) {
                bottom: 46px; 
            }   
            @media (max-width: 575px) {
                display: none; 
            }       
        }
    }
    &.effect-overlay {
        .featured-img a:before {   
            content: "";      
            @include ab(absolute, block, n, 0, 0, 0);
            background: rgba($white, 0.3); 
            width: 100%;
            height: 0;
            z-index: 1;
            @include transition(height 0.35s ease-in-out);  
        }    
        &:hover {
            @media (min-width: 1025px) {
                .featured-img a:before {
                    height: 100%;
                }               
            }
        }    
    }
    &.border-btn {
        .product-button>div {
            border: 1px solid $body-border-color; 
        }
        .detail-deal .is-countdown  {
            border: 1px solid $body-border-color;   
        }
    } 
    .plm-description {
        display: none;
    }  
    .inventory-quantity .line {
        width: 100%;
        height: 10px;   
        background: #e6e6e6; 
        .line2 {
            height: 100%;
            background: $main-color;  
        } 
    }  
    .product-name {            
        color: $body-link-color;     
        font-size: $product-font-size;      
        padding-top: 9px;     
        line-height: 1.6;    
        position: relative; 
        font-weight: 400;  
        @media (max-width: 479px) {
            font-size: $product-font-size - 2px;      
        }
        &.balance-true a {
            display: block;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden; 
        }   
        a {
            color: inherit;
            font-size: inherit;      
        }
        &:hover a {
            color: $body-link-color-hover; 
        } 
    } 
    .product-vendor {  
        display: none;   
        a {
            font-size: $body-font-size - 2px;
            color: $body-link-color;
            text-transform: uppercase;
            &:hover {
                color: $body-link-color-hover;
            }
        }
    }
    .product-price + .product-review {
        padding-top: 12px;    
    }  
    .product-review {        
        margin: 0;
        overflow: hidden;    
        .spr-badge-starrating {
            float: none;
            margin: 0;
            font-size: $body-font-size - 1px;  
            @include padding-end(7px);
        }  
        .spr-badge-caption {
            display: none;  
            font-weight: $font-bold;
            text-transform: uppercase;
            font-size: $body-font-size - 3px;   
        }   
    }  
    .product-list-mode-content {
        display: none; 
    }
    .list-v2-label {
        display: none; 
    }
    .product-list-mode-content-v2 {
        display: none; 
    }       
    .product-content {                    
        .pc-inner {
            display: block;
            width: 100%;    
            @media (max-width: 1024px) {
                position: relative;
            }
        }
        .product-description {               
            margin: 15px 0 0;      
            display: none;   
            font-size: $body-font-size - 1px;
            color: $body-general-color;
            br {
                content: 'A';
                display: block;
                margin: 0 0 15px; 
            }     
            ul li {
                list-style: circle outside none;
            }
            ol li {
                list-style: decimal outside none;
            }
            ul, ol {        
                @include padding-start(18px);
                li {
                    margin: 0 0 5px;          
                }
            }
        }          
        .product-descrip {
            display: none;
        }
        .product-price {  
            padding: 4px 0 0;    
            p {
                display: none;
            }       
        }
    } 
    &:hover {
        @media (min-width: 1025px) {
            .product-button {
                @include opacity(1);
                visibility: visible;
                z-index: 4;  
            }    
            .product-add-cart {
                @include translate(0, 0);
            }
            .detail-deal {
                display: none;    
            }     
        }         
    }  
}
