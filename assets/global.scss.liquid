}
.footer-check-content.push-right { 
	padding-bottom: 43px;
	@media (max-width: 767px) {
		padding-bottom: 15px;
	}
}
.heading-blank {
	.payment-icons ul {
		text-align: end;  
		padding-top: 0;
		@media (max-width: 767px) {
			text-align: start;
		}
		li img {
			@include margin-end(0);
		}
