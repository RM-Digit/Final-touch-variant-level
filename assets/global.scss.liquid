			border: 1px solid rgba($body-border-color, 0.5);  
			z-index: 9;
			padding: 15px !important; 
			margin: 0;
			.form-control {
				color: $header-search-text;
				@include placeholder {
					color: $header-search-text;
				}
			}
			&.s-loading .navbar-form:before {
				color: $header-search-icon;
			}
			button {
				background: transparent;
				border: none;
				color: $header-search-icon;
				@include ab(absolute, block, 50%, n, n, 10px);
				@include translate(0%, -50%);
				line-height: 40px;
				padding: 0;
				&:hover {
					color: $main-color;
				}
			}
			.result-ajax-search {
				width: calc(100% - 30px);
				@include ab(n, n, n, n, n, auto);
			}
		}
		>a {
			display: block;
			width: 100%;
			text-align: center;
			position: relative;
			z-index: 991;
		}
		.search-categories {
			background: $main-color;
			color: $white;
			padding: 0px 15px;
			line-height: 38px;
			height: 38px;
			font-size: $body-font-size - 1px;
			font-weight: $font-bold;
			@media (max-width: 1199px) {
				display: none;
			}
		}
		.dropdown-menu {
			@include ab(n, block, n, auto, n, 0);
			width: 300px;
			visibility: hidden;
			z-index: -99;
			@include translate(0, 10px);
			@include opacity(0);
			@include transition(all 0.35s ease);
			@media (max-width: 400px) {
				width: 280px;
				@include ab(n, n, n, n, n, -60px);
			}
		}
		&:hover {
			@media (min-width: 1025px) {
				.dropdown-menu {
					visibility: visible;
					z-index: 99;
					@include opacity(1);
					@include translate(0, 0);
					&:before {
						content: '';
						height: 30px;
						width: 100%;
						@include ab(absolute, block, -20px, 0, n, n);
					}
				}
			}
		}
		&.show .dropdown-menu {
			visibility: visible;
			z-index: 99;
			@include opacity(1);
			@include translate(0, 0);
		}
		.search-icon {
			@include ab(absolute, block, 50%, n, n, 10px);
			z-index: 3;
			text-align: center;
			padding: 0;
			margin: 0;
			border: none;
			color: $header-search-icon;
			background: transparent;
			@include translate(0, -50%);
			@include transition(all 0.3s ease-in-out 0s);
