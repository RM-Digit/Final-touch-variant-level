.horizontal-menu {
	background: none;
	position: static;  
	.menu-list {
		width: 100%;
		position: static;
		margin: 0 auto;
		border: none;
		@include border-radius-reset;
		>li {
			float: left;
			padding: 2px 0;
			@include padding-start(34px);
			margin: 0;
			@include transition(all 0.35s ease 0s);
			&:first-child {
				@include padding-start(0);
			} 
			>.dropdown-inner >a,
			>a {
				font-size: $nav-font-size;
				margin: 0;
				display: inline-block;
				text-decoration: none;
				line-height: 25px;
				padding: 9px 0;
				color: $nav-link-color;
				position: relative;
				&:after {
					content: "";
					@include ab(absolute, block, n, 0, 6px, n);
					width: 0;
					background: $nav-link-hover-color;
					height: 2px;
					@include transition(width 0.3s ease-in-out);
				}
				&:hover {
					color: $nav-link-hover-color;
				}
			}
			>.dropdown-inner >span.expand,
			>span.expand {
				color: $nav-link-color;
			}
			/*have submenu*/
			&.dropdown {
				position: relative;    
				.dropdown-menu li {
					position: relative;  
					.dropdown-link {
						@include margin-end(30px);  
					}
					span.expand {
						height: 32px;
						width: 30px;
						text-align: center;
						line-height: 32px;
						@include ab(absolute, block, 8px, n, n, 17px);
						font-size: $nav-sub-font-size;
