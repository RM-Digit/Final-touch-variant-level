    @media (max-width: 767px) {
        padding-bottom: 35px; 
    } 
    .page-sub-collections-content >a {
        margin-top: 20px; 
        display: inline-block; 
    } 
    .block-subcollections {
        margin: 0 0 72px;  
        border-bottom: 1px solid $body-border-color;
        @media (max-width: 991px) {
            margin: 0 0 55px; 
        }
        @media (max-width: 767px) {
            margin: 0 0 35px;
        }   
        >div {
            align-items: center; 
        }      
        .text-box {
            h3 {
                font-size: $size-h3 - 4px;
                font-weight: $font-bold;  
