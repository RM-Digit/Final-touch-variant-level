/*==== Vertical menu ====*/
#page-body.wide-padding .header-container.layout-full {
	.vertical-menu .sidemenu-holder {
		.navbar-collapse >ul >li.dropdown >.dropdown-menu { 
			&.width-100 {
				width: calc(100vw - 30px - 17px - 260px);     
				@media (min-width: 1200px) {
					width: calc(100vw - 140px - 17px - 260px);
				}    
			}
			&.width-75 {
				width: calc((100vw - 30px - 17px - 260px) * 3 / 4);   
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) * 3 / 4);
				}
			}
			&.width-50 {
				width: calc((100vw - 30px - 17px - 260px) / 2);
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) / 2); 
				}
			}
			&.width-25 {
				width: calc((100vw - 30px - 17px - 260px) / 4); 
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) / 4); 
				}
			}
		}
	}
}
.vertical-menu {
	width: calc(25% - 15px);
	@include margin-end(25px);
	&.bg-color-none {
		width: auto;
		@include margin-end(55px);
		@media (max-width: 1440px) {
			@include margin-end(30px);
		}
		.sidemenu-holder .head {
			@include padding-start(0);
			@include padding-end(0);
		}
	}
	.sidemenu-holder {
		@include vertical-menu;  
	} 
}
