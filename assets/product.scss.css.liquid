            @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.06s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.16s);
        }
        .animated-16 {      
            @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.12s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.22s);
        }
        .animated-17 {      
            @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.18s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.28s);
        }
        .animated-18 {      
            @include transition(transform 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.24s, opacity 1s cubic-bezier(0.165, 0.84, 0.44, 1) 1.34s);
        }    
    } 
    .action .btn {
        width: 100%;
        padding: 10px;
        margin: 0 auto 10px;
        &:last-child {
            margin-bottom: 0;
        }
    }  
    .wrap-action {
        @include ab(absolute, block, n, 20px, 20px, 20px);            
        background: $body-background-color; 
    }
    &.opened {
        @include translate(0, 0);    
        margin-top: 0 !important;
        @media (max-width: 319px) {
            max-width: 250px;
        }
        .c-close {
            @include opacity(1);
        }
        #cart-info .items-inner {
            @include opacity(1);
            @include translate(0, 0);
        }
        .cart-content{
            @include opacity(1);
            visibility: visible;
        }   
    }
}
.navmenu-shopping-cart {
    .btn {min-width: 180px;}
}
/*Popup Cart*/
#layer-addcart-modal  {
    z-index: 99999;
    .addcart-overlay {
        background: rgba(0, 0, 0, 0.5);   
        height: 100%;
        @include ab(fixed, block, 0, 0, n, n); 
        width: 100%;
        z-index: 99992;    
    }
    .addcart-modal-close {
        @include ab(absolute, block, -25px, n, n, 0); 
        cursor: pointer;
        @include opacity(0.65); 
        color: $white;  
        @media (max-width: 1024px) {
            @include opacity(1);
        } 
        @media (max-width: 767px) {
            color: $body-general-color; 
            @include ab(n, n, 5px, n, n, 5px); 
        }
        i:before {
            content: "\e816";
            font-family: $font-family-icon; 
        }
        &:hover {
            @include opacity(1);
        } 
    } 
    .addcart-modal-box {
        @include ab(absolute, block, 15%, n, n, n);
        margin: 0 calc((100% - 680px) / 2);
        width: 625px; 
        height: auto;
        max-height: 90%;    
        z-index: 99999;
        background: $body-background-color;    
        @media (max-width: 767px) { 
            top: 49%;
            width: 90%;
            margin: 0 5%;
        }
        .addcart-modal-mobile {
            display: none;
            @media (max-width: 767px) {
                display: block;
                padding: 20px;
                text-align: center;
                font-size: 14px;
                font-weight: $font-bold;
                text-transform: uppercase;
            }
            .note {
                margin: 0;
                padding: 0 0 5px;
                i {
                    color: $main-color;
                }
            } 
            a {
                display: block;
                margin: 5px auto;
                text-decoration: underline;
            }  
        }
        .addcart-modal-desktop {
            display: table;
            width: 100%;
            vertical-align: middle;
            @media (max-width: 767px) {
                display: none;
            }
            .addcart-modal-product {
                padding: 30px;
                width: 275px; 
                display: table-cell;
                vertical-align: middle;
                .note {
                    font-size: $size-h4;
                    text-transform: uppercase;
                    font-weight: $font-bold;  
                    line-height: 1; 
                    margin: 0; 
                    @include padding-start(55px);
                    position: relative;
                    i {
                        font-size: $font-icon + 2px;
                        color: $main-color;
                        position: absolute;
                        @include margin-start(-40px);
                    }  
                }
                .addcart-modal-image {
                    text-align: center;
                    padding-top: 37px;
                }
                .addcart-modal-info {
                    text-align: center;
                }
                .addcart-modal-title {
                    padding-top: 15px;
                    font-size: $product-font-size;
                    color: $body-link-color;
                    &:hover {
                        color: $body-link-color-hover;
                    }
                }
                .addcart-modal-variant {
                    padding-top: 7px; 
                }  
                .addcart-modal-qty {
                    padding: 7px 0; 
                    strong {
                        font-weight: 400;
                    }
                }    
            }  
            .addcart-modal-action {
                background: #f2f2f2; 
                padding: 40px;
                width: 350px;
                display: table-cell;
                vertical-align: middle;
                .addcart-modal-number {
                    margin: 0 0 6px; 
                }
                .addcart-modal-subtotal {
                    margin-bottom: 16px; 
                    strong {
                        font-weight: 400;
                    }
                    .money {
                        font-size: 35px;
                        color: $main-color;
                        font-weight: $font-bold;
                    }
                }
                .addcart-modal-button {
                    >div {
                        margin-top: 10px;
                    } 
                    .btn {
                        width: 100%;
                        display: block; 
                    } 
                }
            }
        }
    }
}

/* Quick view ----------------------------------*/
.modal-content {
    background: $body-background-color;
}
#quick-shop-modal {
    background: rgba(0,0,0,0.5);
    z-index: 999;  
    .modal-content {
        z-index: 999;     
    }
}
.modal-open .modal-backdrop { 
    display: none;
}
.btn-round .quantity {
    @include border-radius-50;
    input {
        @include border-radius-reset; 
    }  
}
.quantity {
    position: relative;
    display: inline-block;
    height: 48px;                                 
    padding: 0 30px;  
    background: #e6e6e6;            
    border: none;
    input {  
        width: 100%;   
        padding: 5px 0;
        height: 47px; 
        line-height: 48px; 
        border: none; 
        text-align: center; 
        color: $body-general-color;                     
        background-repeat: no-repeat;
        background-attachment: fixed;
        background: transparent;
        -moz-appearance:textfield;  
        -webkit-appearance:textfield;     
        -webkit-appearance: none;  
        @include border-end(1px solid #cfcfcf);
        @include border-start(1px solid #cfcfcf);
        @include placeholder {
            color: $body-general-color; 
        }  
        &:focus, 
        &:active,
        &:hover {
            background: transparent;  
            @include border-end(1px solid #cfcfcf);
            @include border-start(1px solid #cfcfcf);
        }
    }  
    .qty-inner > span {       
        width: 30px;  
        height: 48px;
        line-height: 48px;
        text-align: center;
        padding: 0;            
        cursor: pointer;
        display: inline-block;      
        z-index: 2;    
        i {
            width: 100%;            
            display: block;  
            font-style: normal;
            font-size: 8px;      
            @include transition(all 0.35s ease 0s);
            &:before {
                font-family: $font-family-icon;
            } 
        }
		&.qty-up {  
            @include ab(absolute, block, n, n, 0, 0);          
        }
        &.qty-down { 
            @include ab(absolute, block, n, 0, 0, n);  
        }
        &:hover i {
            color: $body-link-color-hover;
        }
    }
    &.ajax-true {
        padding: 0 10px !important;
        input[type="number"] {            
            padding: 7px 0 !important;
            width: 100%;
        }   
    }
} 
.input-bg-none {
    .quantity {
        background: transparent;
        border-bottom: 1px solid #cfcfcf; 
        input {
            border: none; 
            &:focus, 
            &:active,
            &:hover {
                background: transparent;  
                @include border-end(1px solid transparent);
                @include border-start(1px solid transparent); 
            } 
        }
    }
    .skin-simple .quantity { 
        border: none;
        border-bottom: 1px solid #e1e1e1; 
    }
}   
.input-bg-white .quantity input {
    border: none; 
    @include border-end(1px solid #cfcfcf);
    @include border-start(1px solid #cfcfcf);  
    &:focus, 
    &:active,
    &:hover { 
        background: transparent;  
    } 
}

.close {
    @include opacity(1);
    filter: alpha(opacity=100);
    text-shadow: 0 0 0 $white;
    -webkit-text-shadow: 0 0 0 $white;
    font-weight: normal;
    margin: 0;
}
.modal-dialog {
    max-width: 100%;
}
#quick-shop-popup {
    background-color: rgba(0,0,0,0.5);  
    z-index: 99999;
    .modal-dialog { 
        width: 900px;   
        @include translate(0, 0);   
        @include flexbox();
        justify-content: space-between;
        align-items: center;
        align-self: center; 
        height: 100vh;
        margin: 0 auto;    
        @media (max-width: 1024px) {
            max-width: calc(100% - 30px);
        }
        .modal-content {           
            border: none;
            max-height: 90%;      
            @include border-radius-reset;  
            .modal-header {
                border: 0;
                padding: 0;
                text-align: end;
                min-height: auto;
                span { 
                    @include ab(absolute, block, -33px, n, n, -10px);
                    @include width-btn-1; 
                    cursor: pointer;          
                    text-align: center;
                    z-index: 9999;     
                    padding: 0;
                    margin: 0; 
                    @include opacity(0.65);    
                    @include transition(all 0.35s ease 0s);             
                    &:before {
                        @include close-btn;   
                        color: $white;                             
                    }   
                    &:hover {
                        @include opacity(1);   
                    }  
                }        
            }
            .modal-body {
                padding: 30px;              
                width: 100%;
                height: 100%;
                overflow-y: auto;  
                overflow-x: hidden;  
            }
        }
    }  
    .product-image {
        #qs-product-image {
            width: calc(100% - 70px);
            float: right;
            position: relative;
            .qs-slider-for {
                @include opacity(0);
                visibility: hidden;
                display: none;
                &.slick-initialized {
                    @include opacity(1);
                    visibility: visible; 
                    display: block; 
                }
            }
            .slick-slide {
                display: block;
                float: left; 
            }  
            .slick-arrow {
                width: 60px;
                height: 60px;
                line-height: 60px;
                background: $body-background-color;
                color: $body-general-color;
                z-index: 1000;    
                text-align: center; 
                cursor: pointer;
                @include opacity(0);
                visibility: hidden;
                @include translate(0, -50%); 
                i { 
                    font-size: $font-small-icon; 
                } 
                &.btn-prev {
                    @include ab(absolute, block, 50%, 30px, n, n);
                }  
                &.btn-next {
                    @include ab(absolute, block, 50%, n, n, 30px);
                }
                &:hover { 
                    color: $main-color;  
                } 
            } 
            &:hover .slick-arrow {
                @media (min-width: 1025px) {
                    @include opacity(1);
                    visibility: visible; 
                }  
            }  
        }   
        #qs-slider-thumbs {  
            width: 60px;
            float: left; 
            @include opacity(0);
            visibility: hidden;
            display: none;
            &.slick-initialized {
                @include opacity(1); 
                visibility: visible; 
                display: block;
            }
            .slick-list.draggable {
                position: relative;   
                overflow: hidden;       
            }    
            .slick-slide {
                padding-bottom: 10px;
                display: inline-block;  
                .slick-item { 
                    cursor: pointer;   
                    @include opacity(1);
                    visibility: visible;
                    height: auto; 
                }  
                &.slick-current .slick-item {
                    border: 2px solid $main-color;
                }    
            }   
        }  
    }
    .product-info {
        #qs-product-author {
            font-size: $body-font-size - 3px;     
            margin: 0 0 14px;   
            text-transform: uppercase;  
            a {
                color: $body-link-color;
                &:hover {
                    color: $body-link-color-hover; 
                } 
            }
        }
        .product-label {
            @include ab(absolute, block, 4px, auto, n, 0); 
            > span {
                margin: 0;
                @include margin-start(10px);
                display: inline-block; 
            }    
        }
        #qs-product-title {
            text-align: start; 
            font-weight: $font-bold;
            font-size: $product-detail-title-size;           
            margin: 0 0 10px;    
            padding: 0;
            line-height: 1.1;  
            color: $body-link-color;
            text-transform: uppercase;
        } 
        #qs-rating {
            margin: 0 0 19px;
            line-height: 20px;
            overflow: hidden;
            clear: both;
            .spr-icon {
                top: 0;
                &:before {
                    font-size: $body-font-size - 1px; 
                }
            }    
            .spr-starrating {
                float:left;
                @include margin-end(5px);
            }
            .spr-badge-caption {
                float: left;
                color: $body-general-color;
                display: inline-block;      
                font-size: $body-font-size - 1px;
                &:before {
                    content: '(';
                }
                &:after {
                    content: ')';
                }
            } 
        } 
        .product-sku-stock {
            margin: 0 0 14px;
            list-style: disc outside none;
            li {      
                font-size: $body-font-size - 3px;
                text-align: start;
                position: relative;      
                color: $body-general-color;
                line-height: 1.5;  
                text-transform: uppercase; 
                > span {
                    color: $body-general-color;        
                    display: inline-block; 
                    min-width: 90px; 
                    &#sku, &.stock  {
                        padding: 0;
                        font-weight: $font-bold; 
                        color: $body-general-color;
                    }   
                } 
                > a {
                    color: $body-general-color;
                    &:hover {
                        color: $main-color;
                    }
                } 
            } 
        }  
        .detail-price {
            text-align: start;
            padding: 0;
            margin: 0 0 3px; 
            .price-sale {      
                font-size: $size-h1 + 10px;  
            }  
            .price {                    
                text-align: start;
                padding: 0;
                margin: 0;        
                border: none;                                             
                font-size: $size-h1 + 10px;   
            }  
            .price-compare {                          
                font-size: $size-h1 - 10px;   
                margin: 0;  
                @include margin-end(20px);     
            } 
        } 
        #qs-action-wrapper { 
            position: relative;
            display: block;
            width: 100%;
            margin: 30px 0 24px;
            clear: both;
            .selector-wrapper {
                margin-bottom: 26px;
                clear: both;
                text-align: start;
                label {    
                    padding: 0 0 4px; 
                    display: block;     
                    margin: 0;                                               
                    color: $main-color;  
                    text-transform: uppercase;   
                    font-size: $body-font-size - 2px;
                    font-weight: $font-bold;    
                }   
                select { 
                    min-width: 50%;
                    height: 34px;
                    padding: 0 10px;
                    line-height: 32px;            
                    font-size: $body-font-size;        
                    color: $body-general-color;                                                   
                } 
            }
            .qty-add-cart {
                padding: 16px 0 22px;
                overflow: hidden;
                .quantity-product {
                    margin: 0;
                    @include margin-end(10px);
                    width: 145px;
                    float: left;
                }
                .qs-product-button {   
                    list-style: none;
                    float: left;
                    width: calc(100% - 155px);
                }
                .btn {
                    margin: 0;      
                    padding: 10px 20px;  
                    width: 100%;        
                    font-size: $body-font-size;             
                    .demo-icon { 
                        margin: 0;
                        @include margin-end(7px);
                        font-size: 18px;
                        display: none; 
                    }   
                    &.sold-out {          
                        @include margin-end(15px);          
                    }
                    &.disabled {  
                        position: relative;         
                        color: transparent !important;
                        @include opacity(1);
                        .demo-icon {
                            @include opacity(0);
                        }
                        &:after {
                            content: '\f110';   
                            font-family: $font-family-icon;                 
                            font-size: $font-icon;
                            color: $white;
                            @include margin-start(-13px);
                            -webkit-animation: fa-spin 2s infinite linear;
                            animation: fa-spin 2s infinite linear;                      
                            @include ab(absolute, block, 10px, 50%, n, n);                                
                        }
                    }  
                    &.disabled[disabled="disabled"] {
                        color: $button-btn1-txt !important;       
                        @include opacity(0.65);   
                        &:after {
                            content: none;
                        }
                    }
                    &:hover {
                        color: $button-hover-btn1-txt;
                    } 
                }   
            }
        }
        #qs-wl-cp a {
            display: block;
            text-transform: uppercase;
            color: $body-link-color;
            line-height: 20px;
            font-size: $body-font-size - 3px; 
            text-decoration: underline; 
            font-weight: $font-bold;  
            padding-bottom: 8px;  
            clear: both;    
            i, svg {
                float: left;    
                font-size: $body-font-size;    
                @include margin-end(5px);              
            } 
            &:hover { 
                color: $body-link-color-hover;     
            }   
        }   
    }
}    
/*=== Fix skin fashion-modern ===*/
.skin-modern {
    .title-wrapper, .block-heading {
        @include transform-text;
    }
    .quantity {
        font-weight: 600; 
        input {
            font-weight: 600;  
        }
    }
    .left-column-container,
    .sidebar {
        .sb-widget {
            h5:not(.product-name) {
                font-size: $size-h1; 
            }
            .blog-menu,
            .categories-menu {
                li a {
                    text-transform: capitalize;
                }
            }   
        }
    }
}

/*=== Fix skin fashion-simple ===*/
.skin-simple {
    .title-wrapper, .block-heading {
        @include transform-text;
    }
    .left-column-container, .sidebar {
        .sb-widget {
            h5:not(.product-name) {
                font-size: $size-h1; 
            }
            .blog-menu li a,
            .categories-menu li a {
                font-size: $body-font-size + 3px;  
            }   
        }
    }
    .quantity {
        background: #ffffff;
        border: 1px solid #e1e1e1;
    } 
}
/*=== Fix skin clean ===*/
.skin-clean {
    .title-wrapper, .block-heading {
        @include transform-text;
    }
    .quantity { 
        background: #ffffff;
        border: 1px solid #d9d9d9;
        input {
            font-size: $body-font-size + 5px;
            font-weight: 500;
        }
    }
    .product-price, .price, .price-sale {
        font-weight: 500;    
    }
    .left-column-container, .sidebar {
        .sb-widget {
            h5:not(.product-name) {
                font-size: $size-h1 - 5px; 
            }
            .blog-menu li ,
            .categories-menu li  {
                padding: 8px 0;
                a {
                    font-size: $body-font-size + 3px; 
                    @include transform-text; 
                }  
            } 
        }
    }
    .btn {
        text-transform: none; 
    }
}
/*=== Fix skin Furniture ===*/
.skin-furniture {
    .left-column-container, .sidebar {
        .sb-widget {
            h5:not(.product-name) {
                font-size: $size-h1 - 5px;  
            }
            .blog-menu li ,
            .categories-menu li {
                padding: 6px 0; 
                a {
                    font-size: $body-font-size - 1px; 
                    @include transform-text;  
                } 
            }  
        }
    }
}
/*=== Fix skin Market-light ===*/
.skin-market-light {
    .title-wrapper, .block-heading {
        @include transform-text;
    }
    .left-column-container, .sidebar {
        .sb-widget { 
            h5:not(.product-name) {
                font-size: $size-h1 - 5px;  
            } 
            .blog-menu li a,
            .categories-menu li a {  
                @include transform-text;  
                font-size: $body-font-size - 1px;
                font-weight: 400; 
            } 
        }
    }
}
/*=== Fix skin Market-gray ===*/
.skin-market-gray {
    .title-wrapper, .block-heading{
        @include transform-text;  
    }
    .left-column-container, .sidebar {
        .sb-widget { 
            .blog-menu li a,
            .categories-menu li a {  
                @include transform-text;  
                font-size: $body-font-size;
            } 
        }
    }
    .header-content.input-bg-white .searchbox .form-control { // fix market home 2 & market-gray home 3 
        border: none;
    }
    .footer-container .newsletter-layout-1, 
    .footer-container .newsletter-layout-5  {  // fix market home 2 & market-gray home 3
        form  {   
            input {
                border: none;
            } 
            .newsletter-checkbox label:before {
                border: none; 
            }
        }
    } 
}
/*=== Fix skin Tools-orange ===*/
.skin-tools { 
    .title-wrapper, .block-heading {
        @include transform-text;
    }
}
