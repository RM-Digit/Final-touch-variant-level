/* Category page --------------------------*/
.srp-filter-wrapper {  
    display: table;
    width: 100%;
    margin-bottom: 15px;
    > a {
        line-height: 18px;
        display: inline-block;
        @include margin-end(15px);
        @include padding-end(15px);
        font-size: $body-font-size;
        @include border-start(1px solid $body-border-color);
        &:before {
            content: "\e8cc";
            font-family: $font-family-icon;
            font-size: 12px;
            @include margin-end(7px);         
        }
        &:hover {
            &:before {
                content: '\e8cc'; 
            }
        }
    }
    .srp-filter {
        display: inline-block;
        li {
            @include margin-end(15px);
            span {
                display: none;        
            }
            a {
                line-height: 18px;
                font-size: $body-font-size;
                &:before {
                    content: "\e8cc";
                    font-family: $font-family-icon;
                    font-size: 12px;
                    @include margin-end(7px);
                }
                &:hover {
                    &:before {
                        content: '\e8cc';       
                    }
                }
            }
        }
    }
}  
.left-column-container, .sidebar {
    .sb-filter-wrapper {    
        .sbw-filter {   
            overflow: hidden; 
            .sbf-title { 
                .clear-filter {
                    text-transform: capitalize;
                    cursor: pointer;
                    font-weight: 400;
                }   
                span {                
                    cursor: pointer;        
                    i {
                        display: none;
                        &:before {
                            content: none;
                        }         
                    } 
                }
            }    
        } 
        .sb-filter-tag {
            overflow: auto;
            li {
                margin: 0 0 13px;
                line-height: 20px;
            }
        }
    }       
}
.sb-filter-tag li {
    span {
        position: relative;
        @include padding-start(30px);   
        display: inline-block;   
        font-size: $body-font-size - 1px;   
        font-weight: $font-bold; 
        text-transform: uppercase; 
        word-break: break-word; 
        color: $body-general-color;   
        &:before {
            content: " "; 
            border: 1px solid $bg-other; 
            background: $bg-other; 
            width: 20px;
            height: 20px;
            @include ab(absolute, block, -2px, 0, n, n);   
            @include transition(all 0.35s ease 0s);
        }   
        &:after {
            content: "\e83d";
            font-size: 10px;
            font-family: $font-family-icon; 
            width: 20px;
            height: 20px;
            text-align: center;  
            line-height: 18px; 
            @include opacity(0); 
            visibility: hidden; 
            @include ab(absolute, block, 0, 0, n, n);
            @include transition(all 0.35s ease-in-out); 
        } 
        &:hover {
            @media (min-width: 1025px) {
                color: $body-link-color-hover; 
                text-decoration: underline;  
                &:after {
                    @include opacity(1);
                    visibility: visible;  
                }                 
            }               
        }
    }
    &.active {
        > span { 
            color: $body-link-color-hover; 
            text-decoration: underline;  
            &:after {
                @include opacity(1);
                visibility: visible;  
            }      
        } 
    }
} 
.sbw-filter {  
    > ul,
    > o {
        list-style: none;
        margin: 0;
        padding: 15px; 
        li {
            margin: 8px 0;
            a:hover {
                color: #888;
            } 
        }
    }
    .price {
        color: $main-color;
    }
    .price-compare {
        color: #aaa;
    }
    .sb-title {
        padding: 14px 15px;
        margin: 0 0 25px;    
    }
    > div {    
        .row {
            margin-bottom: 15px;
            &:last-child {
                margin-bottom: 0;
            }
        }        
        .sbf-title {
            color: $main-color;
            margin: 0;      
            padding: 0;       
            font-size: $body-font-size - 2px;  
            font-weight: $font-bold;          
        }   
        .advanced-filters {
            list-style: none;
            padding: 16px 0 0; 
            max-height: 290px;
            overflow: auto; 
            list-style: none;
            &.afs-color {
                margin: 0 -2px;          
            }      
            > li {
                .inner > a {  
                    position: relative;
                    @include padding-start(30px);
                    color: $body-general-color; 
                    display: inline-block;      
                    font-size: $body-font-size - 1px; 
                    font-weight: $font-bold;  
                    text-transform: uppercase;  
                    &:hover {
                        color: $body-link-color-hover;           
                    } 
                }
                &.advanced-filter:not(.af-color) {
                    line-height: 20px;
                    margin-bottom: 11px;   
                    a {
                        &:before {
                            content: " "; 
                            border: 1px solid $bg-other; 
                            background: $bg-other; 
                            width: 20px;
                            height: 20px;
                            @include ab(absolute, block, -2px, 0, n, n);   
                            @include transition(all 0.35s ease 0s);
                        }  
                        &:after {
                            content: "\e83d";
                            font-size: 10px;
                            font-family: $font-family-icon; 
                            width: 20px;
                            height: 20px;
                            text-align: center;  
                            line-height: 18px; 
                            @include opacity(0); 
                            visibility: hidden; 
                            @include ab(absolute, block, 0, 0, n, n);
                            @include transition(all 0.35s ease-in-out); 
                        }  
                    }
                    &.active-filter .inner > a {  
                        color: $body-link-color-hover; 
                        &:after {
                            @include opacity(1);
                            visibility: visible;  
                        }         
                    }   
                }
                &.af-color {               
                    margin: 0 2px;
                    padding: 1px;
                    width: 28px;
                    height: 28px;
                    line-height: 28px;                  
                    position: relative; 
                    cursor: pointer;
                    float: left;      
                    display: block;         
                    border: 2px solid transparent;   
                    @include transition(all 0.35s ease 0s);  
                    .inner {
                        width: 100%;
                        height: 100%;
                        border: 1px solid $body-border-color;  
                    }       
                    a {                            
                        padding: 0; 
                        position: relative;
                        display: block;    
                        font-size: 0;
                        width: 100%;   
                        height: 100%; 
                    } 
                    &:hover {
                        @media (min-width: 1025px) {   
                            border-color: #bcbcbc;                   
                        } 
                    }
                    &.active-filter { 
                        border-color: #bcbcbc;       
                    }  
                } 
            }
        }    
    }  
}
.filter-order-form {
    @include drawer-sidebar;
}
.filter-sidebar { 
    &.position-sidebar {
        &.style-normal {
            .sb-filter-wrapper .f-close {
                @media (min-width: 992px) {
                    display: none;
                }
            } 
        }
        &.style-accordion {  
            .sb-filter-wrapper {
                .accordion {
                    .sbf-title {
                        position: relative;
                        cursor: pointer;
                        padding: 3px 0;    
                        @include padding-end(20px);        
                        &:before {
                            content: "\e873";
                            font-family: $font-family-icon;
                            font-size: $body-font-size - 2px; 
                            width: 20px;  
                            height: 20px;
                            line-height: 20px;
                            text-align: center;
                            font-weight: 400;
                            @include ab(absolute, block, 2px, n, n, 0);
                            @include transition(all 0.35s ease 0s);
                        }             
                    }
                    &.accordion-active {           
                        .sbf-title:before {
                            content: "\e874"; 
                        }               
                    }
                }        
                .f-close {
                    @media (min-width: 992px) {
                        display: none;
                    }
                }
            }
        }
    }
    &.position-body {
        &.style-toggle {
            @include toggle-sidebar; 
            .sb-filter-tag {
                padding-bottom: 10px;
                letter-spacing: -0.31em;
                vertical-align: top;
                li {
                    padding: 0;
                    @include padding-end(10px);
                    display: inline-block;
                    letter-spacing: 0;
                    vertical-align: top;
                    margin: 0 0 13px;
                    line-height: 20px; 
                    @media (min-width: 1200px) {
                        width: 16.666667%;
                    }
                    @media (max-width: 1199px) and (min-width: 992px) {
                        width: 20%;
                    }
                    @media (max-width: 991px) and (min-width: 768px) {
                        width: 25%;
                    }
                    @media (max-width: 767px) and (min-width: 556px) {
                        width: 33.333333%;
                    }
                    @media (max-width: 555px) {
                        width: 50%;
                    }
                    &:first-child {
                        display: block;
                        width: 100%;
                    } 
                }
            }
        }
        &.style-drawer {
            @include drawer-sidebar;
            h5 {
                font-size: $size-h4;      
                font-weight: 400;     
                text-transform: capitalize; 
                border-bottom: 1px solid $body-border-color;
                line-height: 1.4;
                margin: 0 0 35px;
                padding: 0 0 10px;
                position: relative;
                &:before {
                    content: '';
                    width: 70px;
                    height: 2px;
                    background: $main-color;
                    @include ab(absolute, block, n, 0, -1px, n);
                }
            }
            .sbw-filter > div .sb-filter {
                margin: 0 0 30px; 
                &:last-child {
                    border-bottom: none;
                    padding-bottom: 0;
                }   
            }
        }
    }
}
.group-toolbar {
    .filter-of-icon,
    .filter-icon {
        float: left;
        line-height: 30px;
        cursor: pointer; 
        @include padding-end(54px);
        color: $body-general-color; 
        @media (max-width: 991px) {
            @include margin-end(auto);
            @include padding-end(0);
        }        
        i {
            @include margin-end(10px);
            font-weight: 400;
            float: left;
            font-size: $font-small-icon;
            color: $body-general-color;    
            @include transition(all 0.35s ease 0s); 
            &:before {
                display: block;
                line-height: 30px;
            }      
        }    
        &:hover i {
            color: $body-link-color-hover;    
        }   
    }    
}
 
.templateCollection { 
    #body-content {                  //fixed fashion-modern
        padding-bottom: 0;
    } 
    .main-content .shopify-section:first-child {            //fixed fashion-modern
        padding-bottom: 65px;  
        @media (max-width: 991px) {
            padding-bottom: 50px;
        }
        @media (max-width: 767px) {
            padding-bottom: 30px;
        }
    }
    .left-column-container {
        @media (max-width: 991px) {
            @include drawer-sidebar;
        }
    }
    #col-main:not(.page-sub-collections-content) { //button next prev sub collection 
        overflow: hidden; // fixed left right border-top 
    } 
    &.sidebar-opened,
    &.order-sidebar-opened {    
        .filter-order-form,
        .filter-sidebar.position-body.style-drawer {             
            @include opacity(1);        
            @include translate(0, 0);       
        } 
        .left-column-container {
            @media (max-width: 991px) {
                @include opacity(1); 
                @include translate(0, 0);
            }
        }
    } 
    .block-banner {
        margin-bottom: 72px;  
        @media (max-width: 991px) {
            margin-bottom: 32px;
        }
        @media (max-width: 767px) {
            margin-bottom: 15px;   
        }    
        >div {
            margin: 0;
            >div {
                padding: 0;
            }   
        }
        .banner {
            position: relative;
            img {
                width: 100%;
            } 
        }
        .banner-content {
            @include ab(absolute, block, 50%, n, n, n);
            @include translate(0, -50%);
        }
        p {
            font-size: $body-font-size + 10px;
            font-weight: 400; 
            margin: 0; 
            @media (max-width: 767px) {
                font-size: $body-font-size; 
            }    
        }  
        h3 {
            padding: 10px 0; 
            font-size: $size-h1 + 20px;
            font-weight: $font-bold;
            line-height: 1.0; 
            @media (max-width: 1024px) {
                font-size: $size-h1;
            }  
            @media (max-width: 767px) {
                font-size: $size-h1 - 10px;
                padding: 5px 0; 
            }
        }
        .btn {
            margin-top: 13px;
            @media (max-width: 767px) {
                margin-top: 5px;  
                line-height: 18px;
            }   
        }   
        .half-width .banner-content {
            @include ab(n, n, n, 0, n, 0); 
            text-align: center;
        }   
        .full-width .banner-content {
            @include ab(n, n, n, 99px, n, n); 
            @media (max-width: 1024px) {
                @include ab(n, n, n, 70px, n, n);
            } 
            @media (max-width: 767px) {
                @include ab(n, n, n, 20px, n, n);
            }  
        }  
    }   
}
.page-cata {  
    padding: 23px 0 0;
