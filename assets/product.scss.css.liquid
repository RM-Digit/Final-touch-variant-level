        font-weight: 400;  
        @media (max-width: 479px) {
            font-size: $product-font-size - 2px;      
        }
        &.balance-true a {
            display: block;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden; 
        }   
        a {
            color: inherit;
            font-size: inherit;      
        }
        &:hover a {
            color: $body-link-color-hover; 
        } 
    } 
    .product-vendor {  
        display: none;   
        a {
            font-size: $body-font-size - 2px;
            color: $body-link-color;
            text-transform: uppercase;
            &:hover {
                color: $body-link-color-hover;
            }
        }
    }
    .product-price + .product-review {
        padding-top: 12px;    
    }  
    .product-review {        
        margin: 0;
        overflow: hidden;    
        .spr-badge-starrating {
            float: none;
            margin: 0;
            font-size: $body-font-size - 1px;  
            @include padding-end(7px);
        }  
        .spr-badge-caption {
            display: none;  
            font-weight: $font-bold;
            text-transform: uppercase;
            font-size: $body-font-size - 3px;   
        }   
    }  
    .product-list-mode-content {
        display: none; 
    }
    .list-v2-label {
        display: none; 
    }
    .product-list-mode-content-v2 {
        display: none; 
    }       
    .product-content {                    
        .pc-inner {
            display: block;
            width: 100%;    
            @media (max-width: 1024px) {
                position: relative;
            }
        }
        .product-description {               
            margin: 15px 0 0;      
            display: none;   
            font-size: $body-font-size - 1px;
            color: $body-general-color;
            br {
                content: 'A';
                display: block;
                margin: 0 0 15px; 
            }     
            ul li {
                list-style: circle outside none;
            }
            ol li {
                list-style: decimal outside none;
            }
