        margin: 0 -15px;  
        >div {
            padding: 0 15px;
        }
    } 
    .left-column-container { 
        .sb-widget { 
            margin-bottom: 36px;
            @media (max-width: 991px) {
                margin: 0 0 20px;
            } 
            .sb-menu h5, .sb-filter-wrapper h5{
                font-size: $size-h5 - 5px;
                color: $main-color;
                font-weight: $font-bold;   
            } 
            &.sb-border {
                padding: 25px 30px; 
            }  
        }
        .sb-menu {
            .sb-title {
                margin: 0 0 12px; 
            }      
            > ul {  
                padding-bottom: 5px;
            }   
        }
    }  
    .loading_img {  
        text-align: center;
        i {
            font-size: 35px;  
            &:before {
                color: $main-color;
                -webkit-animation: fa-spin 2s infinite linear;
                animation: fa-spin 2s infinite linear;
            }
        }
    }
}
.cata-image {
    margin: 0 auto 30px;
    img {
        width: 100%;   
    }
}
#col-main + .cata-description {
    padding: 30px 0 0;
}  
.cata-description { 
    padding: 0 0 30px;
    text-align: justify;
