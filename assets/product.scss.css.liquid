        padding-top: 15px;
        padding-bottom: 15px;
        @media (min-width: 1400px) {
            @include padding-start(60px);
            @include padding-end(60px);
        }
        @media (max-width: 1399px) {
            @include padding-start(calc(5%));
            @include padding-end(calc(5%));
        }
    }
    #hi-cookie-box {    
        width: 100%;
        max-width: 1200px; 
        margin: 0 auto;
        overflow: hidden;  
        @media (min-width: 1200px) {
            text-align: end;
        }
    }
    .hi-cookie-head,
    .hi-cookie-intro {
        margin: 0;
        width: 55%;
        padding: 0 15px;
        float: left;    
        clear: both;
        text-align: start;
        @media (max-width: 1199px) {
            width: auto; 
            display: table-cell; 
            float: none;
        }
        @media (max-width: 767px){
            width: 100%; 
            display: block; 
            padding: 0;
        }
        .read-more {
            display: none;
        }
        br {
            display: none;
        }
    }  
    .hi-cookie-happy {
        display: inline-block;
        padding: 0 15px;
        margin: 0;
        width: 18%;    
        text-align: start;
        @media (max-width: 1199px) {
            width: auto; 
            display: table-cell; 
            float: none;
            margin: 0;
        }
        @media (max-width: 767px){
            width: 100%; 
            display: block; 
            padding: 0;
        } 
    }
    .hi-cookie-btn {
        display: inline-block;
        width: auto;
        margin: 3px 5px 0;                
        cursor: pointer;  
        @media (max-width: 1199px) {
            margin-top: 5px;
        }      
        &.disagree {      
            padding: 5px 25px;
            @include border-radius-50;      
            @media (max-width: 767px) {
                @include padding-start(20px);
                @include padding-end(20px);
            }
        }
        &.agree {      
            padding: 5px 25px;
