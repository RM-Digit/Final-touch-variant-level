@mixin swatch-product {
	.swatch {
		margin: 0 0 21px;
		position: relative;
		@media (max-width: 767px) {
			@include margin-end(15px);
		}
		.header {
			padding: 0 0 4px;
			display: block;
			margin: 0;
			color: $main-color;
			text-transform: uppercase;
			font-size: $body-font-size - 2px;
			font-weight: $font-bold;
		}
		input {
			display: none;
		}
		.swatch-element {
			margin: 0;
			padding: 0;
			float: left;
			&.colour label {
				background-color: transparent;
			}
			.crossed-out {
				display: none;
			}
			label {
				min-width: $color-swatch-width;
				height: $color-swatch-height;
				line-height: $color-swatch-height - 5px;
				position: relative;
				margin: 0 0 5px;
				@include margin-end(5px);
				text-align: center;
				color: $body-general-color;
				font-size: $body-font-size - 1px;
				word-break: break-word;
				white-space: normal;
				font-weight: 600;
				text-transform: none;
				cursor: pointer;
				background-color: #e6e6e6;
				background-repeat: no-repeat;
				background-position: center;
				background-size: cover;
				border: 2px solid transparent;
				padding: 0 5px;
				@include border-radius-reset;
				@include box-shadow(none);
				&:before {
					@include ab(absolute, block, 0, 0, n, n);
					width: 100%;
					height: 100%;
					border: 1px solid $body-border-color; 
				}
			}
			input:checked+label {
				box-shadow: none;
				cursor: default;
				border: 2px solid $main-color; 
			}
			&.circle label {
				@include border-radius-circle;
			}
			&.img-true label {
				min-width: $image-swatch-width;
				height: $image-swatch-height;
			}
			&:hover {
				@media (min-width: 1025px) {
					label {
						border: 2px solid $main-color;
						font-weight: 600;  
					}
				}
			}
			&.soldout {
				label {
					cursor: default;
				}
				&:hover {
					@media (min-width: 1025px) {
						label {
							cursor: default;
							color: $body-general-color;
							border: 2px solid transparent;
						}
					}
				}
			}
			&.colour:not(.soldout) {
				input:checked+label {
					cursor: default;
					border: 2px solid $main-color;
				}
				&:hover {
					@media (min-width: 1025px) {
						label {
							border: 2px solid $main-color;
						}
					}
				}
			}
		}
		&:last-child {
			margin-bottom: 15px;
		}
	}
}
@mixin drawer-sidebar {
	@include ab(fixed, n, 0, 0, n, n);
	z-index: 999999;
	height: 100%;
	width: 300px;
	padding: 50px 20px 30px;
	overflow-y: auto;
	background: $body-background-color;
	@include translate(-300px, 0);
	@include opacity(0);
	@include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));
	@media (max-width: 300px) {
		width: 100%;
		padding-bottom: 20px;
	}
	.f-close,
	.fof-close {
		text-align: center;
		cursor: pointer;
		@include border-radius-circle;
		@include transition(all 0.35s ease 0s);
		@include width-btn;
		color: $body-link-color;
		@include ab(absolute, block, 10px, n, n, 10px);
		i:before {
			content: "\e816";
			font-family: $font-family-icon;
		}
		&:hover {
			color: $body-link-color-hover;
		}
	}
}
@mixin toggle-sidebar {
	position: relative;
	width: 100%;
	display: none;
	padding: 25px 15px 0;
	margin: 0 auto 20px;
	background: $body-background-color;
	@include box-shadow(0 0 10px rgba($black, 0.05));
	.sb-title,
	.f-close {
		display: none;
	}
	.sb-filter-wrapper {
		width: 100%;
		display: table;
		vertical-align: top;
	}
	.sbw-filter {
		width: 25%;
		display: inline-block;
		vertical-align: top;
		@media (max-width: 991px) {
			width: 33.33333%;
		}
		@media (max-width: 767px) {
			width: 50%;
		}
		@media (max-width: 575px) {
			width: 100%;
		}
		>div .sb-filter {
			padding: 0 15px;
			clear: none;
			float: none;
			display: inline-block;
			letter-spacing: 0;
			vertical-align: top;
			border: none;
			margin: 0 0 30px;
			width: 100%;
		}
	}
}
@mixin icons-number {
	@include ab( absolute, block, -15px, n, n, -10px);
	@include border-radius-circle;
	width: 20px;
	height: 20px;
	line-height: 20px;
	background: $header-icon-color;
	color: $header-number-on-icon-color;
	text-align: center;
	font-size: 11px;
	font-weight: $font-bold;
}
@mixin hover-icons-number {
	-webkit-animation-name: hvr-icon-bob-float, hvr-icon-bob;
	animation-name: hvr-icon-bob-float, hvr-icon-bob;
	-webkit-animation-duration: .3s, .75s;
	animation-duration: .3s, 0.75s;
	-webkit-animation-delay: 0s, .3s;
	animation-delay: 0s, .3s;
	-webkit-animation-timing-function: ease-out, ease-in-out;
	animation-timing-function: ease-out, ease-in-out;
	-webkit-animation-iteration-count: 1, infinite;
	animation-iteration-count: 1, infinite;
	-webkit-animation-fill-mode: forwards;
	animation-fill-mode: forwards;
	-webkit-animation-direction: normal, alternate;
	animation-direction: normal, alternate;
}
@mixin clearfix {
	&:before,
	&:after {
		display: block;
		width: 100%;
		content: "";
		clear: both;
	}
}
@mixin is-countdown-separate($size, $lheight, $height) {
	&:after {
		content: ':';
		color: $body-general-color;
		font-size: $size;
		font-weight: $font-bold;
		line-height: $lheight;
		height: $height;
		@include ab(absolute, block, 0, n, n, -4px);
	}
	&:first-child {
		@include padding-start(0); 
	}
	&:last-child {
		@include padding-end(0);
		&:after {
			content: none;
		}
	}
}

@mixin carousel-loading {
	@include opacity(0); //height 0 vertical srcoll
	visibility: hidden;
	&.owl-loaded {
		@include opacity(1);
		visibility: visible;
	}
}
@mixin product-add-cart {
	.demo-icon {
		@include margin-end(10px);
		display: none;
	}
	.btn-add-cart {
		text-align: center;
		background: $button-btn1;
		color: $button-btn1-txt;
		padding: 9px 4px 10px;
		text-transform: uppercase;
		font-weight: $font-bold;
		display: block;
		&.sold-out {
			@include opacity(0.65);
		}
	}
	&:hover .btn-add-cart {
		text-decoration: underline;
		&.disabled {
			text-decoration: none;
		}
		&.sold-out {
			text-decoration: none;
			cursor: default;
		}
	}
}
@mixin icon-product {
	float: left;
	width: 36px;
	height: 36px;
	line-height: 34px;
	text-align: center;
	@include margin-end(2px);
	background: $body-background-color;
	color: $body-link-color;
	cursor: pointer;
	&:last-child {
		@include margin-end(0);
	}
	a {
		color: inherit;
		font-size: $body-font-size - 1px;
		@include transition(none);
	}
	i {
		color: inherit;
		font-size: $body-font-size - 1px;
	}
	&:hover {
		color: $body-link-color-hover;
	}
}
@mixin width-boxed {
	width: 100%;
	margin: 0 auto;
	@include padding-start(15px);
	@include padding-end(15px);
	@media (min-width: 576px) {
		max-width: 100%;
	}
	@media (min-width: 768px) {
		max-width: 760px;
	}
	@media (min-width: 992px) {
		max-width: 980px;
	}
	@media (min-width: 1200px) {
		max-width: 1200px;
	}
	@media (min-width: 1440px) {
		max-width: $container-width;
	}
}
@mixin icon-btn-banner {
	position: relative;
	font-weight: 500;
	padding: 5px 30px; 
	&:after {
		content: "\e876";
		font-size: 12px;
		font-family: $font-family-icon;
		color: inherit;
		display: inline-block;
		vertical-align: middle;
		@include padding-start(10px);
	}    
}
@mixin vertical-menu {
	position: relative;
	background: $vertical-header-nav-bg;
	.navigation {
		border: 1px solid $vertical-nav-border; // fixed bug ul
		&.border-color-none {
			border: none;
			.main-nav {
				padding: 22px 0;
				min-height: 450px; 
				>li {
					border-bottom: none;
					>.dropdown-inner >a, 
					>a {
						padding: 9px 0; 
					}
				}
			}
		}
	} 
	.head {
		color: $vertical-header-nav-text;
		font-size: $nav-font-size;
		font-weight: $font-bold;
		cursor: pointer;
		padding: 12px 0; //header 1
		@include padding-start(20px);
		@include padding-end(78px);
		@media (max-width: 1199px) {
			@include padding-end(30px);
		}
		i {
			@include padding-end(9px);
		}
	}
	.navbar {
		z-index: 1001;
		width: 100%;
		min-width: 260px;
		background: $vertical-nav-bg;
		padding: 0;  
		@include ab(absolute, block, 100%, 0, n, n);
		@include transform(scaleY(0));
		@include transform-origin(0 0);
		@include transition(transform 0.3s ease-in-out);
		.navbar-collapse {
			position: relative;  
			display: block;
		}
		&.opened {
			@include transform(scaleY(1));  
		}
	}
	.navbar-collapse >ul {   
		position: static;
		border: none;
		margin: 0;
		padding: 0;
		.labels-wrapper {
			@include ab(n, n, 2px, auto, n, -30px);
		}  
		>li {
			padding: 0;
			@include padding-start(29px);
			@include padding-end(15px);
			position: static;
			display: block;
			border-bottom: 1px solid $vertical-nav-border; 
			cursor: pointer;
			>.dropdown-inner >a,
			>a {
				font-size: $nav-font-size - 2px; // furniture
				margin: 0;
				padding: 14px 0;
				display: inline-block;
				text-decoration: none;
				color: $vertical-nav-link-color;
				line-height: 19px;
				position: relative;
				@media (max-width: 1199px) {
					font-size: $nav-font-size - 2px;
				}
				>span:not(.labels-wrapper) {  
					position: relative; 
					&:after {
						content: "";
						@include ab(absolute, block, n, 0, 1px, n);
						width: 0;
						background: $vertical-nav-hover-color;  
						height: 1px;
						@include transition(width 0.3s ease-in-out);  
					}
				}
			}
			>.dropdown-inner {
				position: relative;
			}
			&:hover {
				>a,
				>.dropdown-inner >a {
					color: $vertical-nav-hover-color;
					>span:after {
						width: 100%;
					}
				}
			}
			/*have submenu*/
			&.dropdown {
				>.dropdown-menu {
					background: $nav-dropdown-background;
					font-size: $body-font-size;
					padding: 20px 0 35px;
					@include ab(n, n, 0, 100%, n, n);
					@include margin-start(0);
					@include border-start(1px solid $body-border-color); 
					min-height: 100%;   
					z-index: -1001; 
					@include transition(all 0s); 
					&.width-100 {
						width: calc(980px - 30px - 260px); // skin market    
						@media (min-width: 1200px) {
							width: calc(1200px - 30px - 260px);   
						}
						@media (min-width: 1440px) {
							width: calc(#{$container-width} - 30px - 260px);
						}
					}
					&.width-75 {
						width: calc((980px - 30px - 260px) * 3 / 4);   
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) * 3 / 4);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) * 3 / 4);
						}
					}
					&.width-50 {
						width: calc((980px - 30px - 260px) / 2);    
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) / 2);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) / 2);
						}
					}
					&.width-25 {
						width: calc((980px - 30px - 260px) / 4);    
						@media (min-width: 1200px) {
							width: calc((1200px - 30px - 260px) / 4);  
						}
						@media (min-width: 1440px) {
							width: calc((#{$container-width} - 30px - 260px) / 4); 
						}
					}
					>li {
						position: static;  
						.dropdown-inner {
							position: relative;
						}
						a.dropdown-link {
							@include padding-end(0);
							@include margin-end(30px);
						}
						.dropdown-menu {
							background: $nav-dropdown-background;
							@include border-start(1px solid $body-border-color); 
							min-height: 100%;  
							>li {
								>a {
									color: $nav-dropdown-link-color;
									white-space: normal;
								}
								&:hover {
									>a {
										color: $nav-dropdown-link-hover-color;
									}
								}
							}
						}
						&:hover {
							>a,
							>.dropdown-inner >a {
								color: $nav-dropdown-link-hover-color;
							}
						}
					}
					.block-text {
						margin-top: 24px;
						.text-1 {
							color: rgba($nav-dropdown-link-color, 0.5);  
							.special {
								color: #d80006; 
							}
						}
						.text-2 {
							color: $nav-dropdown-link-color;
							font-size: $size-h1 - 5px; 
							line-height: 1.2; 
						}
					}
				}
				.dropdown-menu-sub {
					background: transparent;
					border: 0;
					@include border-radius-reset;
					box-shadow: none;
					position: static;
					width: 100%;
					min-width: auto;
					margin: 0;
					>li {
						>a {
							color: $nav-dropdown-link-color;
							padding: 0;
							border-bottom: none;
							span {
								color: $nav-dropdown-link-color;
							}
						}
						&:hover {
							background: none;
							>a {
								color: $nav-dropdown-link-hover-color;
								span {
									color: $nav-dropdown-link-hover-color;
								} 
							}
						}
					}
				}
				.banners {
					.dropdown-menu-sub >li >a {
						display: block;    
						width: 100%; 
						&:after {
							content: none; 
						}
					}
					img {
						@include dropdown-hidden;  
					}
				}
				&.mega-is-hover {
					.dropdown-menu {
						z-index: 1001;
						@include dropdown-visible; 
					} 
					.banners img {
						@include dropdown-visible;
					}
				}   
			}
			&:last-child {
				border-bottom: none;
			}
		}
		>li.mega-menu {
			>.dropdown-menu {
				padding: 33px 37px;
			} 
			.dropdown-menu {
				.mega-sub-link {
					margin: 0 0 20px;
					&.big-size >a {
						font-size: $size-h1 + 5px;
						margin-bottom: 18px;  
					}
					>a {
						font-size: $nav-sub-font-size + 1px;
					}
					.dropdown-menu-sub {   
						li {
							padding: 5px 0 4px; 
							a:hover {
								color: $nav-dropdown-link-hover-color;
							}
							&.bp-item-2 {
								padding-top: 15px;
							}
						}
					}
					&:last-child {
						margin: 0;
					}
				}
				.product-wrapper {
					background: none;
					padding: 0;
					margin-top: 0; 
					&:after,
					&:before {
						content: none;
					}
					.product-head .product-add-cart .btn-add-cart {
						padding: 5px 4px;
						border: 1px solid $button-btn1-border;
						&:hover {
							color: $button-hover-btn1-txt; 
							border: 1px solid $button-hover-btn1-border;   
						}  
					}
					.featured-img a {
						padding: 0;
					}
					.product-name {
						border: none;
						font-size: $product-font-size;
						&:before {
							content: none;
						}
						a {
							padding: 0;
							font-size: $product-font-size;
						}
					}
					.product-price+.product-review {
						padding-top: 5px; 
					}
				}
			} 
		}
	} 
	&.vertical-secondary-nav { 
		.vertical-navbar {
			border: 2px solid $main-color; 
		} 
		.navbar-collapse >ul >li {
			>.dropdown-inner >a,
			>a {
				font-size: $nav-font-size;	   
			}
			>.dropdown-menu {
				border: 2px solid $main-color;
				@include border-start(1px solid $body-border-color); 
				margin-top: -2px;   
			}  
			&.mega-menu .dropdown-menu .mega-sub-link .dropdown-menu-sub li {
				padding: 2px 0; 
			}
		} 
		.navigation.border-color-none .navbar-collapse >ul:not(.secondary-nav) {
			min-height: auto;
			padding: 19px 0 22px;     
			>li {
				>.dropdown-inner >a,
				>a {
					padding: 4px 0;  
				}  
			}
		}
		ul.secondary-nav {
			border-top: 1px solid $body-border-color;  
			padding: 22px 0;     
			li {
				font-weight: 400;   
				border-bottom: none; 
				a {
					padding: 4px 0;    
					font-size: $nav-font-size - 1px;  
				}
			}
		}
	}   
} 
@mixin progress-bar($bg-bar) {
	@include border-radius-reset;
	height: 10px;
	background: #e6e6e6;
	.progress-bar {
		background: $bg-bar;
	}   
}
@mixin padding-follow-menu($pt, $pb) {
	.table-row {
		>.header-logo, >.header-icons, //furniture home 1  
		>.searchbox, >.header-switcher {   
			@media (min-width: 992px) {
				padding-top: $pt;
				padding-bottom: $pb; 
			}
		}
	}
}
@mixin currency-language {
	.translate-position .weglot-container .country-selector ul {
		@media (max-width: 1024px) {
			@include ab(n, n, n, auto, n, 0);
		}
	}
	.currency-position ul.currencies {
		@media (max-width: 1024px) {
			@include ab(n, n, n, auto, n, 0);
		}
	}
}
@mixin similar-search($w) { 
	.searchbox {
		width: $w;
		.navbar-form {
			width: 100%;
		}
		.search-icon {
			@include ab(n, n, n, n, n, 0);
			width: 40px;
			height: 100%;
			background: $main-color;
			color: $header-search-icon;
		}
		.result-ajax-search {
			width: 100%;
		}
	}
	+.searchbox-mobile .search-icon {
		@include ab(n, n, n, n, n, 0);
		width: 40px;
		height: 100%;
		background: $main-color;
		color: $header-search-icon;
	} 
}

@mixin line-left-right {
	@include flexbox;
	position: relative;
	align-items: center;
	&:before, &:after {
		content: "";
		border-bottom: 1px solid $body-border-color;     
		width: 3000px;
		@include ab(absolute, block, n, n, n, n);
	}
	&:before {
		@include ab(n, n, n, n, n, 100%);
	}
	&:after {
		@include ab(n, n, n, 100%, n, n);  
	} 
}
@mixin header-icon-left {
	@include ab(n, n, -15px, -15px, n, auto);
}
/*=== pages.scss ===*/
.share-links {
    &.social-sharing {
        .list-inline {
            margin: 0;
            li {       
                margin: 0;  
                @include margin-end(10px);
                width: auto;
                height: auto;
                a {
                    border: none;
                    position: relative;
                    display: block;   
                    overflow: hidden;
                    line-height: 28px;
                    padding: 0 8px;
                    color: $white;             
                    @include border-radius(2px, 2px, 2px, 2px);          
                    i {
                        font-size: $body-font-size + 2px;
                        line-height: 28px;
                        float: left;
                        @include margin-end(5px);
                    }              
                    span {
                        display: inline-block;
                        font-size: $body-font-size - 1px;
                    } 
                    &.facebook {
                        background: #3b5998;
                    }
                    &.twitter {
                        background: #1da1f2;
                    }
                    &.google {
                        background: #dd4b39;
                    }
                    &:hover {
                        @include opacity(0.8);
                        color: $white;
                    }         
                }
                &:last-child {
                    @include margin-end(0);
                }        
            }
        }
    }
}
.question-ship-block {
    margin-bottom: 30px;
} 
.panel-group {
    margin: 0;
    padding: 0;
    clear: both;
    .faq-title {
        font-size: $size-h2;
        font-family: $font-heading;
        font-weight: $font-bold;
        margin-bottom: 20px;
    } 
    .panel {
        border-radius: 0;
        border: none;
        margin: 0;
        box-shadow: none;
        background: none;
        padding: 0;
        .collapse  {
            border: none;
        }
        .panel-heading {
            background: none;           
            text-transform: uppercase;
            padding: 0;
            border-bottom: 1px solid $body-border-color;
            .panel-title {
                color: $body-link-color;
                font-size: $size-h4;
                font-family: $font-heading;
                padding: 7px 0;
                a {
                    color: $body-link-color;
                    font-size: $size-h4;
                    font-family: $font-heading;
                    display: block;                           
                    position: relative;
                    text-transform: initial;  
                    line-height: 30px;          
                    @include transition(all 0.35s ease 0s);           
                    &.collapsed {
                        color: $body-link-color;             
                    }
                    &:hover {            
                        color: $body-link-color-hover;             
                    }         
                }
            }
        }
        .panel-collapse {
            > .panel-body, > .list-group {
                border: none;
                border-bottom: 1px solid $body-border-color;
                padding: 25px 0;
                background: none;
            }
        }
        + .panel {
            margin-top: 0;
        }
        .panel-body {   
            font-size: $body-font-size;
            line-height: 24px;      
            iframe {
                max-width: 100%;
            }
            ul li {
                padding: 1px 0;
            } 
        }
        &:last-child {
            padding-bottom: 0;
            border-bottom: none;
            .panel-heading {
                border-bottom: none;  
            }
            .panel-collapse {
                > .panel-body, > .list-group {
                    border-top: 1px solid $body-border-color;
                    border-bottom: none;
                }
            }      
        }
    }
}
.spr-icon { 
    @include opacity(1); 
    background: none;
    display: inline-block; 
    width: 16px;
    height: 14px;
    font-style: normal;   
    &:before {    
        content: "\e830";
        font-family: $font-family-icon;
        font-size: 14px;
    }
    &.spr-icon-star-empty {
        color: $body-ratting-empty-color;
        &:before {   
            color: $body-ratting-empty-color;
            content: "\e831";
            font-family: $font-family-icon;
            font-size: 14px; 
        } 
    }
}
#main-column {
    margin: 35px auto 0;
}
.main-content{ 
    position: relative; 
    clear: both; 
    > section.cata-header {  
        position: relative;
        .catalog-title {     
            width: 100%;
            text-align: center;
            z-index: 1;
            margin: 0;
            font-size: 48px;
            @include ab(absolute, block, 25%, 0, n, n);
        }
        > img {
            min-height: 104px;
        }
    } 
}
/*=== breadcrumb ===*/ 
.breadcrumb {
    padding: 0 10px;
    margin: 0 -10px 0;
    background: transparent;  
    @include border-radius(0, 0, 0, 0);     
    @include line-left-right;      
    li { 
        position: relative;   
        float: left;     
        color: $body-link-color;  
        text-transform: uppercase;
        font-size: $body-font-size - 3px;
        padding: 0 10px; 
        &:nth-child(3) {
            @media (max-width: 575px) {
                @include padding-start(0);
            } 
        }
        &:first-child {
            @include padding-start(0);
            @media (max-width: 575px) {
                display: none;
            }   
        }      
        &:last-child {
            @include padding-end(0);
            &:before { 
                content: none;
            }
        }
        &:before {
            content: "/";
            @include ab(absolute, block, 0, n, n, 0);   
        }     
        a { 
            color: inherit; 
            &:hover {
                color: $body-link-color-hover;
                text-decoration: underline;
            }
        }     
    }  
}   
.wrap-breadcrumb {
    &.breadcrumb-center .breadcrumb-holder {
        justify-content: center;
    } 
    .breadcrumb-holder {
        @include flexbox;
        flex-wrap: wrap;
        align-items: center; 
        height: 100%;
        padding: 0;
        max-width: calc(100% - 30px);   
        @media (min-width: 768px) {
            max-width: calc(760px - 30px);
        }
        @media (min-width: 992px) {
            max-width: calc(980px - 30px);
        }
        @media (min-width: 1200px) {
            max-width: calc(1200px - 30px);
        }
        @media (min-width: 1440px) {
            max-width: calc(#{$container-width} - 30px);
        }
    }
    .category-top-menu {
        width: 100%;
        padding-top: 11px;  
        @media (max-width: 767px) {
            display: none; 
        }
        .category-top-menu-list {
            padding-top: 65px;
            @media (max-width: 991px) {
                padding-top: 32px; 
            }
            .menu-border-right {
                @include padding-end(40px);
                @media (max-width: 991px) {
                    @include padding-end(20px);   
                }
                .menu-box {
                    @include border-end(1px solid $body-border-color);  
                }
            }
        } 
        h6 {
            color: #656a7e;
            padding-bottom: 15px;  
            min-height: 32px; 
        }  
        ul li {
            padding-bottom: 10px; 
            a {
                font-size: $body-font-size + 2px;    
            }
        }
    }
} 
.bw-image {  
    clear: both;
    width: 100%;
    height: $breadcrumb-height;  
    margin: 0 auto 20px;
    overflow: hidden;    
    position: relative;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;  
    @media (max-width: 767px) {
        background-image: none !important;
        height: auto !important;
    } 
}
.bw-color {   
    background: $breadcrumb-bg-color;
    margin: 15px auto 18px;              
    overflow: hidden; 
    position: relative; 
    @include flexbox;
    align-items: center; 
}
.flex-header-left {
    .bw-image {
        margin-top: 20px; 
    }
    .bw-color {
        margin-top: 20px;
    }
}  
.breadcrumb-none #body-content {
    padding-top: 30px;
} 
/*=== comments ===*/
.comment-form {
    h3 {                
        margin: 0 0 6px;    
        position: relative;
        font-size: $size-h1;
        text-transform: uppercase; 
    }  
    p {
        margin: 0 0 13px; 
    }    
    .control-wrapper {
        input {
            width: 100%;             
            margin-bottom: 25px;  
            height: 38px;
            padding-top: 8px;    
            padding-bottom: 8px; 
        }
        textarea {
            margin: 0;
            width: 100%; 
            max-width: 100%;      
            min-height: 180px;
        }    
        &:last-child {
            padding-top: 0;
        }    
    }
    > .btn {
        margin: 25px 0 0;
        padding: 5px 30px;
    }
}
.grid-comments {
    h5 {
        margin: 0 0 15px;    
        position: relative;
        font-size: $size-h1;
        text-transform: uppercase;  
    }
    .comments {
        list-style: none;
        margin: 45px -26%;
        padding: 44px 0 0;
        border-top: 1px solid $body-border-color; 
        @media (max-width: 1024px) {
            margin: 45px -10.5%; 
            padding: 34px 0 0;
        }
        @media (max-width: 767px) {
            margin: 25px 0;
            padding: 25px 0 0; 
        } 
        .comment {
            border-bottom: 1px solid $body-border-color;
            margin: 0 0 44px;
            padding: 0 0 44px;
            @media (max-width: 1024px) {
                margin-bottom: 34px;
                padding-bottom: 34px; 
            } 
            @media (max-width: 767px) {
                margin-bottom: 25px;
                padding-bottom: 25px;
            }
            &:last-child {
                margin-bottom: 0; 
            }  
        }  
        .load-comment {
            display: table;
            width: 100%;
            color: $body-general-color; 
            .comment-avatar {
                display: table-cell;
                vertical-align: top;
                width: 80px; 
                @include border-radius-circle;
                @media (max-width: 479px) {
                    width: 50px; 
                } 
                img {
                    @include border-radius-circle;
                    width: 100%;
                }
            } 
            .author {
                font-weight: $font-bold;
                font-size: $size-h4;
                text-transform: uppercase;
            }
            .date {
                font-weight: $font-bold; 
                font-size: $body-font-size - 3px;
            } 
            p { 
                margin: 10px 0 0;
                font-size: $body-font-size; 
            }
        }
    }
}
/*=== popup-sell-people ===*/
.popup-sell-people {
    @include box-shadow(0px 2px 5px rgba(0, 0, 0, 0.1)); 
    width: 410px; 
    display: table;
    vertical-align: middle;
    position: fixed;
    background: $body-background-color;
    border: 1px solid #cccccc;   
    z-index: 1002;
    padding: 20px;
    display: none;
    @media (max-width: 575px) { 
        width: calc(100% - 20px);    	
    }
    &.show {
        display: block;
    }
    &.bottom-left {
        @include ab(n, n, n, 10px, 10px, n);
        @media (max-width: 991px) {
            bottom: 64px; 
        } 
    }
    &.bottom-right {
        @include ab(n, n, n, n, 10px, 10px);
        @media (max-width: 991px) {
            bottom: 64px; 
        } 
    }
    &.top-left {
        @include ab(n, n, 10px, 10px, n, n);
    }
    &.top-right {
        @include ab(n, n, 10px, n, n, 10px);
    }
    .img-user {
        width: 40px;		
        display: table-cell;
        vertical-align: middle;
        img {
            width: 100%;
        }
    }	
    .text-detail {
        display: table-cell;
        vertical-align: middle; 
        @include padding-start(22px);     
        .sell-text {
            margin: 0;
            line-height: 20px;
            font-size: $body-font-size;
            font-weight: 400;
            strong {
                display: block;
                font-size: 22px;
                margin-bottom: 5px;  
            }
        }
    }
}
/*=== discount-modal ===*/
#discount-modal {
    z-index: 99991; 
    @include ab(fixed, none, 0, 0, n, n); 
    width: 100%;
    height: 100%;
    margin: 0 auto;
    .underlay {
        background: rgba(0, 0, 0, 0.5);   
        height: 100%;
        @include ab(fixed, n, 0, 0, n, n);
        width: 100%;
        z-index: 99992;    
    }
    .wrap-modal {
        background: $body-background-color;
        z-index: 99999;
        padding: 0;        
        margin: 0 calc((100% - 750px) / 2); 
        width: 750px;
        @include ab(absolute, block, 50%, 0, n, n);
        @include translate(0, -50%);   
        @media (max-width: 767px) {       
            width: 90%;
            margin: 0 5%;
            max-height: 90%;    
        }
        > a {
            @include ab(absolute, block, 4px, n, n, 14px);   
            @include transition(all 0.35s ease 0s);
            @include width-btn;  
            z-index: 1000;   
            @media (max-width: 1024px) {
                @include opacity(1);
            }  
            @media (max-width: 767px) {
                @include ab(n, n, 5px, n, n, 5px);
            }
            i:before {
                @include close-btn;
                font-size: 25px;                                
                color: #000000;
            }   
            &:hover {
                text-decoration: none; 
            } 
        }
    }
    .modal-body {
        padding: 0;
        min-height: 405px;  
        @include flexbox;
        align-items: center;
        justify-content: space-between; 
        @media (max-width: 767px) { 
            max-height: 100%;
            overflow-y: auto;
        }
        .image {
            @include padding-start(15px);
            min-width: 328px; 
            @media (max-width: 767px) {
                display: none;
            }  
        }
        .content { 
            padding: 15px 15px 15px 60px;
            @media (max-width: 767px) {
                padding: 25px 30px;
                width: 100%; 
            }
            h5 {
                font-weight: 600; 
                font-size: 60px;
                line-height: 1.0;
                @media (max-width: 767px) {
                    font-size: 35px;
                }
            }
            p {
                line-height: 1.1;
                margin: 16px 0 39px; 
                font-size: 30px;
                font-weight: 300;
                @media (max-width: 767px) {
                    font-size: 20px; 
                } 
            }
            .btn {
                text-transform: uppercase;
                font-size: 16px; 
                padding: 20px 40px;
                @media (max-width: 767px) {
                    padding: 15px; 
                }         
            }
        }
    }
} 

/*=== pre-order-popup ===*/
#pre-order-popup {  
    margin: 0 auto;
    max-height: 80vh;
    max-width: 680px;
    overflow: visible;
    background: none;     
    h4 {
        margin: 0 0 10px;
        text-transform: uppercase;
    } 
    label {
        text-transform: uppercase;
        font-weight: $font-bold;  
        font-size: $body-font-size - 3px;
    }
    #pre-order-form {
        padding: 40px;
        background: $body-background-color;
        overflow-x: auto;
        overflow-y: auto;
        @media (max-width: 479px) {
            padding: 20px;
        }
        .form-control {  
            background-repeat: no-repeat;
            background-attachment: fixed;
            font-size: $body-font-size;
            color: $body-general-color;
            -moz-appearance:textfield;  
            -webkit-appearance:textfield;     
            -webkit-appearance: none;      
        }
        textarea {
            min-height: 80px;
        }
    }  
}
/*=== shipping-return-popup ===*/
#shipping-return-popup {
    max-width: 800px;     
    h4.fan-modal-title {
        padding: 15px 30px;
        text-transform: uppercase;
        border-bottom: 1px solid $body-border-color; 
    }
    .fan-modal-content {
        padding: 15px 30px;
        h4 {
            padding: 10px 0;
        }
        ul {
            @include padding-start(30px);
            margin-bottom: 20px;
            li {
                list-style: disc inside;
                line-height: 1.6; 
            } 
        }
    }
}
/*=== page-category-top ===*/
#shopify-section-page-category-top-template {
    .wrap-breadcrumb {
        margin-bottom: 60px;
        @media (max-width: 991px) {
            margin-bottom: 30px;
        }
