@mixin toggle-sidebar {
	position: relative;
	width: 100%;
	display: none;
	padding: 25px 15px 0;
	margin: 0 auto 20px;
	background: $body-background-color;
	@include box-shadow(0 0 10px rgba($black, 0.05));
	.sb-title,
	.f-close {
		display: none;
	}
	.sb-filter-wrapper {
		width: 100%;
		display: table;
		vertical-align: top;
	}
	.sbw-filter {
		width: 25%;
		display: inline-block;
		vertical-align: top;
		@media (max-width: 991px) {
			width: 33.33333%;
		}
		@media (max-width: 767px) {
			width: 50%;
		}
		@media (max-width: 575px) {
			width: 100%;
		}
		>div .sb-filter {
			padding: 0 15px;
			clear: none;
			float: none;
			display: inline-block;
			letter-spacing: 0;
