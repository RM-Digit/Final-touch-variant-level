					@media (min-width: 992px) {
						@include padding-end(calc((100vw - 980px + 15px) / 2));
					}
					@media (min-width: 1200px) {
						@include padding-end(calc((100vw - 980px + 15px) / 2));
					}
					@media (min-width: 1440px) {
						@include padding-end(calc((100vw - #{$container-width} + 15px) / 2));
					}
				}
			}
		}
	}
}
.layout-full {
	.sidemenu-holder .menu-list >li.mega-menu {
		.dropdown-menu-box {
			@media (max-width: 1200px) {  
				@include padding-start(15px !important); 
