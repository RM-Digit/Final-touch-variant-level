/*=== product-simple-tab ===*/
.product-table {
    margin: 55px auto 0;
} 
.product-simple-tab {
    padding: 0 0 72px;
    overflow: hidden;
    position: relative;  
    clear: both;
    @media (max-width: 991px) {
        padding: 0 0 40px;    
    }   
    @media (max-width: 767px) {
        padding: 0 0 34px;   
    } 
    .big-title {
        text-align: center;
        font-weight: $font-bold;
        font-size: $size-h1;
        font-family: $font-heading;
        padding: 30px 0 10px;
    }  
    .nav { 
        padding-bottom: 22px; 
        margin-bottom: 0;        
        position: relative;             
        @media (max-width: 991px) {
            padding-bottom: 15px;         
        }      
        @media (max-width: 812px) {
            -webkit-overflow-scrolling: touch;
        }        
        li a {    
            font-size: $size-h1;
            text-transform: uppercase; 
            @media (max-width: 1440px) {
                font-size: $size-h1 - 6px;
            } 
            @media (max-width: 991px) {
                font-size: $size-h1 - 10px; 
            }                 
            &:before {
                content: "";
                @include ab(absolute, block, n, 0, -5px, 0);  
                width: 100%;
                height: 3px;
                background: $body-link-color-hover;
                @include opacity(0);  
                visibility: hidden;
                @include transition(opacity 0.35s ease-in-out);
                @media (max-width: 575px) {
                    bottom: 0;
                }   
            }  
            &.show,    
            &.active { 
                &:before {
                    @include opacity(1);
                    visibility: visible;
                }        
            }  
        }
    }  
    .product-description {
        text-align: center;
        h2, h5, p {
            max-width: 730px;
