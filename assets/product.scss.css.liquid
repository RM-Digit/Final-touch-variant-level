    }
} 
/* Sidebar -------------------------------*/
.left-column-container,
.sidebar {
    .sb-product-carousel { 
        @include carousel-loading();
        .owl-item {
            @include opacity(0);
            &.active {
                @include opacity(1);
            }  
        }
        .owl-stage-outer {
            padding: 0;
            margin: 0;
        } 
        .owl-controls .owl-nav {
            top: -75px;  
        }
    }
    > div:last-child .sb-widget {
        margin-bottom: 0;
    } 
    .price {     
        font-size: $body-font-size + 1px; 
        color: $body-price-color; 
        clear: both;  
    } 
    .price-compare {        
        color: $body-price-compare-color;   
        font-size: $body-font-size - 3px;    
        margin: 0;
    } 
    .price-sale {       
        font-size: $body-font-size + 1px;
        float: none;
        display: inline-block; 
    }   
    .filter-sidebar {
        display: block;
    }    
    .sb-widget {    
        width: 100%;    
        padding: 0;
        position: relative;    
        clear: both;
        margin: 0 0 52px;  
        @media (max-width: 991px) {
            margin-bottom: 30px;  
        }           
        &:last-child {
            margin-bottom: 0;
        }  
        &.filter-sidebar { 
            margin-bottom: 36px;  
            @media (max-width: 991px) {
                margin: 0 0 20px;
            }  
        } 
        &.style-accordion {
            .accordion-active .sbf-title:after {
                content: "\e874"; 
            } 
            .sbf-title {
                position: relative;
                cursor: pointer;
                margin: 0;
                &:after {
                    content: "\e873";
                    font-family: $font-family-icon;
                    font-size: $body-font-size - 3px; 
                    font-weight: 400;  
                    @include ab(absolute, block, -1px, n, n, 0);
                    @include transition(all 0.35s ease-in-out);
                }     
            }
            ul.advanced-filters {
                display: none;
            }
            .clear-filter {
                @include padding-end(25px);
            } 
        }
        .content {      
            h5.sb-subtitle {
                font-size: $size-h4;      
                margin: 0 0 20px; 
                text-transform: capitalize;
            }
            .blog-menu {
                background: none;
                border: none;       
            }
        }    
        .sb-product >a {
            margin-top: 20px; 
        }
        .latest-product-item {    
            margin-bottom: 10px;
            &:last-child {
                margin-bottom: 0; 
            }       
            .listing-item {        
                >div {
                    margin: 0px -10px;
                    align-items: center;
                }
                .product-image {
                    padding: 0 10px; 
                    .product-ratio-false img:not(.img-lazy) {
                        position: static; 
                    } 
                    img {
                        @include ab(absolute, block, 0, 0, n, n);
                        max-width: 100%;
                        max-height: 100%;
                        width: 100%;
                        height: 100%; 
                        margin: 0 auto;
                        @include transition(all 0.65s ease-in-out);
                        &.auto-crop-false {
                            object-fit: contain;
                            font-family: 'object-fit:contain';
                        }
                        &.auto-crop-true {
                            object-fit: cover;
                            font-family: 'object-fit:cover'; 
                        }
                    }         
                }
                .product-content {
                    padding: 0 10px;
                }  
                .product-content-inner {  
                    > a {
                        color: $body-link-color;
                        display: block;
                        margin: 0 0 5px;
                        font-size: $product-font-size; 
                        &:hover {
                            color: $body-link-color-hover; 
                        }
                    }
                    .product-price {
                        margin-top: auto;
                        + .product-review {
                            margin: 10px 0 0;  
                        }
                    }
                    .product-review {
                        display: block;
                        margin: 0 0 10px; 
                    } 
                }     
            }           
        }
        .collection .product {               
            margin: 0 0 20px;
            padding: 0 0 20px;
            width: 100%; 
            border-bottom: 1px solid $body-border-color;
            .row {
                border: none;
                margin: 0;
                padding: 0;
            }
            .sb-product-head {
                @include padding-start(0);        
            }
            .sb-product-content {
                @include padding-start(5px);
                @include padding-end(0);
            }
            .bp-content-inner > a {
                color: $body-link-color;
