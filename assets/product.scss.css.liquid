							>a.dropdown-link {
								@include margin-end(0);
							}
							>span.expand {
								@include ab(n, n, n, 0px, n, auto);
								&:before {
									content: '\e875';
								}
							}
						}
						>a.dropdown-link {
							@include margin-end(0);
						}
						>span.expand {
							@include ab(n, n, n, 0, n, auto);
							&:before {
								content: '\e825';
							}
						}
						>.dropdown-menu {
							@include ab(n, n, n, auto, n, 100%);
							@include border-end(1px solid $body-border-color);
							@include border-start(none);   
						}
					}
				}
				>.dropdown-menu {
					@include ab(n, n, 100%, 0, n, n);
					background: $nav-dropdown-background;
					font-size: $nav-sub-font-size;
					>li {
						>a {
							color: $nav-dropdown-link-color;
							white-space: normal;
