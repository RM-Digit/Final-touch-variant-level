        clear: none;
        .spr-form-message-success {
            color: #4CD964;
        }     
        .spr-form-title {
            font-size: $size-h5;
            margin: 0 0 35px;
            color: $body-general-color; 
            text-transform: uppercase;     
            @media (max-width: 767px) {
                margin-bottom: 10px;
            }
        } 
        .spr-container {
            padding: 0;
            border: 0;
            .spr-header-title {      
                margin: 0 auto 15px; 
                font-size: $size-h1;  
