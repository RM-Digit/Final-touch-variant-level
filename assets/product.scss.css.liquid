.pagination-holder {      
    width: 100%;
    display: table;
    margin-top: 35px;
    @media (max-width: 767px) {
        margin-top: 15px;
    }
    .pagination {
        margin: 0 auto;
        display: block;    
        background: none;       
        text-align: center;  
        @include border-radius-reset;       
        > li { 
            display: inline-block; 
            &:first-child >a {
                @include padding-start(0);
            } 
            &:last-child >a {
                @include padding-end(0);
            }    
            > a {
                display: block;                
                padding: 0 7px;        
                font-size: $size-h4;
                color: $body-link-color;  
                text-transform: uppercase;
                font-weight: $font-bold;
                @media (max-width: 767px) {
                    font-size: $size-h4 - 5px; 
                }                              
                &.disabled {
                    @include opacity(0.7);
                    cursor: no-drop;
                }
            }          
            &:hover {  
                > a, > span {                    
                    @media (min-width: 1025px) {
                        color: $body-link-color-hover;   
                        &.disabled {
                            color: $body-link-color;
                            text-decoration: none;
                        }                         
                    }
                }
            }      
            &.active {   
                > a, > span {          
                    color: $body-link-color-hover;
                    text-decoration: underline;                                   
                }
            }  
        }
    }
    .pagination-showing {
        text-transform: uppercase;
        font-size: $body-font-size - 2px;
        text-align: center; 
        padding-top: 12px;
        color: $body-general-color; 
    }   
}
/*=== Collection page ===*/ 
.fix-boxed {
    .category-grid-view {
        @media (min-width: 1440px) {
            max-width: $container-width;  
        }     
    }
}
#page-body.wide-padding {
    .category-grid-view {
        max-width: 100%;
        @media (min-width: 1200px) { 
            @include padding-start(70px);     
            @include padding-end(70px);
        }  
    }
}
.category-grid-view {  
    @include width-boxed;  
} 
.category {    
    margin: 0;
    padding: 0 0 20px;       
    position: relative;   
    @include transition(all 0.35s ease 0s);
    .category-name {                
        margin: 0 auto; 
        font-size: $body-font-size + 3px;       
        color: $body-link-color; 
        text-transform: uppercase;
        font-weight: $font-bold;  
        @media (max-width: 767px) {
            font-size: $body-font-size; 
        }      
        &:hover {
            @media (min-width: 1025px) {
                color: $main-color;
            }      
        } 
    }
    .category-image {
        margin: 0 auto 10px;
        position: relative;
        &:before, &:after {
            content: "";
            background: $main-color;
            @include ab(absolute, block, n, n, -4px, -4px);
        }
        &:before {
            width: 2px;
            height: 0;
            @include transition(height 0.4s ease-in-out);
        }  
        &:after {
            width: 0; 
            height: 2px; 
            @include transition(width 0.4s ease-in-out);
        } 
        > a {
            display: block;
            position: relative; 
            overflow: hidden;
            img {
                @include transition(transform 0.4s ease-in-out); 
            }
        } 
    }
    &:hover {
        @media (min-width: 1025px) {
            .category-image {
                &:before {
                    height: 100%;
                }
                &:after {
                    width: 100%;
                }
                >a img {
                    @include transform(scale(1.1));
                }     
            }
        }
    }   
}

/*=== product-col-sku ===*/ 
