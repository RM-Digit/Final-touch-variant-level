            border-top: 1px solid $body-border-color;      
        }    
        .spr-form-review {
            clear: both;
        }
        .spr-form-contact-name, .spr-form-contact-email, 
        .spr-form-contact-location, .spr-form-review-rating, 
        .spr-form-review-title, .spr-form-review-body{
            margin: 0 0 15px;
        }
    }   
