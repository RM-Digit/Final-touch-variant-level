            @include ab(absolute, block, 0, 50%, n, n);
            border-top: 1px solid $body-border-color;
            width: 66%;  
            @include translate(-50%, 0%); 
            @media (max-width: 991px) {
                width: 100%;
            }   
        }  
    } 
    .page-category-top {
        .block-banner {
            padding-bottom: 2px;
            .row-1 {
                @include margin-end(-1px);
                @include margin-start(-1px);
                .image {
                    @include padding-start(1px); 
                    @include padding-end(1px);
                }    
            }
        }
        .block-list-collections {
            padding-top: 20px;  
            padding-bottom: 8px;
        }
        div:nth-of-type(5) {     
            .block-product-inner:before {
                content: none;        
            }    
        }   
    }   
}
/*=== Fix skin Market-gray ===*/
.skin-market-gray {
    .breadcrumb li {
        @include transform-text; 
        font-size: $body-font-size - 3px; 
    }
    .page-product {
        padding-top: 5px; 
        .product-content-wrapper {
            .product-vendor {
                @include transform-text;
            }
            h1[itemprop="name"] {
                @include transform-text; 
            }
            .spr-badge .spr-badge-caption, .rating-links >a {
                @include transform-text;   
                font-size: $body-font-size - 1px;
                font-weight: 400;
            }
            .product-sku-stock li {
                @include transform-text;   
                font-size: $body-font-size - 1px; 
                font-weight: 400;  
            }
            .inventory-quantity .text {
                @include transform-text;   
                font-size: $body-font-size + 3px;
                color: $body-price-sale-color; 
            }
            .line, .line2 {
                @include border-radius-10; 
            }
            .group-cw .swatch {
                .header {
                    color: $body-general-color;
                    font-size: $body-font-size;
                    @include transform-text;  
                }
                &.swt-type-button label {
                    background: #ffffff;
                    border: 1px solid #d9d9d9;
                    input:checked+label {  
                        border: 2px solid $main-color; 
                    } 
                }
            }
            .quantity input {
                font-size: $body-font-size + 5px;  
                font-weight: $font-bold;  
            }
            .action-button .btn, .redirect-cart .btn,   
            .shopify-payment-button button { 
                @media (min-width: 992px) {
                    font-size: $button-size + 7px;
                } 
            } 
            .is-countdown .countdown-section .countdown-period {
                @include transform-text; 
            }
            .pre-order, .add-to-wishlist, .add-to-compare {
                font-size: $body-font-size - 1px;
                @include transform-text; 
                text-decoration: none;
                &:hover {
                    text-decoration: underline;
                }
            }
            .size-chart, .shipping-return {
                font-size: $body-font-size - 1px;
                @include transform-text;  
                font-weight: 400; 
            }
            .is-countdown .countdown-section .countdown-period {
                @include transform-text;   
            }  
        } 
    } 
    .quantity {
        background: #ffffff;
        border: 1px solid #d9d9d9;   
    }
    .product-simple-tab .nav {
        position: relative;
        @include flexbox;
        align-items: center;
        &:before {   
            content: "";
            @include ab(absolute, block, n, 0, n, 0); 
            height: 1px;
            background: $body-border-color;
        }   
        li {
            background: $body-background-color; 
            padding-bottom: 4px; 
            &:first-child {
                @include padding-start(15px);
            }
            &:last-child {
                @include padding-end(15px); 
            }
            a {
                @include transform-text;  
            }
        } 
    }
    .cata-toolbar {
        border-bottom: 1px solid $body-border-color;
        padding-bottom: 15px;
        margin-bottom: 40px;  
        .cata-title h1 {
            @include transform-text;  
        }
    }
    .page-cata .left-column-container .sb-widget {
        margin-bottom: 40px;  
    }
    .pagination-holder {
        border-top: 1px solid $body-border-color; 
        padding-top: 55px;
        margin-top: 25px;  
        @media (max-width: 991px) {
            padding-top: 27px;  
        }
        .pagination-showing {
            font-size: $body-font-size - 1px;
            @include transform-text;  
        }
    }
    .page-about-us {
        .block-textbox {
            padding-bottom: 60px;
            @media (max-width: 991px) {
                padding-bottom: 30px; 
            }
            .title {
                margin-bottom: 10px; 
            }
            .subtitle {
                margin-bottom: 24px;
            }
        }
        .block-one-banner {
            padding-bottom: 70px;
            @media (max-width: 991px) {
                padding-bottom: 35px;
            }  
            .textbox {
                text-align: start; 
                align-items: start; 
            }
            .title {
                padding-bottom: 7px;   
            }
        }
        .block-visual {
            padding: 0 0 120px;
            border-top: none;
            @media (max-width: 991px) {
                padding-bottom: 60px;
            }        
            h4 {
                font-size: 50px; 
                line-height: 1.1;   
            }
            .item {
                .heading {
                    font-size: 90px;
                }
                p {
                    font-size: $body-font-size + 5px;
                    font-weight: 300;
                } 
            }
        }
        .block-textbox-2 {
            h2 {
                font-size: 50px;
                padding-bottom: 40px;  
            }
            h3 {
                color: #666666;
                font-weight: 300;
                span {
                    padding-bottom: 37px; 
                    display: block;
                } 
            }
            p {
                color: $body-general-color;
                font-weight: 500;
                font-size: 20px; 
                span {
                    padding-bottom: 37px;
                    display: block;
                }
            }
        }
    }
    .page-category-top {
        .title-wrapper {
            overflow: hidden;  
            @include flexbox;   
            justify-content: center;        
            h3 {
                padding: 0 30px;   
                @include line-left-right;     
            }
        }
        .btn-link {
            text-align: center; 
            margin-top: 27px;  
        }
        .block-banner {
            padding-bottom: 20px; 
            .owl-carousel .owl-nav button {
                &.owl-next {
                    @include ab(n, n, n, n, n, 20px);
                }
                &.owl-prev {
                    @include ab(n, n, n, 20px, n, n);
                }   
            }
        }
        .block-menu {
            padding-top: 59px;
            @media (max-width: 991px) {
                padding-top: 30px;   
            }
            h6 {
                font-size: $size-h6 - 1px;
                min-height: 14px; 
            }
            ul { 
                padding-top: 22px;   
                li {   
                    padding-bottom: 2px;
                    a {
                        font-size: $body-font-size - 1px; 
                        font-weight: 400; 
                    } 
                }
            }
        }
        .block-logo-list {
            padding-bottom: 76px;
            @media (max-width: 991px) {
                padding-bottom: 38px;
            } 
        }
    }
}
/*=== Fix skin Furniture ===*/
.skin-furniture {
    .category-top-heading {
        text-transform: uppercase;
        font-weight: 700; 
        font-size: $size-h1; 
    }
    .breadcrumb li {
        @include transform-text;
        font-size: $body-font-size - 1px; 
        color: #666666; 
        font-weight: $font-bold;
    }
    .wrap-breadcrumb .category-top-menu ul li a {
        font-size: $body-font-size - 1px;
        font-weight: $font-bold;  
    }
    .page-cata .left-column-container .sb-widget {
        .sb-menu h5, .sb-filter-wrapper h5 {
            color: #666666;
            font-size: $body-font-size - 1px; 
        }
    }
    .sbw-filter>div {
        .sbf-title {
            color: #666666;
            font-size: $body-font-size - 1px;
        }
        .advanced-filters>li .inner>a {
            @include transform-text; 
        }
    }
    .page-about-us {
        .block-textbox { 
            .subtitle {
                padding-bottom: 40px;
            }
            .caption {
                margin-top: 30px; 
            }
        }
        .block-one-banner {
            @media (min-width: 992px) {
                padding-bottom: 93px;
            }
        }
        .block-bar {
            @media (min-width: 992px) {
                padding-bottom: 54px; 
            }
            .item {
                .icon {
                    color: $body-link-color-hover;  
                    &:before {
                        content: none;  
                    }
                }
                .text {
                    .heading {
                        font-size: $size-h1;
                        font-weight: $font-bold;
                    }
                    p {
                        font-size: $body-font-size - 2px;
                        font-weight: $font-bold;  
                        color: $body-general-color; 
                    }
                }
            }
            .service-bar-container .row-1 {
                margin: 0 -5px;
                >div {
                    padding: 0 5px;
                    &:not(:last-child) {
                        @include border-end(1px solid $body-border-color);
                        @media (max-width: 767px) { 
                            @include border-end(none);
                            .item {
                                margin-bottom: 10px;
                                padding-bottom: 10px;  
                                border-bottom: 1px solid $body-border-color; 
                            }
                        }
                    }
                } 
            }  
        }
        .block-banner {
            padding-bottom: 20px;
        }
    }
    .page-category-top {
        .block-product {
            padding-bottom: 35px;  
            @media (max-width: 991px) {
                padding-bottom: 0;   
            } 
        }    
        .block-product-inner {
            padding-bottom: 63px;
            border-bottom: 1px solid $body-border-color; 
            @media (max-width: 991px) {
                padding-bottom: 31px;
            }
        }
    }
    #shopify-section-section-instagram .section-instagram {
        padding-bottom: 0;
    }
    #shopify-section-section-newsletter .newsletter-section {
        padding-top: 80px;
        @media (max-width: 991px) {
            padding-top: 40px;
        }
    }    
}
/*=== Fix width sub page follow width header ===*/
.fix-container-width {
    .page-about-us .block-one-banner .textbox { 
        @media (min-width: 1200px) { 
            padding: 0 70px !important;   
        }  
    }
    .page-product.layout-v5 .product-content-wrapper .product-info-inner.block-sticked {
        @media (min-width: 992px) {
            width: calc((100% - 30px) / 2 - 10px);    
        }   
        @media (min-width: 1200px) {  
            width: calc((100% - 140px) / 2 - 10px);   
        }  
    }
}
