        .owl-carousel .owl-nav {
            top: -50px; 
        }    
    }
    .owl-carousel {     
        .owl-nav { 
            @media (min-width: 768px) {
                @include ab(absolute, block, -65px, n, n, 0);   
                width: 100px; 
                height: 30px;    
                background: $body-background-color;   
                z-index: 9;        
                button {
                    top: 0;  
                    &.owl-prev {
                        @include ab(n, n, n, auto, n, 40px);
                    } 
                    &.owl-next {
                        @include ab(n, n, n, n, n, 0); 
                    }        
                }
