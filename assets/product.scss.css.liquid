                @include opacity(1);
                visibility: visible;
                @include translate(0, 0);
            }   
            @media (max-width: 767px) {
                display: none; 
            } 
            .btn-add-cart.disabled { 
                position: relative;        
                &:before {
                    content: '\f110';       
                    font-size: 20px;
                    font-family: $font-family-icon;
                    @include ab(absolute, block, 6px, 0, n, 0);     
                    -webkit-animation: fa-spin 2s infinite linear;
                    animation: fa-spin 2s infinite linear;
                }                     
                .text {    
                    color: transparent;
                }           
            }                
        } 
        .detail-deal {
            @include ab(absolute, block, n, 10px, 10px, 10px);       
