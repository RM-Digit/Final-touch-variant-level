        }
        .action-button, .redirect-cart {                   
            margin: 0 0 15px;         
            list-style: none;  
            float: left; 
            width: calc(100% - 200px);  
            @media (max-width: 1600px) {
                width: calc(100% - 100px);  
            }
            @media (max-width: 767px) {
                width: calc(100%);  
            }  
            @media (max-width: 575px) {
                width: 100%;
                display: block;         
            }
            .btn {
                margin: 0;      
                padding: 10px 15px;    
                width: 100%;        
                font-size: $button-size + 4px;
                @media (max-width: 991px) {
                    font-size: $button-size;
                }  
                @media (max-width: 575px) {
                    display: block;
                    width: 100%; 
                }           
                .demo-icon {
                    margin: 0; 
                    @include margin-end(7px);
                    font-size: 18px;
                    display: none; 
                }   
                &.sold-out { 
                    @include margin-end(15px);         
                }
                &.disabled {
                    position: relative;         
                    color: transparent !important;
                    @include opacity(1);
                    .demo-icon {
                        @include opacity(0);
                    }
                    &:after {
                        content: '\f110';   
                        font-family: $font-family-icon;                 
                        font-size: $font-icon;
                        color: $button-hover-btn1-txt;
                        @include margin-start(-13px);
                        -webkit-animation: fa-spin 2s infinite linear; 
                        animation: fa-spin 2s infinite linear;                      
                        @include ab(absolute, block, 10px, 50%, n, n);                                
                    }      
                }  
                &.disabled[disabled="disabled"] {
                    color: $button-btn1-txt !important;       
                    @include opacity(0.65);   
                    &:after {
                        content: none;
                    }
                }
                &:hover {
                    color: $button-hover-btn1-txt;
                } 
            }  
        }   
        .shopify-payment-button { 
            margin: 0 0 15px;   
            clear: both;      
            button {
                width: 100%; 
                padding: 10px 20px;    
                line-height: 26px; 
                font-weight: $font-bold; 
                white-space: normal;
                font-size: $button-size + 4px;
                text-transform: uppercase; 
                background: $button-btn1;  
                color: $button-btn1-txt;
                border: 1px solid $button-btn1-border;
                @include border-radius-reset; 
                @include transition(all 0.35s ease 0s);
                @media (max-width: 991px) {
                    font-size: $button-size;
                } 
                &:hover {
                    background: $button-hover-btn1;  
                    color: $button-hover-btn1-txt;
                    border: 1px solid $button-hover-btn1-border;
                    text-decoration: underline; 
                }
                &.shopify-payment-button__more-options {          
                    @include border-radius-reset;  
                }
            }
            .shopify-payment-button__button--hidden {
                height: 0;
                margin: 0 auto;
                padding: 0;
            }
        }
        .pre-order,
        .add-to-wishlist,
        .add-to-compare {        
            display: block;
            text-transform: uppercase; 
            color: $body-link-color; 
            line-height: 20px;
            font-size: $body-font-size - 3px; 
            text-decoration: underline; 
            font-weight: $font-bold;  
            padding-bottom: 8px;  
            clear: both;    
            i, svg { 
                float: left;  
                @include margin-end(12px); 
                font-size:  $font-small-icon - 2px;                  
            }
            &:hover { 
                color: $body-link-color-hover;     
            }       
        }  
        .add-to-wishlist {
            @include margin-end(30px);
        }
        .product-action {     
            .variants-wrapper {       
                .selector-wrapper {
                    margin-bottom: 26px;   
                    clear: both;
                    text-align: start;      
                    display: none;  
                    @media (max-width: 767px) {
                        margin-bottom: 15px; 
                    }      
                    select { 
                        min-width: 50%;
                        height: 34px;
                        padding: 0 10px;
                        line-height: 32px;            
                        font-size: $body-font-size;        
                        color: $body-general-color;                                                     
                    }          
                }
                label {    
                    padding: 0 0 4px; 
                    display: block;     
                    margin: 0;                                               
                    color: $main-color;  
                    text-transform: uppercase;  
                    width: 110px;  
                    font-size: $body-font-size - 2px;
                    font-weight: $font-bold;    
                }
            } 
            .wrap-quantity-add-cart {
                list-style: none; 
                padding: 0;
                margin: 0;      
            }     
            .quantity-product {
                margin: 0 0 15px;    
                @include margin-end(10px);               
                width: calc(100% - 200px);
                float: left;
                @media (max-width: 1600px)  {
                width: calc(100% - 100px);
                }    
                @media (max-width: 767px)  {
                    width: 185px;  
                }                    
                label {          
                    margin: -6px 0 0;          
                    line-height: 22px;  
                    font-size: $body-font-size - 1px;        
                    color: $body-general-color;        
                    display: block;                          
                }                            
                input[type=number]::-webkit-inner-spin-button,
                input[type=number]::-webkit-outer-spin-button {
                    -webkit-appearance: none;    
                }               
                .qty-stock {
                    display: inline-block;
                    margin-bottom: 15px;
                } 
            }          
            .total-price {        
                color: $body-price-color;
                display: inline-block;
                margin-top: 16px;
                @include margin-end(10px); 
                .price-sale { 
                    @include margin-end(10px);
                }
            }     
        }    
    }     
}
.templateProduct { 
    .bw-color {
        margin-bottom: 11px;  
    } 
    .fancybox-slide--video {
        top: 20px;
    }
    .fancybox-slide--image .fancybox-content { 
        top: 20px;
    } 
    .product-v5 .header-container.header-fixed .header-main {
        position: static;  
    }    
    #body-content {
        padding-bottom: 0; 
        iframe {
            max-width: 100%;  
            width: 100%;  
        }
    }     
    .video {
        display: block;
        position: relative;
        width: 100%;
        height: 0;
        padding-bottom: 56.25%;
        iframe {
            @include ab(absolute, block, 0, 0, n, n); 
            height: 100%;
        }
    }
    .flex-header-left {
        .tab-content .bundled-items {
            @media (max-width: 1200px) and (min-width: 992px) {
                #crust-items {
                    display: block;
                    float: none;
                    width: 100%;
                    padding: 0;
                } 
                .action-bundle {
                    padding: 15px 0 0; 
                    display: block;
                    float: none;
                    width: 100%;
                }
            }
        } 
    }
    .tab-content {
        padding: 50px 0;  
        #tabs-description {
            line-height: 22px;
            h1, h2, h3, h4, h5, h6 {      
                font-weight: normal; 
            }
        }
        .tab-pane {
            display: block;
            height: 0;
            @include opacity(0);
            &.active { 
                height: auto;
                position: relative;
                @include opacity(1);
            }
        } 
        table {
            border-collapse: collapse;
            margin: -8px auto 23px;
            th {      
                padding: 7px 30px; 
                background: $bg-other;
                color: $body-general-color;
                text-transform: uppercase;
            }
            td {
                border-bottom: 1px solid $bg-other;
                padding: 7px 30px;
                text-align: start;  
                line-height: 1.5;           
            }
            tbody {
                tr:first-child td {
                    border-top: none;
                    padding-top: 0;
                } 
                tr:last-child td {
                    border-bottom: none;
                }
            }
        } 
        h5 { 
            color: $body-link-color;      
        }
        h1, h2, h3, h4, h5, h6 {
            margin: 0 0 23px;    
            line-height: 40px;  
        } 
        ul li {
            list-style: disc inside; 
        }
        p {          
            @media (max-width: 767px) {
                margin-bottom: 15px; 
            }
            &:last-of-type {
                margin-bottom: 0;
            }
        } 
        .description-image {
            margin: -50px 0; 
            @media (max-width: 1199px) {
                padding-bottom: 60px;
            }    
        }   
        .bundled-items { 
            overflow: hidden;
            #crust-items {
                @media (min-width: 992px) {
                    float: left;
                    width: 75%;
                    @include padding-end(15px);
                }
            }
            .action-bundle {
                @media (min-width: 992px) {
                    float: left;
                    width: 25%;
                    @include padding-start(15px);
                }
                @media (max-width: 991px) and (min-width: 576px) {
                    float: right;
                }
                @media (max-width: 575px) {
                    margin-top: 20px;
                }
                .total-price {
                    margin: 0 0 15px;
                    .price {
                        color: $body-price-sale-color;  
                        font-size: $size-h3 + 4px;
                    }
                }
                .btn { 
                    padding: 10px 25px;    
                    font-size: $button-size + 4px; 
                    @media (max-width: 991px) {
                        font-size: $button-size - 1px;  
                    }
                    @media (max-width: 575px) {
                        width: 100%;
                        display: block; 
                    } 
                }
            }
            .bundled-images {
                padding: 0;
                list-style-type: none;
                overflow: hidden;
                li {
                    position: relative;
                    padding: 20px 25px;
                    overflow: visible;
                    float: left;
                    list-style: none;
                    text-align: center;
                    &:not(:first-child) {
                        &:after {
                            content: '';
                            height: 70%;
                            width: 1px;
                            background: $body-border-color;
                            @include ab(absolute, block, 50%, 0, n, n);
                            @include translate(0, -50%);
                        }
                        &:before {
                            content: '+';
                            height: 32px;
                            width: 32px;
                            line-height: 28px;
                            text-align: center;
                            border: 1px solid $body-border-color;
                            background: $body-background-color;
                            color: $body-general-color;
                            font-size: $size-h4;
                            z-index: 2;
                            @include ab(absolute, block, 50%, -16px, n, n);
                            @include translate(0, -50%);
                            @include border-radius-circle;
                        }
                    }
                    @media (min-width: 576px) {            
                        width: 33.333333%;
                    }  
                    @media (max-width: 575px) {             
                        width: 50%;
                        @include padding-start(20px);
                        @include padding-end(20px);
                        &:nth-child(3) {
                            clear: both;  
                            width: 100%;
                            padding-top: 30px;
                            &:after {
                                height: 1px;
                                width: 200%;
                                @include ab(n, n, 0, -25%, n, -25%);               
                                @include translate(0, 0);
                            }
                            &:before {
                                @include ab(n, n, -16px, 50%, n, auto);  
                                @include translate(-50%, 0);            
                            }
                        }
                    }
                    h5 {
                        color: $body-link-color;     
                        font-size: $product-font-size;      
                        font-weight: $font-bold;
                        margin: 0 0 15px; 
                        display: -webkit-box;
                        display: -ms-box;
                        -webkit-line-clamp: 2;
                        -webkit-box-orient: vertical;
                        line-height: 18px; 
                        overflow: hidden;
                        @media (max-width: 479px) {
                            font-size: $product-font-size - 2px;      
                        }    
                        a {
                            color: $body-link-color;
                            &:hover {
                                color: $body-link-color-hover;
                                text-decoration: underline; 
                            }                              
                        } 
                    }
                    img { 
                        display: block; 
                        margin: 0 auto 10px;
                    }
                    .price {
                        display: block;
                    }
                    &:first-child .price {
                        color: $body-general-color;
                    } 
                }
            }  
            .wrap-bundled-item {
                overflow: hidden;
            }
            .bundled-item {
                margin: 0 0 7px;
                padding: 0;
                @include padding-start(20px);
                position: relative;
                line-height: 20px;
                float: left;        
                input[type="checkbox"] {
                    @include ab(absolute, block, 0, 0, n, n);
                    @include opacity(0);
                    margin: 0;
                    width: 100%;
                    height: 100%;     
                    z-index: 2;     
                }   
                .wrap-item,
                > .b-info {
                    position: relative;
                    &:before {
                        content: '';
                        width: 14px;
                        height: 14px;
                        border: 1px solid $body-border-color;
                        background: $bg-other;
                        @include border-radius(3px, 3px, 3px, 3px);
                        @include ab(absolute, block, 3px, -20px, n, n);
                    }
                } 
                input:checked + .wrap-item,
                input:checked + .b-info {
                    &:after {
                        content: '\e83d';
                        font-family: $font-family-icon;
                        font-size: 10px;
                        line-height: 12px;
                        color: $body-general-color;
                        @include ab(absolute, block, 4px, -18px, n, n);
                    }
                }       
                .bundled-price {
                    display: inline;
                    @include padding-start(5px);
                    .price {
                        font-size: $body-font-size + 2px;
                        color: $body-price-sale-color;
                    } 
                }
                &:not(:first-child) {
                    clear: both;
                    cursor: pointer;
                    .bundled-product-title {
                        color: $body-link-color;
                        border-bottom: 1px solid $body-link-color;
                    }
                    input[type="checkbox"] {
                        cursor: pointer;
                    }
                }
                &:first-child {
                    .bundled-product-title {
                        color: $body-general-color;
                    }
                }
            }   
            .wrap-bundled-item {
                @media (max-width: 991px) and (min-width: 576px) { 
                    width: calc(100% - 200px); 
                    float: left;
                }
            }
        }
    }
    
    #shopify-section-product-template-v2 {
        #shopify-product-reviews .spr-container .spr-review:last-child {
            border-bottom: none;   
        }
    }
    &.btn-round  {               
        #shopify-product-reviews .spr-container {
            .spr-summary-actions a {
                @include border-radius-20;
            }
            .spr-button, .spr-button-primary {
                @include border-radius-20;
            } 
        }           
    }  
