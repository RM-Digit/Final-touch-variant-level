                    @include margin-start(185px);
                    @media (max-width: 767px) {
                        @include margin-start(165px); 
                    }    
                    @media (max-width: 575px) {
                        @include margin-start(0);
                        width: 100%; 
                        display: block; 
                    } 
                }
            } 
