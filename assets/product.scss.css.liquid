/*=== Store Locator ===*/
.storelocator-filter {
    display: block;
    padding: 0 0 25px;
}
.location-search {
    h4 {
        margin: 0 0 15px;
    }
    input {
        width: 100%;
        height: 38px;        
        font-size: $body-font-size;
        background: $white;
        padding: 7px 10px;   
        line-height: 1.5;   
        border: 1px solid $body-border-color;   
        -moz-appearance:textfield;  
        -webkit-appearance:textfield;     
        -webkit-appearance: none;  
        @include border-radius-reset;     
        @include box-shadow(inset 0 0 0 $white);
        &:focus {
            border-color: $main-color;
            @include box-shadow(0 0 0 1000px white inset);
        }
    } 
}
.directions-panel {
    @media (max-width: 991px) {
        padding-bottom: 35px;
    }
    .directions-to {
        width: 100%;
        height: 38px;        
        font-size: $body-font-size;
        background: $white;
        padding: 7px 10px;   
        line-height: 1.5;   
        margin: 0 0 10px;
        border: 1px solid $body-border-color;   
        -moz-appearance:textfield;  
        -webkit-appearance:textfield;     
        -webkit-appearance: none;  
        @include border-radius-reset;     
        @include box-shadow(inset 0 0 0 $white);
        &:focus {
            border-color: $main-color;
            @include box-shadow(0 0 0 1000px white inset);
