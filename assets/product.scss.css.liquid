	.header-main {
		.m-cart-icon {
			@include ab(absolute, block, 50%, n, n, 0);
			@include translate(0, -50%);
			min-width: 40px;
			text-align: center;
			&.waiting {
				background: transparent !important;
			}
			.mobile-basket {
				position: relative;
				color: $header-cart-icon-color;
				>.demo-icon {
					font-size: 25px;
					color: $header-cart-icon-color;
				}
				>img {
					max-width: 20px;
				}
				.number {
					position: relative;
					font-weight: $font-bold;
				}
				.n-item {
					@include icons-number;
					background: $header-cart-bg-icon-color;
					color: $header-number-on-icon-color;
				}
			}
		}
	}
	.menu-opened {
		.navbar-responsive-menu span {
			&:nth-of-type(2) {
				@include opacity(0);
			}
			&:nth-of-type(1) {
				@include transform(translate3d(0, 9px, 0px)rotate(45deg));
			}
			&:nth-of-type(3) {
				@include transform(translate3d(0, -5px, 0) rotate(-45deg));
			}
		}
	}
	.navbar-responsive-menu {
		border: none;
		margin: 0;
		padding: 0 !important;
		cursor: pointer;
		z-index: 9;
		@include ab(absolute, block, 50%, 0, n, n);
		@include translate(0, -50%);
		.responsive-menu {
			width: 24px;
			height: 18px;
			position: relative;
			display: block;
			cursor: pointer;
			span {
				width: 100%;
				height: 2px;
				display: block;
				position: relative;
				background: $text-header-color;
				@include transition(all 0.35s ease-in-out);
				&:nth-of-type(2) {
					top: 5px;
				}
				&:nth-of-type(3) {
					top: 10px;
				} 
			}
		}
	}
	.product-image-inner>.col-md-10 {
		width: 80%;
	}
	.sb-wrapper {
		margin: 0;
		background: none;  
		border: none;
		height: 100%;
		overflow: auto;
		.sb-widget {
			margin: 0 auto; 
		} 
	}
	.header-menu-wrapper {
		display: none;
	}
	table {
		thead th {
			padding: 10px 15px;
			&.price {
				padding: 10px 15px;  
			}
		}
		td {
			padding: 15px;
		}
	}
	.cart-table table th {
		padding: 10px 15px;
	}
	.left-column-container #related_item {
		margin-top: 40px;
	}
}
@media (max-width: 479px) {
	.header-main .m-cart-icon .mobile-basket {
		>.demo-icon {
			margin: 0;
		}
		.number .money {
			display: none;
		}
		.n-item {
			@include ab(n, n, n, -15px, n, n); 
		}
	}
}
