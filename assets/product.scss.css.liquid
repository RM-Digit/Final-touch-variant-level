.login-or,
.register-or {
    position: relative;
    @include border-start(1px solid $body-border-color);
    &:before {
        content: 'or';
        width: 50px;
        height: 50px;
        text-align: center;
        line-height: 50px;
        font-style: italic;    
        border: 1px solid $body-border-color;
        background: $white;
        @include ab(absolute, block, 50%, 0, n, n);
        @include translate(-50%, -50%);
        @include border-radius-circle;
    }
}
.login-or { 
    @media (max-width: 575px) {
        margin: 20px 0 0;
        @include border-start(none);
        @include border-end(none);
        padding-top: 50px;
        &:before {
            @include ab(n, n, 2px, 50%, n, auto); 
            z-index: 2;       
        } 
        &:after {
            content: '';
            height: 1px;
            background: $body-border-color;
            z-index: 1;
            @include ab(absolute, block, 0, calc(5% + 15px), n, calc(5% + 15px));
        }
    } 
}
.register-or {  
    @include border-end(1px solid $body-border-color);
    @include border-start(none);
    &:before {
        @include ab(n, n, n, auto, n, -50px); 
    }
    @media (max-width: 575px) { 
        margin: 0 0 40px;
        @include border-start(none);
        @include border-end(none);
        padding-bottom: 50px;
        &:before {
            @include ab(n, n, auto, 50%, -25px, auto); 
            z-index: 2;             
            @include translate(-50%, 0);
        }
        &:after {
            content: '';
            height: 1px;
            background: $body-border-color;
            z-index: 1;
            @include ab(absolute, block, n, calc(5% + 15px), 0, calc(5% + 15px));
        }
    }
}
