    .plm-description {
        display: none;
    }  
    .inventory-quantity .line {
        width: 100%;
        height: 10px;   
        background: #e6e6e6; 
        .line2 {
            height: 100%;
            background: $main-color;  
        } 
    }  
    .product-name {            
        color: $body-link-color;     
        font-size: $product-font-size;      
        padding-top: 9px;     
