/*=== title-wrapper ===*/  
.heading-border-line {
    .title-wrapper,
    .block-heading { 
        align-items: center;
        justify-content: space-around;
        width: 100%;
        @include flexbox;
        flex-wrap: wrap;
        .line-heading {
            border-bottom: 1px solid $body-border-color;
            flex: 1;    
        } 
        h3 {
            position: relative;      
            margin: 0 30px;  
            @media (max-width: 991px) {
                margin: 0 20px;         
            }        
        }    
        &.text-left {
            .line-heading:first-child {
                display: none;
            }
            h3 {
                @include margin-start(0);
            }
        }
        &.text-right {
            .line-heading:last-child {
                display: none;  
            }
            h3 {
                @include margin-end(0);
            } 
        }
        &.subheading-position-float-right {
            .show-all-link {
                position: static;
                @include padding-start(20px);
                margin-top: 0; 
                @include translate(0, 0);
            }     
        }   
    }
    .block-heading h3 { 
        @include padding-start(0);
    }
}
.templateIndex {
    .title-wrapper, .block-heading {
        @include transform-text;
    }
}   
.title-wrapper, .block-heading {     
    margin: 0 auto 34px;   
    padding: 0; 
    position: relative;        
    text-transform: uppercase; 
    @media (max-width: 767px) {
        margin-bottom: 25px;  
    }                     
    h3 {
        font-size: $size-h1; 
        font-weight: $font-bold;     
        margin: 0; 
        line-height: 1.2; 
        display: inline-block;   
        @media (max-width: 991px) {
            font-size: $size-h1 - 5px; 
        }      
    } 
    &.bigger-size {
        margin-bottom: 73px;
        @media (max-width: 991px) {
            margin-bottom: 34px;
        }
        @media (max-width: 767px) {
            margin-bottom: 15px; 
        } 
        h3 {
            font-size: $size-h1 + 10px; 
            @media (max-width: 991px) {
                font-size: $size-h1 + 5px;   
            }  
            @media (max-width: 767px) {
                font-size: $size-h1;    
            } 
        }
        &.subheading-position-normal { 
            margin-bottom: 24px; 
            .show-all-link {
                font-size: $body-font-size + 5px;
            }  
        }
    }
    &.subheading-position-normal {   
        margin-bottom: 16px; 
        .show-all-link {
            display: block;
            font-size: $body-font-size - 1px; 
            font-weight: $font-bold; 
            color: $text-other;  
            width: 100%;   
            margin-top: 3px; 
        }
    }
    &.subheading-position-next { 
        .show-all-link {
            font-size: $body-font-size - 2px; 
            color: $text-other;  
            font-weight: $font-bold;  
            @include margin-start(10px);  
            &:hover {
                color: $body-general-color;    
            }             
        }
    }
    &.subheading-position-float-right {   
        .show-all-link {     
            font-size: $body-font-size - 2px; 
            color: $body-link-color-hover;  
            font-weight: $font-bold;
            @include ab(absolute, block, 50%, n, n, 0);    
            @include transform(translateY(-50%));
            @media (max-width: 767px) {
                position: static;
                margin-top: 15px;
                text-align: end;    
            }  
            &:after { 
                content: "\f105"; 
                font-size: 10px;
                font-family: $font-family-icon;
                @include padding-start(5px);
                position: relative; 
                top: -1px;
            } 
        } 
    }     
} 
