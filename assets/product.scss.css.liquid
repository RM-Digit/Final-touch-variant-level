            }   
        }    
    }
}
.customer_address_table {
    h4.title {
        margin-bottom: 30px;
        text-transform: capitalize;
        color: $body-general-color;
    }
    .control-wrapper {
        label {
            float: left;
            margin: 0;
            line-height: 40px;
            min-width: 150px;
        }
        input,
        select {
            clear: none;      
        }   
        input {
            padding-top: 10px;
            padding-bottom: 10px;
        }
