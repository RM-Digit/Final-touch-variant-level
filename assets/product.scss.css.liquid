/* Popup newsletter ------------------------------*/
.fancybox-opened {
    z-index: 999999;
}
.fancybox-slide {
    padding: 0;    
    @media (max-width: 767px) {
        @include padding-start(10px);
        @include padding-end(10px);
    }
    #newsletter-popup  {  
        width: 810px;
        z-index: 9999; 
        margin: 0px auto;
        max-height: 95vh;     
        @media (max-width: 991px) {      
            width: 90%;
            margin: 0 5%;         
        }
        .newsletter-popup-content {
            display: table;
            width: 100%;
            height: 100%;
            position: relative; 
        }
        .block-image-true {
            .text-box {   
                &.position-right {
                    float: right;
                    width: 60%;
                    @include padding-start(60px);
                    @media (max-width: 991px) {
                        @include padding-start(45px);
                    } 
                    @media (max-width: 575px) {
                        float: none;
                        width: 100%;
                        @include padding-start(15px);  
                    }  
                }
                &.position-left {
                    float: left;
                    width: 60%;
                    @include padding-end(60px);
                    @media (max-width: 991px) {
                        @include padding-end(45px);
                    } 
                    @media (max-width: 575px) {
                        float: none;
                        width: 100%;
                        @include padding-end(15px);  
                    }   
                }
                &.position-top {
                    margin-bottom: -40%; 
                }
                &.position-bottom {
                    margin-top: -43%; 
                }
                &.position-middle {
                    margin-bottom: -43%;  
                }
            }
            .block-image {
                img {
                    width: 100%;   
                }
                &.image-position-right {
                    float: left; 
                    width: 40%;   
                    @media (max-width: 575px) {
                        display: none;
                    }
                } 
                &.image-position-left {
                    float: right; 
                    width: 40%; 
                    @media (max-width: 575px) {
                        display: none;
                    }   
                }
                &.image-position-top {
                    max-height: 440px;  
                    height: 100%;
                    overflow: hidden;
                } 
                &.image-position-bottom {
                    max-height: 440px;  
                    height: 100%;
                    overflow: hidden;
                }
                &.image-position-middle {
                    max-height: 400px;
                    height: 100%;
                    overflow: hidden;
                }   
            }   
        }
        .text-box {
            display: block;
            position: relative;
            z-index: 999;  
            padding: 40px 45px;
            background: $body-background-color;
            @media (max-width: 767px) {
                padding: 15px; 
            } 
            .text-box-content { 
                margin-bottom: 23px;   
                @media (max-width: 767px) {
                    margin-bottom: 15px;
                } 
                &:last-child {
                    margin-bottom: 0; 
                }
            }
            .block-text:first-of-type .text {
                line-height: 1.1; 
                span {
                    color: $main-color;
                }  
            } 
            .block-coupon {
                margin-bottom: 10px;
            }
            .bold-text-true {
                font-weight: 600;   
            } 
            input {
                padding: 13px 20px; 
                height: 40px; 
            }
            button {
                display: block;
            }
            .button-position-bottom {
                input {
                    width: 100%;
                    display: block;
                    margin-bottom: 10px;
                }
                button {
                    width: 100%;
                }
            }
            .button-position-right {
                form {
                    @include flexbox();
                    flex-wrap: wrap;
                    @media (max-width: 991px) {
                    display: block;
                    }
                    input {
                        width: 60%; 
                        @include margin-end(10px);
                        @media (max-width: 991px) {
                            width: 100%;
                            margin-bottom: 20px; 
                        }
                    } 
                    button {
                        @media (max-width: 991px) {
                            width: 100%;
                        }
                    }
                }
            }
            .block-social {
                text-align: center;
            } 
            .widget-social {
                margin: 0; 
                &.border-top-true {
                    border-top: 1px solid #eee;
                    padding-top: 20px; 
                    margin: 30px 0 0; 
                }
                ul li {
                    padding: 0 10px; 
                }
            }
            .countdown-content {
                color: $white;
                text-align: center;      
                .offer-text {
                    padding: 10px;
                    font-size: 12px; 
                }
                .countdown-row {
                    display: table;
                    width: 100%;
                    vertical-align: middle;
                    padding: 0;
                    table-layout: fixed;
                    text-align: start; 
                }
                .is-countdown {
                    border-top: 1px solid #333333;
                }     
                .countdown-section {
                    margin: 0;   
                    width: 25%;
                    display: table-cell;
                    vertical-align: middle; 
                    padding: 10px 15px 15px;    
                    background: transparent; 
                    @include border-end(1px solid #333333); 
                    &:last-child {
                        @include border-end(none);
                    }
                    .countdown-amount { 
                        background: none;
                        color: $main-color;  
                        font-weight: $font-bold;
                        font-size: $size-h1;
                        @media (max-width: 1440px) {
                            font-size: $size-h1 - 10px; 
                        }
                        @media (max-width: 991px) {
                            font-size: $body-font-size;
                        }
                    }
                    .countdown-period {
                        color: inherit; 
                        font-size: 10px;  
                        @media (max-width: 1440px) {
                            font-size: 8px;
                        }
                    }
                }
            } 
        }
        .np-close {
            @include ab(absolute, block, 10px, n, n, 10px);
            width: 30px;
            height: 30px;
            text-align: center;
            line-height: 30px;  
            color: $body-link-color;  
            z-index: 9999; 
            &:before {
                content: "\e816";
                font-family: $font-family-icon;
                font-size: 20px; 
                display: block; 
            }
            &:hover {
                color: $body-link-color-hover;
            }
        }
        .fancybox-close-small {
            display: none;
        }
        .subscribe-result {
            .result-inner {
                padding-top: 45px;
                padding-bottom: 40px;
                @include padding-start(40px);
                @include padding-end(40px);
                text-align: center;
            }  
            .text-1 {
                margin: 0 0 30px;
                font-size: $size-h2;
                font-weight: $font-bold;
            } 
            .coupon-code {
                margin: 0 0 30px;
                .text-2 {
                    margin-bottom: 15px; 
                }  
                .code {
                    font-weight: $font-bold; 
                } 
            } 
        }
    }      
} 
.fancybox-content { 
    padding: 0;
    @include border-radius-reset;  
    overflow: initial; 
    .fancybox-button {    
        @include transition(all 0.35s ease 0s);     
        @include box-shadow(0 0 0 rgba($black, 0.5));
        @include ab(absolute, block, -35px, n, n, -10px);  
        @include opacity(0.65);  
        @include border-radius-reset; 
        cursor: pointer;
        text-align: center; 
        padding: 0;
        z-index: 9;
        @media (max-width: 767px) {
            @include ab(n, n, 0, n, n, 0);  
        }
        svg {
            display: none;
        }
        &:before {
            @include close-btn; 
        }   
        &:hover {
            @include opacity(1);  
        }    
    }
    &.fancybox-error {
        overflow: initial; 
        p {
            padding: 40px;
        } 
    }
}
  
/* Back to top ---------------------------------*/
#scroll-to-top {
    @include transition(all 0.2s ease-out 0s); 
    -moz-backface-visibility: hidden;
    @include opacity(0);
    filter: alpha(opacity=0);
    @include ab(fixed, block, n, n, 10px, 15px);
    z-index: 1000;
    cursor: pointer;
    text-align: center;
    @media (max-width: 1024px) { 
        display: none;
    }
    &.on {   
        bottom: 10%;
        @include opacity(1);      
        filter: alpha(opacity=100); 
    } 
    i {   
        display: none;    
    }
    a {
        position: relative;
        width: 35px;
        height: 35px;
        line-height: 35px;    
        display: block;
        border: 1px solid $body-border-color;
        background: $body-background-color;
        color: $body-general-color; 
        @include transition(all 0.3s ease-in-out 0s);    
        &:before {
            content: "\f106";
            font-size: 22px;
            padding: 0;
            width: 35px;
            height: 35px;
            line-height: 32px; 
            text-align: center;  
            font-family: $font-family-icon;
            display: block;      
            @include transition(all 0.3s ease-in-out 0s);
        }
        &:hover {
            text-decoration: none;
        }
    }
    &:hover a {
        color: $main-color;  
    }
}
/*=== product review ===*/
.spr-icon-star {
    color: $body-ratting-color;  
} 
.loox-rating { 
    >i {
        @include padding-end(2px);
        color: $body-ratting-color; 
    }
