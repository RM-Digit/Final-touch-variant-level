                justify-content: center;   
            }
            .item {
                padding: 0 20px;
                margin-bottom: 40px;
                width: auto;   
                min-width: auto;
                a {
                    display: block; 
                    @include transform-text;
                    font-size: $body-font-size;  
                    span {
                        padding-top: 13px;  
                    }  
                }  
                .image {
                    width: 115px;
                    height: 115px;   
                    padding: 0 20px;
                    @include flexbox;
                    justify-content: center;
                    flex-direction: column;   
                    @include border-radius-circle;
