		@include border-end(1px solid rgba(0, 0, 0, 0.1));
		.header-container {
			@include flexbox;
			flex-wrap: wrap;
			height: 100vh;
		}
		.header-main {
			padding: 0;
			margin: 0;
			width: 100%;
			background: transparent;
			.table-row {
				display: block;
				.header-icons {
					padding: 0;
