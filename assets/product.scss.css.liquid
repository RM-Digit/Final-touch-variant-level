/*===  Section recently-viewed ===*/
.section-recently-viewed {
    padding-top: 34px;
    padding-bottom: 48px; 
    @media (max-width: 767px) {
        padding-top: 20px;
        padding-bottom: 20px;
    }   
    .style-1 {
        .product {
            @include flexbox;
            flex-wrap: wrap;
            align-items: center;
            margin-top: 40px;
            &:nth-child(-n+4) { 
                @media (min-width: 992px) {
                    margin-top: 0;
                }
            }  
            &:nth-child(-n+3) {
                @media (max-width: 991px) and (min-width: 768px) {
                    margin-top: 0; 
                }  
            } 
            &:nth-child(-n+2) {
                @media (max-width: 767px) and (min-width: 576px) {
                    margin-top: 0; 
                }        
            } 
            @media (max-width: 575px) {
                margin-top: 20px; 
                &:first-child {
                    margin-top: 0; 
                }      
            } 
            .product-image {
                width: 80px;
            }      
            .product-content {
                @include padding-start(20px); 
                display: block;
                width: calc(100% - 80px);  
                a {  
                    color: $body-link-color;
                    display: block;
                    padding-bottom: 6px;
                    &:hover {
                        color: $body-link-color-hover; 
                    }  
                }
            }
        } 
    } 
    .style-2 {
        .collection {
            justify-content: center;
        }         
        .product {
            max-width: 11.11111%;
            @include flex(0 0 11.11111%);
            @media (min-width: 992px) {
                &:nth-child(-n+9) { 
                    margin-top: 0;
                }
            }
            @media (max-width: 991px) and (min-width: 768px) {
                max-width: 16.66667%;
                @include flex(0 0 16.66667%);
                &:nth-child(-n+6) {
                    margin-top: 0; 
                }  
            }
            @media (max-width: 767px) and (min-width: 576px) {
                max-width: 33.33333%;
                @include flex(0 0 33.33333%);
                &:nth-child(-n+3) {
                    margin-top: 0; 
                }        
            } 
            @media (max-width: 575px) {
                max-width: 50%;
                @include flex(0 0 50%); 
                &:nth-child(-n+2) {
                    margin-top: 0; 
                }         
            }  
            .product-content {
                display: none;
            }                    
        }      
    }
}   
/*=== nav-tab-wrapper ===*/
.text-left .nav-tabs li:first-child {
    @include margin-start(0);
}
.text-right .nav-tabs li:last-child{
    @include margin-end(0);
} 
.tab-heading-small .wrap-tab-content:not(.border-content) { 
    .nav-tabs.bottom-line li a.nav-link:before { 
        height: 1px;  
        bottom: 5px;
    }         
}
.nav-tabs {
    border: none;
    margin: 0 0 23px;
    @include flexbox();
    text-align: center;  
    flex-wrap: nowrap;
    white-space: nowrap;
    overflow-x: auto;
    overflow-y: hidden;  
    @media (max-width: 991px) {
        margin-bottom: 15px;
    } 
    @media (max-width: 812px) {
        -webkit-overflow-scrolling: touch;
    }       
    li { 
        position: relative;
        border: none;   
        margin: 0;  
        padding: 0 0 16px;  
        @include padding-start(15px);
        @include padding-end(15px); 
        &:first-child { 
            @include padding-start(0);
            @include margin-start(auto); 
        }
        &:last-child {
            @include padding-end(0);
            @include margin-end(auto);   
        }    
        a.nav-link {      
            line-height: 24px;
            margin: 0;   
            padding: 0;            
            border: none;               
            display: block;
            position: relative;          
            color: $body-link-color;                                                          
            background: none;    
            text-align: center;  
            cursor: pointer;  
            font-weight: $font-bold; 
            @include border-radius-reset;                            
            &:hover {
                border: none;
                background: none;
                @media (min-width: 1025px) {            
                    color: $body-link-color-hover;
                    text-decoration: none;   
                }        
            }
            &:focus {
                border: none;
                background: none;          
                color: $body-link-color-hover; 
            }  
            &.show,    
            &.active {
                background: none;
                border: none;
                text-decoration: none;  
                color: $body-link-color-hover;        
            }   
        }   
    }    
    &.bottom-line li a.nav-link {
        &:before {
            content: "";
            @include ab(absolute, block, n, 0, -6px, 0);  
            width: 100%; 
            height: 3px; 
            background: $body-link-color-hover;
            @include opacity(0); 
            visibility: hidden;
            @include transition(opacity 0.35s ease-in-out);
        }        
        &.show,    
        &.active {
            &:before {
                @include opacity(1);
                visibility: visible;
            }          
        }
    }  
}
.nav-tab-wrapper { 
    margin: 0 auto;     
    position: relative;  
    @media (max-width: 767px) {
        margin-bottom: 10px;
    }  
    &.heading-border-line .nav {
        position: relative;  
        &:before { 
            content: "";
            @include ab(absolute, block, 50%, 0, n, 0);    
            width: 100%;
            height: 1px;
            background: $body-border-color; 
            @media (max-width: 767px) {
                content: none;  
            }              
        }  
        li {
            background: $line-tab-bg;  
        }  
    } 
    .nav {    
        &.align-left {
            text-align: start;    
            @media (max-width: 1024px) {
                text-align: center;    
            }
        }  
        &.align-center {
            text-align: center;
            @media (min-width: 1025px) {
                @include ab(absolute, block, -53px, 50%, n, n);
                @include translate(-50%, 0);
                white-space: nowrap;
            }
            &.tab-heading-1 { 
                @media (min-width: 992px) { 
                    top: -48px;
                    border-bottom: none; 
                }  
            }
        }
        &.align-right {
            text-align: end;  
            @media (min-width: 1025px) {
                @include ab(absolute, block, -53px, n, n, 0);
                white-space: nowrap; 
            }  
            @media (max-width: 1024px) {
                text-align: center;    
            }    
            &.tab-heading-1 {
                @media (min-width: 992px) { 
                    top: -42px; 
                    border-bottom: none; 
                }
            }
        }     
    }
}
/*=== cart-sb - cart-dd ===*/
.sb-progress {
