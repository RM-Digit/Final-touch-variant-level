}   
.flag-3 i { 
    width: 24px;
    height: 24px; 
    @include border-radius-circle;
} 
/*=== language ===*/
.wg-flags.flag--1 a:before { 
    display: none; 
}   
.flag-only .translate-position .weglot-container .country-selector .wgcurrent {
    a { 
        @include padding-end(0);
        &:before {
            @include margin-end(0);
        }  
    }
    &:after {
        display: none;    
    } 
} 
/*=== language-popup ===*/
#language-popup {
    background: rgba(0, 0, 0, 0.5);   
    height: 100%;
    @include ab(fixed, none, 0, 0, n, n); 
    width: 100%;
    z-index: 99992;
    .modal-dialog  {  
        z-index: 9999;  
        background: $body-background-color; 
        @include ab(absolute, block, 30%, n , n, n);
        @media (max-width: 991px) {
            width: 50%;
            margin: 0 calc((100% - 50%) /2); 
        }   
        @media (max-width: 767px) {
            width: 90%;
            margin: 0 calc((100% - 90%) /2);
        }   
    }  
    .modal-content {
        border: none;
        @include border-radius-reset;  
    }  
    .language-close {
        @include ab(absolute, block, -35px, n, n, -15px);
        @include transition(all 0.35s ease 0s);                    
        @include width-btn-1;
        cursor: pointer;  
        text-align: center;
        z-index: 9999;     
        padding: 0;
        margin: 0;  
        @media (max-width: 767px) {
            @include ab(n, n, 0, n, n, 0);  
        }   
        i:before {
            @include close-btn; 
        } 
    }
    .modal-header {
        padding: 0;
        border: none;
        @include border-radius-reset;
    }  
    .modal-body {
        padding: 25px;
        h4 {
            padding: 0 0 10px;
            text-transform: uppercase;  
        }
        .currency-position ul, .country-selector ul {
            @include ab(absolute, block, n, 0, n, n);
            width: 100%;
            z-index: 1001;
            background: $body-background-color;
            border: 1px solid $body-border-color;
            padding: 5px 10px;  
            max-height: 200px;
            overflow-y: auto;
            li {
                list-style: none;
                padding: 5px 0;
                i {
                    @include margin-end(5px);
                }  
                a {
                    height: auto;
                    line-height: normal;  
                    font-size: $body-font-size; 
                    padding: 0;    
                    &:hover {
                        color: $body-link-color-hover;
                    } 
                }     
            }
        }
        .currency-position {
            position: relative;
            display: block;
            margin-bottom: 50px;
            a {
                color: $body-general-color;
            }  
            .currency_wrapper {
                padding: 0px 10px;
                display: block;
                position: relative;
                height: 37px;
                line-height: 36px;
                border: 1px solid $body-border-color;
                .expand {
                    @include ab(absolute, block, 0, n, n, 10px);  
                    font-size: 15px;
                    width: 20px;
                    line-height: 28px;
                    height: 100%; 
                    text-align: center;
                    i {
                        display: block; 
                        width: 100%;
                        height: 100%;
                        @include transition(all 0.35s ease-in-out); 
                        font-size: $font-small-icon;
                        line-height: 35px; 
                    }   
                }     
                &.icon-opened .expand i { 
                    @include rotate(180);  
                } 
            } 
            .currency_code  i {
                margin-top: -2px; 
                @include margin-end(5px);
                vertical-align: middle;
            }  
            .c-code {
                display: none; 
            }
            &.flag-none i {
                display: none;
            }   
            ul.currencies {
                @include dropdown-hidden;  
                &.opened {
                    @include dropdown-visible;
                }  
            }
        }
        .translate-position-mobile {
            position: relative;
            display: block;  
            .weglot-container { 
                display: block;    
            }
            .country-selector {
                display: block;
                &.closed { 
                    ul {
                        @include dropdown-hidden; 
                        @include translate(0, 10px);    
                    } 
                    .wgcurrent:after {
                        @include transform(translate(0, -50%) rotate(0deg));
                    }
                } 
            }     
            .wgcurrent {
                color: $body-general-color;  
                padding: 0px 10px;
                display: block;
                position: relative;
                height: 37px;
                line-height: 36px;
                cursor: pointer; 
                border: 1px solid $body-border-color; 
                z-index: 9;  
                a {
                    height: auto;
                    padding: 0; 
                    color: $body-general-color;
                    font-size: $body-font-size;    
                    &:hover {
                        text-decoration: underline;   
                    }
                }
                &:after {
                    content: "\e812" !important;
                    font-family: $font-family-icon;
                    font-size: 22px;   
                    background-image: none;
                    width: 20px;  
                    height: 37px;  
                    text-align: end;
                    line-height: 37px;   
                    @include ab(absolute, block, 50%, n, n, 10px);  
                    @include transform(translate(0, -50%) rotate(180deg));
                    @include transition(all 0.35s ease-in-out); 
                }           
            }
            ul {  
                @include dropdown-visible;
                @include translate(0, 0);     
            }
        }
    } 
}
/*=== mobile-department ===*/
.mobile-department {     
    padding-top: 15px;
    padding-bottom: 15px;  
    @media (max-width: 767px) {
        padding-top: 0px; 
    }
    ul::-webkit-scrollbar {
        height: 6px; 
        background: #eee;
        @include border-radius-25;
    } 
    ul::-webkit-scrollbar-thumb {
        background: $main-color;
        @include border-radius-25;
    }  
    ul {
        @include flexbox();   
        @media (max-width: 812px) {
            -webkit-overflow-scrolling: touch;
        }  
        @media (max-width: 767px) {
            overflow-x: auto; 
            white-space: nowrap;
        } 
        li { 
            list-style: none;
            padding: 0 15px 15px; 
            text-align: center;
            flex: 0 0 auto;
            width: 20%; 
            @media (max-width: 767px) {
                width: 25%;
                padding: 0 calc(15px / 2) 15px; 
            }
            a {
                color: $body-general-color;
                font-size: $body-font-size - 2px;
                font-weight: $font-bold;
                &:hover {
                    color: $body-link-color-hover;
                }  
            } 
            img {
                display: block;
                margin: 0 auto;
                width: auto;
            }    
            .text {
                margin-top: 7px;  
                display: block;
                clear: both;
                text-transform: uppercase; 
            } 
            &.active a {
                color: $main-color;
            } 
        }  
    }
} 
/*=== notifyjs-corner ===*/
.hide-noti .notifyjs-corner {
    display: none !important;
} 
.mobile-bar-outside, 
.templateProduct {
    .notifyjs-corner {
        @media (max-width: 991px) {
            @include ab(n, n, n, n, 52px !important, n);
        }  
    } 
}
.notifyjs-corner {
    position: fixed; 
    width: 335px;
    z-index: 1001 !important;   
    overflow: hidden;  
    @media (max-width: 575px) {
      width: calc(100% - 30px); 
    }  
    .notifyjs-wrapper {
      width: auto !important;
    }
    .notifyjs-container {
        width: auto !important;
        > div {
            width: 100% !important;   
            border: 1px solid #cccccc;    
            @include box-shadow(0px 2px 5px rgba(0, 0, 0, 0.1));
            &.show-mobile-false {
                @media (max-width: 767px) {
                    display: none !important;
                }       
            }
            .close-noti {
                @include ab(absolute, block, 0, n, n, 0); 
                width: 25px;
                height: 25px;
                line-height: 23px;        
                z-index: 2;
                color: $body-general-color;  
                cursor: pointer;
                i:before {
                    content: "\e816"; 
                }
                &:hover {
