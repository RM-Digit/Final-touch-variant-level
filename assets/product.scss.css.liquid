        &.owl-prev {
            @include ab(n, n, 40%, 0, n, n); 
        }
        &.owl-next {
            @include ab(n, n, 40%, n, n, 0);   
        }   
    }       
}  
.owl-carousel .owl-nav {
    button {    
        @include ab(absolute, block, 40%, n, n, n); 
        padding: 0;
        margin: 0; 
        height: 30px;     
        width: 30px; 
        z-index: 9;        
        @include transition(all 0.35s ease 0s); 
        &.owl-prev, &.owl-next { 
            background: $btn-carousel-bg; 
            color: $btn-carousel-txt;
            border: 1px solid $btn-carousel-border;
            &:hover {   
                @media (min-width: 1025px) {
                    background: $btn-carousel-bg-hover; 
                    color: $btn-carousel-txt-hover;
                    border: 1px solid $btn-carousel-border-hover;
                }      
            } 
            &.disabled {
                span {
                    cursor: default;   
                }    
                &:hover {
                    background: $btn-carousel-bg; 
                    color: $btn-carousel-txt;
                    border: 1px solid $btn-carousel-border;
                }
            }         
        }   
        span {
            display: inline-block; 
            height: 100%;
            width: 100%;   
            cursor: pointer;
            text-align: center;
            color: inherit; 
            line-height: 28px; 
            font-size: 0;            
            &:before {        
                font-size: $font-small-icon - 7px;             
                font-family: $font-family-icon; 
                font-weight: 900; 
                display: block; 
                width: 100%;
                height: 100%;   
            }    
        }
        &.owl-next {   
            @include ab(n, n, n, n, n, -33px);   
            @media (max-width: 1440px) {
                @include ab(n, n, n, n, n, 0); 
            }   
            span:before {
                content: "\e818";                    
            } 
        }
        &.owl-prev {
            @include ab(n, n, n, -33px, n, n); 
            @media (max-width: 1440px) {
                @include ab(n, n, n, 0, n, n); 
            }
            span:before {
                content: "\e819";   
            } 
        }
    }
}
