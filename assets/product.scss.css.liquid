                @media (max-width: 991px) {
                    font-size: $size-h1 - 4px; 
                } 
                @media (max-width: 767px) {
                    font-size: $size-h1 - 10px;
                }   
            }
            .spr-summary {
                .spr-starrating {        
                    display: inline-block;  
                    .spr-icon {
                        width: 16px; 
