/*=== page-order ===*/
.page-order {
    h2 {
        font-size: $size-h3;
        font-weight: 100;
        margin: 0 0 10px;
        font-weight: $font-bold;
        text-transform: uppercase; 
    }  
    .note.order_date{
        margin-top: 20px;
        margin-bottom: 20px;
    }
    #order-details{
        margin-bottom: 40px;
        width: 100%;
        overflow-x: auto;
        table {
            border-bottom: 1px solid $body-border-color;
            padding-bottom: 10px;
            td {
                &.product-item {
                    text-align: start;
                    min-width: 160px;
                }
                &.quantity {
                    display: table-cell;
                    padding: 0;
                    border: none;
                }
                &.money {
                    font-size: 18px;          
                    color: $body-general-color;
                    &.total {
                        color: $main-color;
                    }
                }
            }
            tr.order_summary td {
                text-align: end;
                &.price {
                    padding: 10px 25px;
                    font-size: 18px;          
                    color: $body-general-color;
                }
                &.total {
                    color: $main-color;
                    font-size: 18px;          
                }
                &:last-child {
                    text-align: center;
                }
            }
            tfoot tr {
                td {
                    padding-top: 10px;
                    padding-bottom: 10px;
                }
                &:last-child td {
                    border-bottom: none;
                } 
            } 
        }
    }
    #order-address {
        margin: 0;
        .title {
            margin-bottom: 20px;
        }    
        #order-payment {
            @media (max-width: 767px) {
                padding-bottom: 15px;
            }
        }
    }
}
/*=== section blog - blog page ===*/ 
.tags a {
    color: $white;
    background: $black;
    font-size: 11px;
    padding: 2px 5px; 
    font-weight: $font-bold;
    text-transform: uppercase;
    margin: 0;
    @include margin-end(4px);
    &:hover {
        text-decoration: underline;
    }
}  
.post-item {
    .post-item-inner {
        margin-bottom: 35px;  
        @media (max-width: 767px) {
            margin-bottom: 25px;   
        } 
    }  
    .post-image {
        margin: 0 auto 15px;  
        position: relative;
        overflow: hidden;  
        img {
            width: 100%;
            @include transition(transform 0.35s ease-in-out);
        }
        > a {
            position: relative;
            display: block; 
            &:before {
                @include effect-image;
            }  
        } 
        .tags {
            @include ab(absolute, block, 12px, 9px, n, n);
            z-index: 2;
        } 
        &:hover {
            @media (min-width: 1025px) {
                >a:before {
                    @include effect-image-hover;
                }
                img {
                    @include transform(scale(1.06));    
                } 
            }
        } 
    } 
    .post-title {
        display: block;      
        color: $body-link-color;      
        line-height: 1.1;
        margin: 0;      
        word-break: break-word;   
        font-weight: $font-bold;
        text-transform: uppercase;
        a {
            color: inherit; 
        }  
        &:hover {
            color: $body-link-color-hover;
            text-decoration: underline; 
        }
    }
    .meta-data {
        font-size: $body-font-size - 2px;
        padding: 16px 0 0;  
        color: $body-general-color;
        @media (max-width: 767px) {
            padding: 10px 0 0;
        }  
    }
    .post-excerpt {
        font-size: $body-font-size;
        color: $body-general-color;
        padding-top: 8px; 
    }
    .post-des {
        margin: 20px 0 30px;       
        padding: 0;   
        line-height: 24px;  
        @media (max-width: 767px) {
            margin-bottom: 20px;
        }
    }
    .post-comment {
        color: $body-general-color;
        .demo-icon {
            font-size: 20px;
            @include margin-end(5px);
        }
    }
    .post-button {
        margin: 30px 0 0;
        .btn {
            line-height: 26px;
            padding: 8px 40px;
            margin: 0;
            @include margin-end(10px);
            font-weight: 400;
            color: $body-link-color;
        }
    }  
    &.first-item {
        .post-item-inner {
            position: relative;
            margin-bottom: 20px; 
        } 
        .post-content {
            @include ab(absolute, block, n, 30px, 30px, n); 
            max-width: 75%;
            z-index: 1; 
            @media (max-width: 575px) {
                max-width: 90%;   
                @include ab(n, n, n, 10px, 10px, n); 
            } 
            .post-title {
                background: $body-background-color;
                padding: 12px 20px;
                line-height: 1.05;
                font-size: $size-h1 + 10px;
                margin-bottom: 2px;
                @media (max-width: 767px) {
                    font-size: $size-h1 - 10px;
                }  
                @media (max-width: 575px) {
                    padding: 5px 20px; 
                } 
            }   
            .meta-data {
                background: $body-background-color;
                padding: 5px 20px;
                line-height: 1.3;
                display: inline-block; 
            } 
        }
    }
}
.page-blog {
    &.layout-right-sidebar,
    &.layout-left-sidebar {
        @media (min-width: 992px) {
            >div >div { 
                margin: 0 -20px; 
                >div {
                    padding: 0 20px;
                } 
            } 
        }    
    }
} 
#page-body.boxed .page-article .related-posts {
