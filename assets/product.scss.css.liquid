            margin: 23px 0 0; 
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            display: none;
        }  
        .product-button {
            @include ab(absolute, block, n, 0, 47px, 0);    
            @include opacity(0);
            visibility: hidden;
            @include flexbox;
            flex-wrap: wrap;
            justify-content: center; 
            @include transition(opacity 0.2s ease-in-out); 
            @media (max-width: 1024px) and (min-width: 768px) {
                bottom: 5px;
                @include opacity(1);
                visibility: visible;
            }
            @media (max-width: 767px) {
                display: none; 
            }
